ed_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[]},"created_on":1579779626,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"87699747B9DA11E99CA40242AC110005","steps":[{"id":740394,"guid":"71F4AF30735B11E98C5B8B806F82AB7B","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A55A415EBA244E12960E67CD8CF885EC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0FF7EE471FEE4B84A2AD58C25E67BE37","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Retrieve dissection tray from -20\u00b0C, and the glass slide and razor blade from 4\u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740395,"guid":"72048DB0735B11E98C5B8B806F82AB7B","previous_id":740394,"previous_guid":"71F4AF30735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"84DE524B25014AD99578BAD76E87EE3C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"338F26E60653493FB2D05F33CC44B377","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the glass slide, razor blade, and ruler on the dissection tray and perform all cutting on slide to minimize damage to, and contamination of, the tray.<\/div><\/div>"}},{"id":1054725,"guid":"C31901A13D41441CA1DF6F9038F4728B","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">It is helpful to put the dissecting tray on ice to keep it cold and prevent it from moving.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740396,"guid":"7204B4C0735B11E98C5B8B806F82AB7B","previous_id":740395,"previous_guid":"72048DB0735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8E1A44F5DC174297924A8A71B7678422","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5A70F3F9753E4723BD132E9BB638F851","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the frozen sample on the glass slide and shave off the frosty\/freezer-burned tissue from the face you want to sample from, and discard. This is damaged and not worth extracting. Then, moving rapidly but carefully, shave off enough sample to work with.  <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740397,"guid":"7204DBD0735B11E98C5B8B806F82AB7B","previous_id":740396,"previous_guid":"7204B4C0735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2A83649E065D43C0800BC2035F245EED","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FE6686968E5E45F68CD64C240E993A23","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Return the tissue to -80\u00b0C (or put on dry ice as a temporary hold).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740398,"guid":"7204DBD1735B11E98C5B8B806F82AB7B","previous_id":740397,"previous_guid":"7204DBD0735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3DC861A8800E4B89BBD932489027AFCA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EAE355F3164F43DEB9E256D39FDCB285","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rapidly mince the shaved sample with the same razor blade that was used to shave the tissue in step 3 and scrape into the first well of a 6-well plate that contains 4 mL of cold extraction buffer. Pipette up and down 20 times.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740399,"guid":"720502E0735B11E98C5B8B806F82AB7B","previous_id":740398,"previous_guid":"7204DBD1735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7547FDCC6D3C4DBE9803BBDD7412CE89","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DAA0C88417874A439E5DF668AA199BC7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Commence 10-minute incubation, pipetting up and down 20x every ~2.5 minutes with a 1000 uL pipette.<\/div><\/div>"}},{"id":1054725,"guid":"28F0E5D074CB11E99256A30FEE1E5F42","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":4,"unit":"\u00b0C","title":""}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740400,"guid":"720502E1735B11E98C5B8B806F82AB7B","previous_id":740399,"previous_guid":"720502E0735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"91D5040FBE824927B42CCE77019FFDA8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"82BB3A2A58974FDB9C7F6D695F888FA8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pull the sample into the chilled needle\/syringe and express (expel) into the second well of the 6-well plate.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740401,"guid":"720529F0735B11E98C5B8B806F82AB7B","previous_id":740400,"previous_guid":"720502E1735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A6428D095B62405DB823DE43E32AF3DB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DC456D8C75184BA09702580BFCA6427E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Repeat this, pulling the sample into the same syringe and expressing it into the third well of a 6-well plate.  <\/div><\/div>"}},{"id":1054725,"guid":"78F159A2B14C4C00A4264F4218B19671","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Note that one may simultaneously process 2 distinct samples in the top and bottom 3 wells of a 6-well plate.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740402,"guid":"720529F1735B11E98C5B8B806F82AB7B","previous_id":740401,"previous_guid":"720529F0735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B2E36DF417F649D9AC671FB27B88FE1E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"134484BDCA904931AA346ECA7C314C07","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wet the 20 \u00b5m filter with 200 uL cold WB, then pull the sample through the filter using gentle vacuum. Then pass 2 mL of cold WB through the filter as a wash step.<\/div><\/div>"}},{"id":1054725,"guid":"33B4A13664564E548DF96BB6CD727CC4","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Try to avoid frothing (bubbles).<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740403,"guid":"727F8A10735B11E98C5B8B806F82AB7B","previous_id":740402,"previous_guid":"720529F1735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"008EC73E221A4D27A648526B1A8833C4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"44A920F58AFF41739552FA8049097067","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Optional Step: Wet the 5 \u00b5m filter with 200 uL WB and put the sample through that filter, using gentle vacuum. Try to avoid frothing. Then pass 2 mL of cold WB through the filter as a wash step.<\/div><\/div>"}},{"id":1054725,"guid":"99744288EDFD4AC3A6781A0432761544","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Using the 5 \u00b5m filter may not be necessary if you are working with a sample that has a low amount of debris\/white matter, or if you do not want to risk filtering out some of the larger nuclei (although most of the larger nuclei will still squeeze through the 5 \u00b5m filter with the vacuum).<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740404,"guid":"72A20630735B11E98C5B8B806F82AB7B","previous_id":740403,"previous_guid":"727F8A10735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9E0D65B3EFFD46FEA38C17E458246BCC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5A8E3776675940948C7AE25F7B2C0529","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Optional Step: Remove 10 uL of nuclei and mix with 10 mL of the diluted Hoechst stain and view using the brightfield and DAPI filters on the fluorescent scope.  Photograph both brightfield and DAPI-excited nuclei.  <\/div><\/div>"}},{"id":1054725,"guid":"0215343790CD427192F704C2EED2231D","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">This is your baseline extraction and it\u2019s a check to see if you have nuclei (and if it is worth proceeding with the prep).  You will still see debris, despite the filtration steps.  The subsequent spin will remove much of the debris, though some will still pellet with the nuclei.  <\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}},{"id":1054726,"guid":"2B94228070B3482AB79BA46364CF6E96","order_id":3,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">It is helpful to have a second person perform this step while you continue with step 12.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}},{"id":1054727,"guid":"6006C0D19C7C41D6B2D92377CD8FD4E8","order_id":4,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Once you are familiar with extracting nuclei from a particular type of tissue and are consistently getting the expected nuclei yields, this baseline check step can be skipped.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740405,"guid":"72A22D40735B11E98C5B8B806F82AB7B","previous_id":740404,"previous_guid":"72A20630735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CAF6EC74B8ED47619C272A1EF25E421F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"76F23F97DFE645859F87855F07CA4A3B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add chilled WB to the filtered nuclei to a total volume of just under 50 mL.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740406,"guid":"72A25450735B11E98C5B8B806F82AB7B","previous_id":740405,"previous_guid":"72A22D40735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A24DDF08D39B4A049894626D2BF6F623","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AA2C84205B844B4BBD104FB9A5869B9C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Divide these diluted nuclei among four 50 mL conical tubes (~14 mL per tube).  <\/div><\/div>"}},{"id":1054725,"guid":"C5ABD0210B26417993A1491F520662C3","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Don\u2019t use 15 mL tubes as the nuclei do not \u201ctravel\u201d the longer distance well.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740408,"guid":"72A25452735B11E98C5B8B806F82AB7B","previous_id":740632,"previous_guid":"0992B23074CC11E99256A30FEE1E5F42","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E894C6D6ECD9414293F457B1C9ABE1FC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FF80C946A36545C093F970FF6C8C2D8A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Carefully remove as much of the supernatant as possible without disturbing the pellet.<\/div><\/div>"}},{"id":1054725,"guid":"382BF0F848DD48A19EFE54DF005EED5C","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">The pellet is hard-to-impossible to see.  The cleaner the prep, the less visible the pellet is.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":740409,"guid":"72A40200735B11E98C5B8B806F82AB7B","previous_id":740408,"previous_guid":"72A25452735B11E98C5B8B806F82AB7B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EFB037AB64C54C9A9AB35BBFB85044F8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7CC53114562F495BB57123E25612F1B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div cl