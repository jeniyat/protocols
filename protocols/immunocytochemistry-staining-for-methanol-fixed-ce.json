:null,"critical_id":null,"duration":0},{"id":855554,"guid":"BB17074017A611EA946573BD65412254","previous_id":855553,"previous_guid":"BB14211017A611EA946573BD65412254","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EE69B6AE6A0741B2B2277B5AAD04AB2C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Preparation:"}},{"id":1054724,"guid":"8D21ED8313D54C2A996986443B858F87","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rinse three times quickly in PBS.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855555,"guid":"C55ABE9017A611EA946573BD65412254","previous_id":855554,"previous_guid":"BB17074017A611EA946573BD65412254","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DD04807FF97E476AAAC98CBA8308129B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Blocking:"}},{"id":1054724,"guid":"35893851199042309C724FFB53DFFEA2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Block samples in 1 mL of blocking buffer at room temperature for 30 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855557,"guid":"DFBE6BB017A611EA946573BD65412254","previous_id":855555,"previous_guid":"C55ABE9017A611EA946573BD65412254","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"62592FA3CF7C4735AFCA000F13F15B36","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample Staining:"}},{"id":1054724,"guid":"FE9A574E84DA40E99D5C3016100A9A89","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dilute the primary antibody to the recommended concentr