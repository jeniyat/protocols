ess":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":229652,"guid":"79B623A9098E4636AD228E35819BC82B","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"4hrou\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"dkeb1\",\"text\":\"a\",\"type\":\"atomic\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[{\"offset\":0,\"length\":1,\"key\":0}],\"data\":[]},{\"key\":\"dr70a\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"3mb04\",\"text\":\"Black arrows: Annealing part of primer.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"4mis5\",\"text\":\"Dotted lines: overhang part of primer (color coded).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"bsvgr\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[{\"type\":\"imageblock\",\"mutability\":\"MUTABLE\",\"data\":{\"guid\":null,\"source\":\"https:\\\/\\\/s3.amazonaws.com\\\/pr-journal\\\/hcker2w.png\",\"placeholder\":\"https:\\\/\\\/s3.amazonaws.com\\\/pr-journal\\\/hcker2w.png\",\"original_name\":\"Picture1.png\",\"width\":\"326\",\"height\":\"279\",\"mime\":null,\"is_video\":false}}]}","section":"Experimental design\/overview","section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":229753,"guid":"9B5C357374A54E988DF9A370BA40E077","previous_id":229652,"previous_guid":"79B623A9098E4636AD228E35819BC82B","modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"cs2e1\",\"text\":\"Create\\u00a0in silico vector maps for template and target vectors using SnapGene or a similar tool.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":229853,"guid":"D2CC02F2694049BEB79CF76720CA9411","previous_id":229753,"previous_guid":"9B5C357374A54E988DF9A370BA40E077","modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"efivt\",\"text\":\"18-21 bp in length, 40-60 % GC-content, Tm = 55-65 \\u00b0C. Tm- values of two primer pairs (annealing part) should not differ by more than 3 \\u00b0C.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[{\"offset\":41,\"length\":1,\"style\":\"sub\"},{\"offset\":56,\"length\":1,\"style\":\"sub\"}],\"entityRanges\":[],\"data\":[]},{\"key\":\"f8vbe\",\"text\":\"Single nucleotide repeats of four or more should be avoided when possible.\\u00a0\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"b1ekq\",\"text\":\"A 3'-GC-clamp (1 or more G's or C's at the 3'-end of the primer) can be beneficial for annealing.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":"Annealing part of primer:","section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":229909,"guid":"8CA0FAC453D240FF9E3875F9125F2586","previous_id":229853,"previous_guid":"D2CC02F2694049BEB79CF76720CA9411","modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"8el2k\",\"text\":\"16-32 bp in length - the longer, the better! 40-60 % GC-content (total primer)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"edg5v\",\"text\":\"For Tm >72 \\u00b0C, an annealing temperature of 72 \\u00b0C can be used.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":"Overhang part of primer:","section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":229926,"guid":"675F512861E9483486A42EEBA0D5DC52","previous_id":229909,"previous_guid":"8CA0FAC453D240FF9E3875F9125F2586","modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"7lvkd\",\"text\":\"Check primers for Tm, hairpin structures and primer dimers using online tool of choice (but stick with the same one after that, e.g. http:\\\/\\\/eu.idtdna.com\\\/calc\\\/analyzer)\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"79d84\",\"text\":\"Hairpin structures: Check the Tm of the structure. It should be lower than the annealing temperature used in the PCR reaction.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[{\"offset\":0,\"length\":18,\"style\":\"UNDERLINE\"}],\"entityRanges\":[],\"data\":[]},{\"key\":\"dkuvk\",\"text\":\"Primer dimers: Recommended \\u0394G is -9 kcal\\\/mol or more positive values. However, for primers containing palindromic sequences, such as restriction sites, a more negative \\u0394G is not uncommon. These primers can still work.\",\"type\":\"unordered-list-item\",\"depth\":0,\"inlineStyleRanges\":[{\"offset\":0,\"length\":13,\"style\":\"UNDERLINE\"}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":"Quality control","section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":229970,"guid":"37BAB60BB311469EAFBA04AA4FEA0C00","previous_id":229926,"previous_guid":"675F512861E9483486A42EEBA0D5DC52","modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"dnfeq\",\"text\":\"Before ordering, make sure your primers bind in the template you plan on using.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"offset\":40,\"length\":20,\"style\":\"bold\"}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":"Quality control","section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":229984,"guid":"815802D132B14A809DC4F711C5F76C17","previous_id":229970,"previous_guid":"37BAB60BB311469EAFBA04AA4FEA0C00","modified_on":0,"protocol_id":4668,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"59j3s\",\"text\":\"After PCR, fragments should be separated using gel-electrophoresis and extracted using a kit of choice. This minimizes contamination with template backbones that could yield false-positive clones.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":"Quality control","section_color":null,"critical":null,"critical_id":null,"duration":0}],"materials":[],"description":"{\"blocks\":[{\"key\":\"9jpkc\",\"text\":\"This protocol summarizes the process of primer design for assembly-based cloning methods, such as Gibson or AQUA-cloning.\\u00a0\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"9qqii\",\"text\":\"Well-designed primers are necessary for efficient cloning of any kind. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"9civb\",\"text\":\"This protocol can also be used to design primers for overlap extension PCR of linear fragments. Alternatively, overhangs can also include restriction sites for restriction\\\/ligation based cloning.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"4i2gl\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","changed_on":1522216802}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":625498,"guid":"F2CB8457DB2A42C9B9228788961B0259","previous_id":625497,"previous_guid":"6BC5E876E1744C8DAF20B73AC8B4AF93","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F03E8A1B8E34725A2326C17127D1BC5","order_id":1,"type_id":6,"title":"Section","source":{"title":"PCR of parts"}},{"id":1054724,"guid":"BB0AA69C7E63415B81914BB56BD3F92C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/wj6er2w.png\" \/><\/div><div class = \"text-block\">Prepare separate PCR reactions of each part that needs to be assembled.<\/div><table border><tr style = \"text-align:center;\"><td>  <\/td><td>A<\/td><td>B<\/td><\/tr><tr><td style = \"text-align:center;\">1<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">Component<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">Amount [\u00b5L]<\/td><\/tr><tr><td style = \"text-align:center;\">2<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">5x Q5 buffer<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">5<\/td><\/tr><tr><td style = \"text-align:center;\">3<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">5x High GC buffer<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">5<\/td><\/tr><tr><td style = \"text-align:center;\">4<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">dNTP mix, 10 mM each<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">0.5<\/td><\/tr><tr><td style = \"text-align:center;\">5<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">Primer fwd, 10 \u00b5M<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">0.5<\/td><\/tr><tr><td style = \"text-align:center;\">6<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">Primer rev, 10 \u00b5M<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">0.5<\/td><\/tr><tr><td style = \"text-align:center;\">7<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">template DNA<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">0.5<\/td><\/tr><tr><td style = \"text-align:center;\">8<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">Q5 Polymerase<\/td><td rowspan = \"\" colspan = \"\" style =\"display : table-cell;\">0.25<\/td><\/tr><\/table><div class = \"text-block\">Gel-purify fragment of correct size.<\/div><\/div>"}},{"id":1054725,"guid":"D2582DFE140F4816BDF50605183D1BEE","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Since the primers contain long overhangs, it might be necessary to try different annealing temperatures. I generally recommend starting with the annealing temperature corresponding only to the part of the primer which anneals to the template and disregarding the overhang for now.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"comments":[]}},{"id":1054726,"guid":"CCF9AB60A48911E9849335A826F41161","order_id":3,"type_id":20,"title":"reagents","source":{"id":768,"mol_weight":0,"name":"Q5 High-Fidelity DNA Polymerase - 100 units","linfor":"","url":"https:\/\/www.neb.com\/M0491","sku":"M0491S","cas_number":"","rrid":"","public":0,"vendor":{"name":"New England Biolabs","affiliation":null,"affiliations":[],"username":null,"link":"https:\/\/www.neb.com\/","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/2.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/2.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"id":2},"can_edit":0,"st