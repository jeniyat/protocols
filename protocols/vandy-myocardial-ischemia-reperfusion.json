transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":718830,"guid":"2952CD9041D611E999F3076F65FBA88B","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8B69ED3DBE8246B89738C121313882CB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"22E0721D698C407996FD0DA74881F668","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mice are anesthetized with pentobarbital (50 mg.kg, IP).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718831,"guid":"296236E041D611E999F3076F65FBA88B","previous_id":718830,"previous_guid":"2952CD9041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F8CEEE693D304D52A2D68AD9ED6F6B18","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5E3144FAE14C49D18BAAB8187ABA1205","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The ventral neck and left parasternal region isshaved and disinfected with Betadine followed by 70% alcohol.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718832,"guid":"296236E141D611E999F3076F65FBA88B","previous_id":718831,"previous_guid":"296236E041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3CBB0616B4D4444982CABA3507CCE8AE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B6BBD2EF6ACD46E6A82AF127A7A3BD1E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is positioned supineon a heating padand asmallincision is made through the skin underlying the trachea.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718833,"guid":"296236E241D611E999F3076F65FBA88B","previous_id":718832,"previous_guid":"296236E141D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2ED89B4A631449AABB3A8B9E55C7B619","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6F7FE03495B14A3D8612BB250B10DFB9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The trachea is exposed, a small puncture is made in the trachea, and endotracheal intubation is performed using a PE-50 tube.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718834,"guid":"29625DF041D611E999F3076F65FBA88B","previous_id":718833,"previous_guid":"296236E241D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D7D41B3CF38C480B84AD04D19F7E5380","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F33FF3FEF2C74E6BA5A4A42751E457AC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The endotracheal tube is connected to a small rodent ventilator (Harvard Apparatus) for mechanical ventilation of the mouse.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718835,"guid":"29625DF141D611E999F3076F65FBA88B","previous_id":718834,"previous_guid":"29625DF041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"00E27350FAE04F53861083A178341026","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FDF05103496440AFBBBFAE5BD9D636B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">ECG electrode leads are placed subcutaneously to monitor the ECG during myocardial infarction(MI).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718836,"guid":"4CF963D041D611E999F3076F65FBA88B","previous_id":718835,"previous_guid":"29625DF141D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8AD8F424B9474037845F61E38BF37087","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"06B0B28D43A2453E874B83A8F090723C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">With the use of a surgical microscope, a left thoracotomy is performed and the fourth intercostal space is entered using scissors and blunt dissection.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718837,"guid":"4CF9B1F041D611E999F3076F65FBA88B","previous_id":718836,"previous_guid":"4CF963D041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FFE413DF3A114177A65D850B9E38EF86","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"ECC7D1BAD157457A8697E057A84FB424","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">An8-0 silk suture is placed through the myocardium into the anterolateral left ventricular wall around the left anterior descending (LAD) coronary artery.A sterile PE-50 tube is placed against the LAD and the suture is tied against the tube resulting in ligation of the LAD. During the ischemic period, the surgical area is covered with sterile gauze soaked in warm saline. Following the ischemic period, the tube is removed and the LAD ligature cut resulting in restoration of blood flow and reperfusion.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718838,"guid":"4CF9D90041D611E999F3076F65FBA88B","previous_id":718837,"previous_guid":"4CF9B1F041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2957401F01AD422EBE30BB6554B503A9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"363116DFBBCD4C76A5DDC207E8C0935D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The chest is closed in layers with 7-0 sutures.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718839,"guid":"4D651DF041D611E999F3076F65FBA88B","previous_id":718838,"previous_guid":"4CF9D90041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6DD72CDB87F244919D9A196D2FB85EFC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A0160580F70C45CF9D307C3F862E3C2B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is gradually weaned from the ventilator to resolve any possible pneumothorax.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718840,"guid":"4D76AA2041D611E999F3076F65FBA88B","previous_id":718839,"previous_guid":"4D651DF041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F427575817A40F487209BD2CAE6BA96","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BE5E9FB5C4A145A4901987B08E7F8964","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once spontaneous respiration resumes, the endothracheal tube is removed and thetrachea incision is closed with 8-0 suture. Theskin is then closedwith 6-0 suture.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718841,"guid":"4D76AA2141D611E999F3076F65FBA88B","previous_id":718840,"previous_guid":"4D76AA2041D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CAF25D4D16E344639FB8BFFE5AAB4C2A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"40928A3DABCA46F0AA546CED9CC1D902","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouseis maintained on the heating pad until fully recovered from anesthesia.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718842,"guid":"4D76AA2241D611E999F3076F65FBA88B","previous_id":718841,"previous_guid":"4D76AA2141D611E999F3076F65FBA88B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F5FC597D19B42BFB10C45EE0C3995D2","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"74236601D45640FAAFC40809B32EC72A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Buprenorphineis administered SC immediately following surgery and every 8-12 hr for 72 hr.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Summary:<\/span><\/div><div class = \"text-block\">The most common cause of cardiovascular mortality in man is the outcome from myocardial ischemic injury. Accordingly, it is necesarry to study the corresponding process of heart injury recovery in many mouse models relevant to human cardiovascular disease. This protocol describes the surgical induction of myocardial injury via  transient occlusion of a coronary artery followed by reperfusion (ischemia-reperfusion injury).<\/div><\/div>","changed_on":1558014328}