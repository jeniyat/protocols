{"id":22833,"title":"Measurement of duodenal motility using implanted strain gauges","title_html":"<p>Measurement of duodenal motility using implanted strain gauges<\/p>","image":{"source":"https:\/\/s3.amazonaws.com\/protocols-files\/public\/d7524fed64f1ceeadb9d1e2d6845dfa7992a71325373793b96553c01c49c1209\/bv8q4a8e.png","placeholder":"https:\/\/s3.amazonaws.com\/protocols-files\/public\/d7524fed64f1ceeadb9d1e2d6845dfa7992a71325373793b96553c01c49c1209\/bv8q4a8e.png"},"doi":"dx.doi.org\/10.17504\/protocols.io.2irgcd6","doi_status":2,"uri":"measurement-of-duodenal-motility-using-implanted-s-2irgcd6","type_id":1,"published_on":1578492487,"parent_protocols":[],"parent_collections":[],"version_id":0,"created_on":1556719553,"categories":null,"creator":{"name":"Deborah Jaffey","affiliation":null,"affiliations":[{"affiliation":null,"url":null,"is_default":1}],"username":"deborah-jaffey","link":null,"image":{"source":"\/img\/avatars\/012.png","placeholder":"\/img\/avatars\/012.png"},"badges":[{"id":3,"image":{"source":"\/img\/badges\/silver.svg","placeholder":"\/img\/badges\/silver.svg"},"name":"Power author!"}],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"journal":null,"journal_name":null,"journal_link":null,"article_citation":null,"public":1,"has_versions":0,"link":null,"total_collections":0,"number_of_steps":9,"authors":[{"name":"Terry Powley","affiliation":"Purdue University","affiliations":[],"username":"terry-powley","link":null,"image":{"source":"\/img\/avatars\/018.png","placeholder":"\/img\/avatars\/018.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},{"name":"Zhenjun Tan","affiliation":"Purdue University","affiliations":[],"username":"zhenjun-tan","link":null,"image":{"source":"\/img\/avatars\/005.png","placeholder":"\/img\/avatars\/005.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},{"name":"M