":null,"website":null,"location":null,"affiliation":null,"status":{"is_visible":true,"access_level":0},"stats":{"files":[],"total_members":0,"total_followers":0,"total_child_groups":0,"total_parent_groups":0,"has_collaborations":0},"user_status":{"is_member":true,"is_confirmed":true,"is_invited":false,"is_owner":false,"is_admin":false,"is_following":false},"join_link":null,"token":null,"owner":{"name":" ","affiliation":null,"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null},"is_protocol_requested":0,"is_group_requested":0,"is_my":false,"is_request":false,"is_confirmed":1,"is_declined":0,"requester":{"name":" ","affiliation":null,"affiliation_url":null,"username":null,"link":null},"protocol":{"id":0,"title":"UC Davis - Atheroma Quantification","title_html":"UC Davis - Atheroma Quantification","image":{"source":null,"placeholder":null},"doi":null,"doi_status":0,"uri":"uc-davis-atheroma-quantification-ybwfspe","type_id":1,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0}},"created_on":1557765432,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":710231,"guid":"2D7AB190347E11E99953A58D788FF199","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B88FEA609B1B4DBB88EF22822C6F0D3E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"3BA2E02AC70B45E28D56586ABEFF2397","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mice are anesthetized with an intraperitoneal injection with 50 mg pentobarbital\/kg weight.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710232,"guid":"30710E30347E11E99953A58D788FF199","previous_id":710231,"previous_guid":"2D7AB190347E11E99953A58D788FF199","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3FC07B2F43E04A90AF40DB84ED0AB9A7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D93D02FFADC44025BDAA5122C433C918","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Immediately after sacrifice, aortas are perfused with phosphate-buffered saline to remove blood\nafter venting and nicking the heart.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710233,"guid":"393AA8A0347E11E99953A58D788FF199","previous_id":710232,"previous_guid":"30710E30347E11E99953A58D788FF199","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CA389401810F46D5B3F70CD6186EA7E8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9F6C0F045D64453D8AFE8767335DB486","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After careful removal of periadventitial fat, aortas are excised and fixed (4% followed by 10% paraformaldehyde overnight), and cut into three segments*\n      a.  Proximal segment including all of the aortic arch\n      b.  Midsegment consisting of the thoracic and suprarenal aorta\n      c.  Distal segment consisting of the infrarenal aorta proximal to the iliac bifurcation.<\/div><div class = \"text-block\"><span>      * <\/span><span style = \"font-weight:bold;\">Note:<\/span><span> aortas can also be analysed for a desired segment (for example: coronary sinus only, or proximal segment between aortic arch and the aortic valve only)<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710612,"guid":"31A16280351B11E9848C0B74043D5C98","previous_id":710233,"previous_guid":"393AA8A0347E11E99953A58D788FF199","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CEEA7FA0ACCB46F0A7D9E7D45F6C89EA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"47B02AE49CF74304A9A700E0DDB0CA80","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aortic segments are placed side-by-side in cryomolds with optimum cutting temperature (OCT) compound frozen on dry ice in a cryomold, and then stored at -80\u00baC.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710613,"guid":"38B0C9D0351B11E9848C0B74043D5C98","previous_id":710612,"previous_guid":"31A16280351B11E9848C0B74043D5C98","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"40D333ACC60840FF946F69A41F1E403F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"00EC67E800354A8C92DE5328D54E2F7A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tissues are later sectioned with a microtome cryostat (HM 500 M) to 10 \u00b5m thick taken at -30\u00b0C, discarding every other section. (This allows sampling of all three segments of the aorta, permitting direct comparisons of each.) <\/div><div class = \"text-block\">      a. Four sections per slide with a total of 9 slides <\/div><div class = \"text-block\">      b. 36 sections per segment and 108 cryosections per aorta  <\/div><div class = \"text-block\">      c. Total of 360 \u00b5m from each segment and 1,000 \u00b5m from each aorta.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/bdgca.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"6639F04AF05540068CDB6AA1A5E991C2","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/bdgca.jpg\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710614,"guid":"9ABA98E0351B11E9848C0B74043D5C98","previous_id":710613,"previous_guid":"38B0C9D0351B11E9848C0B74043D5C98","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1379717CB78D4B459023278E3BF66DC2","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E3D06603A4014428B377DEAF37C4292F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Quantitative morphometric analyses of aortic lesions is performed by staining cryosections with oil-red-O and Gill\u2019s hematoxylin <\/div><div class = \"text-block\">     a. oil-red-O: define and characterize neutral lipid in the vessel wall <\/div><div class = \"text-block\">     b. Gill\u2019s hematoxylin: provides histological assessment of lesions in artery segments<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710652,"guid":"8966EEF0352411E9A3FBB1A844244652","previous_id":710614,"previous_guid":"9ABA98E0351B11E9848C0B74043D5C98","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1B1BADC485FF4D9493017F9A3FED997A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FBD023946ED64A859A8587A04027BE5C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sections are analyzed for lesion location and area by direct imaging (Olympus BX-40 microscope with a DP11 camera with smart media chip).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710654,"guid":"95DDB2E0352411E9A3FBB1A844244652","previous_id":710652,"previous_guid":"8966EEF0352411E9A3FBB1A844244652","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DEAA588875714AF38A85DEA8145ACDB3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"69552E24121B476F9022BC156ACA2DEB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Atheromas are identified by a single observer blinded to experimental parameters, and mean lesion area (\u00b5m\u00b2 \u00b1 SE) determined for each atheroma by morphometric analysis with a computer-assisted imaging system (Image Pro Plus, version 4.1). <\/div><div class = \"text-block\">     a. The quantification data is highly reproducible having an intraobserver error of  ~6%.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710655,"guid":"AD06A760352411E9A3FBB1A844244652","previous_id":710654,"previous_guid":"95DDB2E0352411E9A3FBB1A844244652","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6869AD9A623248FEA76EFBDE9EA1C44C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CF17FF3B0B834FBD9E80AC64BFED4B1A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Atheroma distribution is expressed as percentage of lesions per segment.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710656,"guid":"AF94F360352411E9A3FBB1A844244652","previous_id":710655,"previous_guid":"AD06A760352411E9A3FBB1A844244652","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB7AA0A48A1D490899A7C3D63F90106B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0FC1BBD6C1774247A2DEEE6E321E816D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Qualitative morphologic assessment of lesion complexity is performed with light microscopy for the following parameters: absence or presence of foam cells, wall thickness, vessel wall architecture integrity, nuclear proliferation, and luminal extension.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710657,"guid":"BBB1B070352411E9A3FBB1A844244652","previous_id":710656,"previous_guid":"AF94F360352411E9A3FBB1A844244652","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0C1ABA782FDA4316B7162D3080F13117","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A7E91C7795834FC3AEDE3A30A03FDD5C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Lesion morphology, complexity, and severity are used to classify the stage of lesions per the classification of Stary.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":710659,"guid":"C3D19D60352411E9A3FBB1A844244652","previous_id":710657,"previous_guid":"BBB1B070352411E9A3FBB1A844244652","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F4BBAACF23A64FA9BFB5C3212B238DF3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"3D09611D792245EF8F086446B99B6686","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Lesion complexity can also be performed (e.g., for foam cells and macrophages)<\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Note: <\/span><span>the above method is for quantitative microscopy. It is also possible to provide en face analysis of aortic atheroma by Sudan IV staining.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[{"id":180965,"mol_weight":0,"name":"DPBS","linfor":null,"url":"https:\/\/www.thermofisher.com\/us\/en\/home\/technical-resources\/media-formulation.147.html","sku":"14190","cas_number":"","rrid":"","public":0,"vendor":{"name":"Invitrogen - Thermo Fisher","affiliation":null,"username":null,"link":"http:\/\/www.thermofisher.com\/us\/en\/home\/brands\/invitrogen.html","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":191},"can_edit":0,"stats":{"total_protocols":0}},{"id":180966,"mol_weight":0,"name":"formaldehyde","linfor":null,"url":"https:\/\/www.fishersci.com\/shop\/products\/formaldehyde-37-by-weight-with-preservative-certified-acs-fisher-chemical-7\/F791","sku":"F79","cas_number":"","rrid":"","public":0,"vendor":{"name":"Fisher Scientific","affiliation":null,"username":null,"link":"https:\/\/www.fishersci.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":31},"can_edit":0,"stats":{"total_protocols":0}},{"id":180967,"mol_weight":0,"name":"Tissue-Tek OCT","linfor":null,"url":"https:\/\/www.fishersci.com\/shop\/products\/tissue-tek-cryo-oct-compound-4-oz\/1437365","sku":"14-373-65","cas_number":"","rrid":"","public":0,"vendor":{"name":"Fisher Scientific","affiliation":null,"username":null,"link":"https:\/\/www.fishersci.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":31},"can_edit":0,"stats":{"total_protocols":0}},{"id":180968,"mol_weight":0,"name":"Oil-red-O","linfor":null,"url":"https:\/\/www.sigmaaldrich.com\/catalog\/product\/sial\/o0625?lang=en&region=US","sku":"O0625","cas_number":"","rrid":"","public":0,"vendor":{"name":"Sigma Aldrich","affiliation":null,"username":null,"link":"http:\/\/sigmaaldrich.com","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":17},"can_edit":0,"stats":{"total_protocols":0}},{"id":180969,"mol_weight":0,"name":"Gills hematoxylin","linfor":null,"url":"https:\/\/www.sigmaaldrich.com\/catalog\/search?term=Gills+hematoxylin&interface=All&N=0&mode=match%20partialmax&lang=en&region=US&focus=product","sku":"","cas_number":"","rrid":"","public":0,"vendor":{"name":"Sigma Aldrich","affiliation":null,"username":null,"link":"http:\/\/sigmaaldrich.com","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":17},"can_edit":0,"stats":{"total_protocols":0}}],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Summary: <\/span><\/div><div class = \"text-block\">Increased lipid accumulation in the artery wall and atherosclerotic lesion development are two hallmarks of atherosclerotic cardiovascular disease. Atheroma quantification and characterization assesses neutral lipid accumulation in the vessel wall, percentage of lesions per aortic segment (quantitative) and lesion morphology, complexity, and severity (qualitative).<\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Modified from: <\/span><span>Villablanca AC, et.al. J Cardiovasc Transl Res. 2009 Sep;2(3):289-99.<\/span><\/div><\/div>","changed_on":1557765432}