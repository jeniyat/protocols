: Calibri; font-size: 11pt; font-style: normal;\">If LyseBlue reagent has been used, the suspension should be mixed until all trace of blue has gone and the suspension is colorless. A homogeneous colorless sus- pension indicates that the SDS has been effectively precipitated.<\/span>"},"is_project":0},{"component_id":"115865","previous_id":"115861","original_id":"0","guid":"B35DABDCBD9F4A56844CA537A26DDCF8","previous_guid":"C5990E3C1D604ED2963228140FE383B5","component_type_id":"6","data_id":"0","data":"Lysis","order_id":"1","name":"Section","data_by_id":"0","type_id":"6","source_data":{"section":"Lysis"},"is_project":0},{"component_id":"115863","previous_id":"115865","original_id":"0","guid":"E65FC9E1C7D9418D99A337EB4261870C","previous_guid":"B35DABDCBD9F4A56844CA537A26DDCF8","component_type_id":"3","data_id":"554","data":"350","order_id":"2","name":"Amount","data_by_id":"1","type_id":"3","source_data":{"amount":"350","unit_id":"1","title":""},"is_project":0},{"component_id":"115864","previous_id":"115863","original_id":"0","guid":"B36DBAC0E9014DFFB29B2552C8740CC5","previous_guid":"E65FC9E1C7D9418D99A337EB4261870C","component_type_id":"14","data_id":"1","data":null,"order_id":"3","name":"Unit","data_by_id":"1","type_id":"14","source_data":[],"is_project":0},{"component_id":"115862","previous_id":"115864","original_id":"0","guid":"3DD79E3EAE804F49AC964E34516B0DD3","previous_guid":"B36DBAC0E9014DFFB29B2552C8740CC5","component_type_id":"20","data_id":"2215","data":null,"order_id":"4","name":"Reagents","data_by_id":"1","type_id":"20","source_data":{"product_type_id":"1","reagent_id":"2215","reagent_mol_weight":"0.00","reagent_name":"Buffer N3","reagent_linfor":"","reagent_vendor_id":"1","reagent_vendor_logo":"\/img\/vendors\/1.png","reagent_vendor_link":"https:\/\/www.protocols.io","reagent_vendor_name":"Contributed by users","reagent_url":"","reagent_sku":"","reagent_public":"1","can_edit":"0"},"is_project":0}],"available_protocols":["1991"]},{"id":"126424","is_changed