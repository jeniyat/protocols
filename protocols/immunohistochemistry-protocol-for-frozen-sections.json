,{"id":837144,"guid":"FEB84B70FB5A11E9AF1079F87FEE186E","previous_id":837143,"previous_guid":"FEB5B360FB5A11E9AF1079F87FEE186E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"11752C2073F74BABB912852B76B3767E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Immunostain frozen tissue sections:"}},{"id":1054724,"guid":"162E051E41334EC8B5084FC117D2D4C3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rinse the slides in 300 ml PBS for 2 changes, 5 min each. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":837145,"guid":"FEBB0A90FB5A11E9AF1079F87FEE186E","previous_id":837144,"previous_guid":"FEB84B70FB5A11E9AF1079F87FEE186E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"95DC728528594E5C86D2507A26449A6F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Immunostain frozen tissue sections:"}},{"id":1054724,"guid":"49EAFF8922064445A9208375947255D1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Optional: <\/span><span>Add 100 \u00b5l blocking buffer (e.g. 10% fetal bovine serum in PBS) onto the sections of the slides and incubate in a humidified chamber at room temperature for 1 hour.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":837146,"guid":"FEBDF0C0FB5A11E9AF1079F87FEE186E","previous_id":837145,"previous_guid":"FEBB0A90FB5A11E9AF1079F87FEE186E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"14117ADE16CA4C729186FBA06649FA59","order_id":1,"type_id":6,"title":"Section","source":{"title":"Immunostain frozen tissue sections:"}},{"id":1054724,"guid":"2E4D6081EF984967937061871355CB91","order_id":1,"type_id":