me":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false}],"versions":[],"groups":[],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":737719,"guid":"4FB71EF06CF111E9BEB1370FDC33C774","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C0FC622458C34C9CAB024E3D5E4938A6","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AA57A611F0574101B8CEE46B7899AA6C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend 500 ng of IDT gBlock gene fragment in 100 uL to make ~100 uM solution.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737720,"guid":"4FC269906CF111E9BEB1370FDC33C774","previous_id":737719,"previous_guid":"4FB71EF06CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"81BACEC32BD740889C51D2EA3E958A33","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E4A3578420F747F7A89BEC30153FCBFC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend 5 nmol of ssDNA IDT ultramer oligos in 50 uL to make 100 uM solution.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737721,"guid":"4FC290A06CF111E9BEB1370FDC33C774","previous_id":737720,"previous_guid":"4FC269906CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"13C111C1459C479E9FC4779F650E13AC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0300D535C29049E9AD9812FCC2E5553B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform gBlock PCR in order to amplify sufficient gblock for transfection (2X50ul reactions per gblock):<\/div><div class = \"text-block\">5 uL gBlock F primer (10 uM)<\/div><div class = \"text-block\">5 uL gBlock R primer (10 uM)<\/div><div class = \"text-block\">25 uL Q5 2X master mix<\/div><div class = \"text-block\">2 uL gBlock (100 uM)<\/div><div class = \"text-block\">18 uL H2O<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737728,"guid":"500FED506CF111E9BEB1370FDC33C774","previous_id":737721,"previous_guid":"4FC290A06CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5F000663DDDD4BEFB697F80A8B521416","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"27DEA06DE4C24AA6BF00A4769C342892","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pool duplicate gBlock PCRs and perform 1X Ampure bead clean-up, resuspending in 25 uL.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737729,"guid":"50271ED06CF111E9BEB1370FDC33C774","previous_id":737728,"previous_guid":"500FED506CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F23705D1D053480495FD4746FF0A1183","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"99E3FAF640EA479B87438FCB307A7CAB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Run a gel to confirm gBlock amplification and quantify using nanodrop or qubit.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737731,"guid":"502745E16CF111E9BEB1370FDC33C774","previous_id":737729,"previous_guid":"50271ED06CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"61A9D35B6D5E4766A0EFCBE90EC5B881","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CF0F3E49B891449FB1562F8B9F7F3FF0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">24 hours before transfection: plate 293T iCas9 cells into 24 well plates with 120K cells\/well in 0.5 mL 5 ug\/mL doxycycline-supplemented media.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737733,"guid":"50276CF16CF111E9BEB1370FDC33C774","previous_id":813233,"previous_guid":"CA0E65F0D80811E9ACA315FDCE679EB5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7DDD805360EF4ABBBFDE795094B40713","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5820A97DFE84488DB84E16CD69774E9F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">24 hours later, change to regular media.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737734,"guid":"502794006CF111E9BEB1370FDC33C774","previous_id":737733,"previous_guid":"50276CF16CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3C2F2E342ADD4521BA28BD0624536B52","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"74D3889E13194F84B0D6C2BCF4C5AC68","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Let cultures grow for nine days post-transfection, splitting as needed to prevent confluency (e.g. day 4 and 6) with 100 uL tryplE.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737735,"guid":"502794016CF111E9BEB1370FDC33C774","previous_id":737734,"previous_guid":"502794006CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A028C74107984A8CBD5F8A78DA2FD8FB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"301360C5C46146F4AFF2DE7BD6FEFB89","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Nine days post-transfection, harvest mRNA and gDNA:<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737736,"guid":"5027BB106CF111E9BEB1370FDC33C774","previous_id":811410,"previous_guid":"26424480D49A11E9B5D007FB90E889F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F6F04E9B57524D6EA6B0EEC8FDF47652","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"29090002D9A94E6C86BCD186307C893F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Measure RNA concentration and quality using nanodrop.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737737,"guid":"50B1F9B06CF111E9BEB1370FDC33C774","previous_id":737736,"previous_guid":"5027BB106CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BA70E33686D24A5580BB7EB0836E605F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CBACF50C76894066A796BE97314A5A22","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Synthesize cDNA using Vilo IV superscript 2x master mix with EZ DNase. Can use up to 2.5 ug of RNA. Pick a volume of RNA where most are around 1 ug and none exceed the 2.5 ug limit.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737738,"guid":"50D009006CF111E9BEB1370FDC33C774","previous_id":737737,"previous_guid":"50B1F9B06CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DB092A19151C4940A43ECFF59E65AE2F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"3944349F5DCC424EAEE533234572ADF4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use custom nextera primers specific to the cDNA sequence of the locus of interest to amplify cDNA. Separately, amplify quick extract gDNA using primers specific to gDNA. Use Q5 master mix 62 degree protocol to amplify 1 uL of cDNA or gDNA PCR reaction:<\/div><div class = \"text-block\">5 uL Q5 2x master mix<\/div><div class = \"text-block\">0.5 uL F&R primer mix (10 uM)<\/div><div class = \"text-block\">1 uL cDNA\/gDNA<\/div><div class = \"text-block\">3.5 uL H2O<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737742,"guid":"50D11A706CF111E9BEB1370FDC33C774","previous_id":737738,"previous_guid":"50D009006CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8B7B1554E97A42E596F172BCAF518B69","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"2C36B1046F994B67A2984DF948C613FA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform 1.2X ampure bead clean up (adjust based on size of amplified product).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737743,"guid":"50D141806CF111E9BEB1370FDC33C774","previous_id":737742,"previous_guid":"50D11A706CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B7E0981C9EA24069823E4FBCC3CFC751","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7274ED7F2FD846A39C94558078F9E5FF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform indexing PCR using Nextera indexing primers:<\/div><div class = \"text-block\">2.5 uL NEB Next master mix<\/div><div class = \"text-block\">0.5 uL Nextera indexing F&R primer mix (10 uM)<\/div><div class = \"text-block\">0.5 uL Clean Q5 PCR reaction<\/div><div class = \"text-block\">1.5 uL H2O<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737748,"guid":"516878706CF111E9BEB1370FDC33C774","previous_id":737743,"previous_guid":"50D141806CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"688833C21D2B4FAF88602B1BE4096E29","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1351FBA599BE4C839F51CC27288DC829","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pool samples equally and perform 1X ampure bead clean up on pooled library.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":737749,"guid":"51689F806CF111E9BEB1370FDC33C774","previous_id":737748,"previous_guid":"516878706CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C3AF0D93F2FF4D7DA20BEDCF219958A6","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AE974DF5F66C400BB9A63F3235B4A44B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Quantify using qubit\/tapestation and dilute for sequencing on Miseq.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":811404,"guid":"E746C350D49911E9B5D007FB90E889F3","previous_id":737735,"previous_guid":"502794016CF111E9BEB1370FDC33C774","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B938B4FF7185435CBE3698048D27ABB3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"ADEBE7E8677143BDB0F6823AA7B740DE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate media from transfected cultures.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":811405,"guid":"E91E4270D49911E9B5D007FB90E889F3","previous_id":811404,"previous_guid":"E746C350D49911E9B5D007FB90E889F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A75D2E6CDFE4ADAAE5365AD80EBA4AA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B877BDED560B4D4D892DA2AA2BAB5426","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 100 uL tryplE to each well. Let sit for 5 min, tapping gently occasionally to dissociate.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":811406,"guid":"E991C4C0D49911E9B5D007FB90E889F3","previous_id":811405,"previous_guid":"E91E4270D49911E9B5D007FB90E889F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5F3511D2E64444E0B467F45576247822","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DCCDD19337D749A2ADA935371137F4FC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 500 uL media to each well.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":811407,"guid":"00D14E80D49A11E9B5D007FB90E889F3","previous_id":811406,"previous_guid":"E991C4C0D49911E9B5D007FB90E889F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ACFC79472A304F41B73694FD9BE6D67C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"97C05142B7294519838894CC2CDA214D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For each well, pipette up and down 400 uL (x5) and move 400 uL to an Eppendorf tube (for RNA).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":811408,"guid":"0ED8C490D49A11E9B5D007FB90E889F3","previous_id":811407,"previous_guid":"00D14E80D49A11E9B5D007FB90E889F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1598C0D657D541EFA4929F1A8F557CC9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"98C8C135BB8D4F339185AEBBCBFB8F4E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin Eppendorf tubes at 1100 rpm at 4 degrees for 7 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":811409,"guid":"195C6B10D49A11E9B5D007FB90E889F3","previous_id":811408,"previous_guid":"0ED8C490D49A11E9B5D007FB90E889F3","modified_on":0,"protocol_id"