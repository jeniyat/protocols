odified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1DF0951B030540A4B7959B6C5FB321D9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"990AFA2DE7B1471880F5DA23CB4BBD71","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Expected procedure duration: <\/span><\/div><div class = \"text-block\">20-40 minutes<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721757,"guid":"12110860475111E9BC6F47998CB7FE9B","previous_id":721756,"previous_guid":"04CB82C0475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"465AF5A835F64BC09139980EBED53B19","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A07E615943774CEFA32E0D8636273531","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Adequacy or depth of anesthesia is monitored by: <\/span><\/div><div class = \"text-block\">Respiratory Rate and Toe Pinch<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721758,"guid":"25596750475111E9BC6F47998CB7FE9B","previous_id":721757,"previous_guid":"12110860475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4C25D10018AF44BB993F943E447A91D5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C50EC721F881444A9177E6DAA49869A4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Frequency of anesthesia depth assessment:<\/span><\/div><div class = \"text-block\">At the start of surgical procedure, a toe or ear pinch can be used to assess the depth of anesthesia. Visual monitoring should be performed thought-out the procedures, as well as toe\/ear pinches.\n<\/div><div class = \"text-block\">Deviations from expected behavior Should be noted.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721759,"guid":"3B512ED0475111E9BC6F47998CB7FE9B","previous_id":721758,"previous_guid":"25596750475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BAE616AC1C9D4C5C825F45FCFF8F573F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8B6908F3FCDE4B338630723019654697","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Anesthesia Regimen:<\/span><\/div><div class = \"text-block\">Ketamine (80-100mg\/kg), Xylazine (5-20mg\/kg) or Isoflurane 1-3%\n<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721760,"guid":"4DDE05F0475111E9BC6F47998CB7FE9B","previous_id":721759,"previous_guid":"3B512ED0475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"421D3BB1EDEC442B89C3665F7246F5E5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"575F5B367ADF4317923B69575891C4FA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Pre-surgical Analgesics:<\/span><\/div><div class = \"text-block\">Approximately 30 minutes prior to undergoing the surgical procedure, mice receive an S.C. injection of Buprenorphine (0.05mg\/kg) and Meloxicam (5mg\/kg).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721761,"guid":"60530F50475111E9BC6F47998CB7FE9B","previous_id":721760,"previous_guid":"4DDE05F0475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7DB4F4F0CC114191B9046C1BA949AF8B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A023726601B148F8ADB362B6AD153B92","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Surgical prep:<\/span><\/div><div class = \"text-block\">Aseptic technique will be maintained by:\nClipping\/shaving fur around incision site, Sterile Instruments.<\/div><div class = \"text-block\">\nIsoflurane, heating pad, forceps, scissors, needle driver, eye ointment, 5-0 Proline, 7-0 Proline suture, 20g iv catheter sheath, PE 90 tubing, 1 ml-syringes, 0.9% NaCl, Sterile gloves, Povidone- idoine, 70% ETOH<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721773,"guid":"83C464C0475111E9BC6F47998CB7FE9B","previous_id":721761,"previous_guid":"60530F50475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"95E1D06214A3432BA2553AD0B69AFBE0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9E47F83DF25D43619B019472E80F894F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Myocardial Ischemia\/Infarction Procedure:<\/span><\/div><div class = \"text-block\">(1). Anesthetize the mouse and ensure depth of anesthesia with a toe pinch.\n<\/div><div class = \"text-block\">(2). Shave the left thoracic area of the mouse.\n<\/div><div class = \"text-block\">(3). The mouse is intubated using a 22-G angiocatheter sheath and placed on a rodent ventilator with the left chest up, secure feet to surgical area with tape.\n<\/div><div class = \"text-block\">(4). Prep the surgical field with 70% isopropanol as well as Betadine solution and drape.\n<\/div><div class = \"text-block\">(5). A left side thoracotomy is performed between the 3rd and 4th intercostal space and opened approximately 6-7 mm with a small retractor. Use caution to avoid injury to the lung.<\/div><div class = \"text-block\">\n(6). After making an opening the pericardium, the left anterior descending coronary artery is\nidentified using a microscope. A 7-0 Proline suture is placed just under the descending coronary artery without entering the left ventricle. This can be tied permanently to ligate the LAD for an acute ischemia model or alternatively, the 7-0 proline suture is secured over a 3-4 mm piece of PE-90 tubing to ligate the LAD for 30 minutes, and then the suture is removed to facilitate reperfusion of the heart for an ischemia reperfusion model. The suture is placed 2mm below the left atrium for a \u201clarge\u201d infarction; 3 mm below the left atrium for a \u201cmedium\u201dinfarction; or 4 mm below the left atrium for a small infarction.\n<\/div><div class = \"text-block\">(7). Two separate 6-0 Proline sutures are place to close the rib cage\n<\/div><div class = \"text-block\">(8). A 20g iv catheter sheath is left between the sutures to evacuate air after closure is complete.\n<\/div><div class = \"text-block\">(9). The dermis is sutured closed with 6-0 Proline, Tissue clips or glued with surgical glue. <\/div><div class = \"text-block\">(10). Air is removed from thoracic cavity via the sheath and a 1-mL syringe. Gently pinching the skin around the sheath slowly draw back on the syringe while withdrawing the sheath. <\/div><div class = \"text-block\">(11). Animal is weaned off anesthesia and extubated when spontaneous breathing resumes. <\/div><div class = \"text-block\">(12). Injection 0.5 ml of 0.9% NaCl subcutaneously to each mouse to prevent dehydration.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721774,"guid":"29618CA0475211E9BC6F47998CB7FE9B","previous_id":721773,"previous_guid":"83C464C0475111E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F5A60836EA6844DF87214C05BDAC8D00","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B0DE7E1FFCD641F6B510B0B642B236FB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Post-procedure Analgesics:<\/span><\/div><div class = \"text-block\">Buprenorphine (0.05mg\/kg) every 12 hours, for 72 hours post-op. <\/div><div class = \"text-block\">Meloxicam (5mg\/kg) every 24 hours, for 72 hours post-op<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721775,"guid":"39FA8300475211E9BC6F47998CB7FE9B","previous_id":721774,"previous_guid":"29618CA0475211E9BC6F47998CB7FE9B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"93D76BC56922463481C87FB860F9BFEE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"22158E5B38604529AE3AE28E9DFF7D42","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Post-procedure Monitoring:<\/span><\/div><div class = \"text-block\">Mice are monitored 2x daily for the first 72 hours after the surgery. Thereafter, mice are monitored at least 3x per week.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[{"id":182044,"mol_weight":0,"name":"Ketamine","linfor":null,"url":"https:\/\/www.pattersonvet.com\/ProductItem\/078036637","sku":"07-803-6637  ","cas_number":"","rrid":"","public":0,"vendor":{"name":"Patterson Veterinary","affiliation":null,"username":null,"link":"https:\/\/www.pattersonvet.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_inter