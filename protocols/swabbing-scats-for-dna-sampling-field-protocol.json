move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"ECD6E56005B411EABF69A5FFE083BBB4","steps":[{"id":846205,"guid":"36F64F4005B611EABF69A5FFE083BBB4","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"63A8ED1B9AEB492B9EAFD79676178DE0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"06B008CCCC5E484B8B17EE0AFD6CEEE6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Before touching the scat:<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">photograph the scat<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">assign a sample number<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">record GPS coordinates<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">record any other required field data<\/li><\/ul><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846206,"guid":"36F7D5E005B611EABF69A5FFE083BBB4","previous_id":846205,"previous_guid":"36F64F4005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"98D37B5E248147EA83623DAF4A1BDECB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"667EBF9F4333482BB60D48A05811AEAB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Swab the scat before otherwise moving or disturbing it.<\/div><\/div>"}},{"id":1054725,"guid":"F2138FFE4DA545C8B7658B286319DAD4","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">Wear clean gloves for every sample. Change gloves as often as needed to avoid contamination.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846207,"guid":"36F8E75005B611EABF69A5FFE083BBB4","previous_id":846206,"previous_guid":"36F7D5E005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39150D9E92E7478F89FF82B408332014","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"580363F531B14559B24929654B67A6AB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Select a clean, unused swab and check that the paper seal is undamaged to ensure the swab is sterile (discard the swab if the seal is damaged).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846208,"guid":"36F9AAA005B611EABF69A5FFE083BBB4","previous_id":846207,"previous_guid":"36F8E75005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"08D4F566FE4948899A1A7BE2F97E23A9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C3EE3D57AB634ADF98CEE39E1B0B1C80","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Record the sample number and all other details on the swab tube label. Ensure the writing is clear and legible and will not smudge or wash off easily.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846209,"guid":"36FA6DF005B611EABF69A5FFE083BBB4","previous_id":846208,"previous_guid":"36F9AAA005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7D38F2CB61F2495ABCDFB20F91F4B441","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F0F3F2D89057484598FEA1106AEB02BD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Twist the swab to break the seal and carefully remove the swab from its case. Take care not to touch any part of the swab that was inside the collection tube and do not allow the swab to touch anything else that may contaminate it.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846210,"guid":"36FB7F6005B611EABF69A5FFE083BBB4","previous_id":846209,"previous_guid":"36FA6DF005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2FCB5ECB2B804C83A126F50625B2E972","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D165CF3C44764F54AA492CD3FAB5693D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If the scat still retains obvious moisture, there is no need to pre-moisten the swab. Proceed to step 8.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846211,"guid":"36FC69C005B611EABF69A5FFE083BBB4","previous_id":846210,"previous_guid":"36FB7F6005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A289A36CB505443EB26649E6D15E4C3A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"327A8982F9774B33B0525F486E49B259","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If the scat is dry (older scats), pre-moisten the swab before use: <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Immediately after opening the swab, open a 2ml tube of 1 x PBS <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Briefly dip the swab into the PBS solution \u2013 you just need to moisten the outside of the swab, it should not be         saturated or soaked for a long time <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Immediately close the tube of PBS to avoid contaminating it <\/li><\/ul><\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Briefly dip the swab into the PBS solution \u2013 you just need to moisten the outside of the swab, it should not be         saturated or soaked for a long time <\/li><li style = \"counter-reset:ol0;list-style-type:disc;\"><\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Immediately close the tube of PBS to avoid contaminating it <\/li><\/ul><\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><\/ul><\/div><\/div>"}},{"id":1054725,"guid":"93254B7D04F947ACA7EC2DF13ABB395C","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">ONLY dip clean swabs into the PBS solution \u2013 if the swab has touched ANYTHING (your hands, the ground, the scat...) do not dip it into the tube! Otherwise you risk cross-contamination of other swabs later on.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846212,"guid":"36FD7B3005B611EABF69A5FFE083BBB4","previous_id":846211,"previous_guid":"36FC69C005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A11C1D342ABA44419AB84E1A490FBF8E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B6DB1331F9E34584A15838D65D0449F4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Swab the scat comprehensively by rolling the tip of the swab across the surface of the scat several times. Make sure you sample from each scat pellet and from the ends and middle of the scat.<\/div><\/div>"}},{"id":1054725,"guid":"E6A4E6FE643F400990567B1DDAB30690","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">NB see this EcoGene video on YouTube for a demonstration of a similar procedure (swabbing starts at 1 min 38 secs): https:\/\/www.youtube.com\/watch?v=A7WFnoIttUA<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846213,"guid":"36FEDAC005B611EABF69A5FFE083BBB4","previous_id":846212,"previous_guid":"36FD7B3005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CB9F39CA1F124B1C8B52BD2C2BBB6AF8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"70C1F680E99E4451A6D11F2F53129492","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Briefly allow the swab to air dry \u2013 take care not to touch the swab against anything that may contaminate it while it dries.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846214,"guid":"3700134005B611EABF69A5FFE083BBB4","previous_id":846213,"previous_guid":"36FEDAC005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"865FA887CCAC45A0885F33EC724B0301","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8A12102732BA4D1B8FCD084C49129E93","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the swab back into its container and ensure it has properly closed. Swab containers can be stored in a box or bag during the field trip. They should be kept dry and out of direct sunlight. Store used swabs separately from unused swabs.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":846215,"guid":"370172D005B611EABF69A5FFE083BBB4","previous_id":846214,"previous_guid":"3700134005B611EABF69A5FFE083BBB4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"01EC65CDC27742CE949BEA495EFD4DAC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FF9D34B27627454DA2DCB88D1D37E8D5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In the lab, extract DNA from swabs as soon as possible, or else store swabs in dry conditions below 4\u00b0C (freeze if possible) to inhibit growth of micro-organisms.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[{"id":3592,"mol_weight":0,"name":"Gloves","linfor":"","url":"","sku":"","ca