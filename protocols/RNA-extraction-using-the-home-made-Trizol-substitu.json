","reagent_url":"http:\/\/www.sigmaaldrich.com\/catalog\/product\/sigma\/g5516","reagent_vendor_id":"1","reagent_vendor_link":"https:\/\/www.protocols.io","reagent_vendor_logo":"\/img\/vendors\/1.png","reagent_vendor_name":"Contributed by users","can_edit":"0"},{"product_type_id":"1","reagent_id":"3062","reagent_linfor":"","reagent_mol_weight":"0.00","reagent_name":"Phenol, Saturated, pH 4.3, Liquid","reagent_public":"1","reagent_sku":"BP1751I","reagent_url":null,"reagent_vendor_id":"31","reagent_vendor_link":"https:\/\/www.fishersci.com\/us\/en\/home.html","reagent_vendor_logo":"\/img\/vendors\/placeholder.png","reagent_vendor_name":"Fisher Scientific","can_edit":"0"},{"product_type_id":"1","reagent_id":"3063","reagent_linfor":"C2H6N4S","reagent_mol_weight":"0.00","reagent_name":"Guanidine Thiocyanate","reagent_public":"1","reagent_sku":"BP221","reagent_url":null,"reagent_vendor_id":"31","reagent_vendor_link":"https:\/\/www.fishersci.com\/us\/en\/home.html","reagent_vendor_logo":"\/img\/vendors\/placeholder.png","reagent_vendor_name":"Fisher Scientific","can_edit":"0"},{"product_type_id":"1","reagent_id":"3064","reagent_linfor":"NH4SCN","reagent_mol_weight":"0.00","reagent_name":"Ammonium Thiocyanate","reagent_public":"1","reagent_sku":"A709","reagent_url":null,"reagent_vendor_id":"31","reagent_vendor_link":"https:\/\/www.fishersci.com\/us\/en\/home.html","reagent_vendor_logo":"\/img\/vendors\/placeholder.png","reagent_vendor_name":"Fisher Scientific","can_edit":"0"},{"product_type_id":"1","reagent_id":"3065","reagent_linfor":"CH3COONa\u00b73H2O","reagent_mol_weight":"0.00","reagent_name":"Sodium Acetate, Trihydrate","reagent_public":"1","reagent_sku":"","reagent_url":null,"reagent_vendor_id":"1","reagent_vendor_link":"https:\/\/www.protocols.io","reagent_vendor_logo":"\/img\/vendors\/1.png","reagent_vendor_name":"Contributed by users","can_edit":"0"},{"product_type_id":"1","reagent_id":"3066","reagent_linfor":"CHCl3","reagent_mol_weight":"0.00","reagent_name":"Chl