earch_interests":null,"website":null,"location":null,"affiliation":null,"status":{"is_visible":true,"access_level":0},"stats":{"files":[],"total_members":0,"total_followers":0,"total_child_groups":0,"total_parent_groups":0,"has_collaborations":0},"user_status":{"is_member":true,"is_confirmed":true,"is_invited":false,"is_owner":false,"is_admin":false,"is_following":false},"join_link":null,"token":null,"owner":{"name":" ","affiliation":null,"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null},"is_protocol_requested":0,"is_group_requested":0,"is_my":false,"is_request":false,"is_confirmed":1,"is_declined":0,"requester":{"name":" ","affiliation":null,"affiliation_url":null,"username":null,"link":null},"protocol":{"id":0,"title":"U Mass - Chronic\/acute phloridzin treatment","title_html":"U Mass - Chronic\/acute phloridzin treatment","image":{"source":null,"placeholder":null},"doi":null,"doi_status":0,"uri":"u-mass-chronic-acute-phloridzin-treatment-xujfnun","type_id":1,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0}},"created_on":1557495873,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":705548,"guid":"24FB2E002A4211E99D667BDAE44627A5","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AAE43D288B1B49D3AA8D040FD863BAE3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CCE1A1E783024FE99629D810D2C57C36","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">For chronic phloridzin treatment: <\/span><\/div><div class = \"text-block\">1. Anesthetize mice with an intraperitoneal injection of ketamine (100 mg\/kg body weight) and xylazine (10 mg\/kg body weight). <\/div><div class = \"text-block\">2. Shave hair at the incision site on the back. <\/div><div class = \"text-block\">3. Make an incision (~0.5 cm) using sterilized scalpel between the scapulae. <\/div><div class = \"text-block\">4. Subcutaneously insert an Alzet mouse osmotic pump containing phloridzin (0.4 mg\/kg body weight). <\/div><div class = \"text-block\">5. Suture or close the incision site using sterilized staples. <\/div><div class = \"text-block\">6. Administer ketoprofen to minimize pain and house mice individually. <\/div><div class = \"text-block\">7. Alternatively, phloridzin may be chronically administered using a twice daily intraperitoneal injection at 0.4 mg\/kg body weight.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":705549,"guid":"4B9AE0002A4211E99D667BDAE44627A5","previous_id":705548,"previous_guid":"24FB2E002A4211E99D667BDAE44627A5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"29FF94BCEF92449EAA05E9F89AA63470","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CF5198DA03094ACC860F983BCFC64685","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">For acute phloridzin treatment:<\/span><\/div><div class = \"text-block\">1. Survival surgery is performed to establish a chronic indwelling catheter at 5~6 days prior to experiment for intravenous infusion.  (refer to M1023: Surgery-jugular vein cannulation) <\/div><div class = \"text-block\">2. Mice are fasted overnight (~15 hours) or for 5 hours prior to the start of experiment. <\/div><div class = \"text-block\">3. Place a mouse in a rat-size restrainer with its tail tape-tethered at one end. <\/div><div class = \"text-block\">4. Expose and flush the intravenous catheter using saline solution. Then, connect the catheter to the CMA Microdialysis infusion pump. <\/div><div class = \"text-block\">5. Phloridzin is intravenously infused at 100 \uf06dg\/kg\/min for 60~120 min to lower serum glucose levels in hyperglycemic, diabetic mice.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[{"id":180277,"mol_weight":0,"name":"Phloridzin","linfor":null,"url":"https:\/\/www.sigmaaldrich.com\/catalog\/product\/sigma\/p3449?lang=en&region=US","sku":"P3449","cas_number":"","rrid":"","public":0,"vendor":{"name":"Sigma-aldrich","affiliation":null,"username":null,"link":"https:\/\/www.sigmaaldrich.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":74},"can_edit":0,"stats":{"total_protocols":0}},{"id":180278,"mol_weight":0,"name":"Osmotic pump","linfor":null,"url":"http:\/\/www.alzet.com\/products\/guide_to_use\/pump_selection.html","sku":"1007D","cas_number":"","rrid":"","public":0,"vendor":{"name":"Alzet","affiliation":null,"username":null,"link":"http:\/\/www.alzet.com\/company\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":522},"can_edit":0,"stats":{"total_protocols":0}}],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Summary:<\/span><\/div><div class = \"text-block\">Phloridzin is a potent inhibitor of renal glucose reabsorption and may be used to lower serum glucose levels. Phloridzin may be administered chronically to induce glycosuria and lower hyperglycemia in diabetic mice.  Phloridzin may be administered acutely to reduce serum glucose levels for experiments in diabetic mice. <\/div><\/div>","changed_on":1557495873}