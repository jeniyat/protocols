umber_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[]},"created_on":1578299856,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"2AC0A6C0305A11EAA00BC547A53CE423","steps":[{"id":866733,"guid":"F2D50CA0305A11EAA00BC547A53CE423","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E1FF15DB439A410497FE2AC66D9462AC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare seeds"}},{"id":1054724,"guid":"CDFA9A994ABE4F06B065C88D88EE4FEA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Sterilize the Arabidopsis seeds (no more than +\/- 400 seeds) in 2ml Eppendorf tube with 1 ml of 50% household bleach (diluted with MQ water) - keep for 10-20 minutes<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":866734,"guid":"2B7163B0305B11EAA00BC547A53CE423","previous_id":866733,"previous_guid":"F2D50CA0305A11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D4F88DBC10324B8CB70802EEE028DF1A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare seeds"}},{"id":1054724,"guid":"B58D0E0FBD9C4BFD953C032BFC0E58C7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash the seeds with 2 ml of sterile MQ water - 5 to 8 times - preferably in the laminar hood<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":866735,"guid":"409AB7A0305B11EAA00BC547A53CE423","previous_id":866734,"previous_guid":"2B7163B0305B11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F758BD58F514866A6A32C07682A4AC3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare seeds"}},{"id":1054724,"guid":"A5DBB4E3189942CAAEE4EB4A5913D4DA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate the seeds in MQ water in the dark at 4C at least overnight, but not longer than one week, to break the seed dormancy<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":86400,"critical":null,"critical_id":null,"duration":0},{"id":866736,"guid":"54BC86A0305B11EAA00BC547A53CE423","previous_id":866735,"previous_guid":"409AB7A0305B11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"72FC3DA4EA2E4D7FA10BFEE6F118EFDE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"102536106C3641F5808CDA305C5FAE44","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using the large and sturdy scissors - cut the yellow pipette tip so that you only keep the wider stud - as in the picture <\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwhxg3kw.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"993AD64B34B049DE81CEA93279F30843","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwhxg3kw.jpg\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866737,"guid":"3D561E80305C11EAA00BC547A53CE423","previous_id":866736,"previous_guid":"54BC86A0305B11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9D0D3D455CAD459DAEAC2311C65DB74D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"52685B1A53BC4B45A42179CAFFC086B3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare Agar medium solution. Per 1 L use:<\/div><div class = \"text-block\">2.2 g Murashi Skoog nutrients<\/div><div class = \"text-block\">1 g MES buffer<\/div><div class = \"text-block\">adjust pH with KOH to 5.8<\/div><div class = \"text-block\">After adjusting pH add 10 g of Dashin Agar<\/div><div class = \"text-block\">Autoclave the solution or melt the agar in the microwave.<\/div><div class = \"text-block\">You can store the nutrient agar solution on your benchtop, at room temperature, for about two weeks, or at 65 C cabinet for up to three days. Before using make sure that the solution is liquid by warming it up in the microwave <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866738,"guid":"BFE371E0305C11EAA00BC547A53CE423","previous_id":866737,"previous_guid":"3D561E80305C11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FBD44669422D4AF9B43DE61DBB74B1D5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"6F43E11B79464A94AF6DF072280EF34F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the pipette studs into a clean PCR plate and add liquid nutrient agar solution so that it covers the bottom part - wait for the solution to solidify. <\/div><div class = \"text-block\">After the initial solidification of the media in the bottom part of the pipette tip - add more agar to fill the pipette tip<\/div><div class = \"text-block\">TIP: Make sure that your media forms a meniscus rather than a \"bulb\" - this will make it easier to place tiny Arabidopsis seeds without them sliding to the side<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwhyg3kw.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"E0CA5718BE2E4CE9A13F8EF7060F4F67","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwhyg3kw.jpg\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866739,"guid":"33AC40C0305D11EAA00BC547A53CE423","previous_id":866738,"previous_guid":"BFE371E0305C11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"579426429A204EB99943D97DF1E48BB4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"EA5709671E8D46CAA655A40E64A3E924","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the agar-filled pipette studs into the 1 ml tip box (they should be placed deeper into the box than in \"yellow tip box\"), fill the box using the liquid medium. Per 1L of liquid medium you need:<\/div><div class = \"text-block\">2.2 g Murashi Skoog nutrients<\/div><div class = \"text-block\">1 g MES buffer<\/div><div class = \"text-block\">adjust pH with KOH to 5.8<\/div><div class = \"text-block\">The above solution does not have to be autoclaved - unless you have a specific reason to do so. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866740,"guid":"71F8E450305D11EAA00BC547A53CE423","previous_id":866739,"previous_guid":"33AC40C0305D11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C7A70B2D29B0427F8071E52298401DA7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"546F850D85E44320824CACB7A9EA7593","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using 1 ml pipette tip - drop the Arabidopsis seeds that underwent 24h cold treatment on top of the agar in the pipette studs. Place the pipette tip boxes in the growth chamber set at 18C, 12h light\/dark cycle, 60% humidity for the seeds to germinate.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwhzg3kw.jpg\" \/><\/div><div class = \"text-block\">NOTE - it is important that the boxes are covered with the transparent lid - so the agar in the studs won't dry out<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh2g3kw.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"F3B6EAEC26E9404189C79F92F1F1CECE","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwhzg3kw.jpg\" \/><\/div>"}},{"id":1054726,"guid":"C647C80263B644B1AED9CD2062573F54","order_id":3,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh2g3kw.jpg\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866741,"guid":"E7070470305D11EAA00BC547A53CE423","previous_id":866740,"previous_guid":"71F8E450305D11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0E05C358E6E644B88F20DDA4A347134A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"E59DB2501E454168BCE6FC1491A558B7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make sure to check on the level of liquid media every day and replace the media completely every 5-7 days<\/div><div class = \"text-block\">Four days after placing the boxes into the growth room you should observe thegermination of your seeds. I would advise you to \"thin out\" the seedlings at this stage - so you have only one germinating seedling per agar stud<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh3g3kw.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"8AC1D142D72E44A3BD6C03B188F0C8FA","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh3g3kw.jpg\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866742,"guid":"383E0870305E11EAA00BC547A53CE423","previous_id":866741,"previous_guid":"E7070470305D11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1B1998E2B60441CEBBCE633CAB5A06C4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"CDDC5E87586A4E8882B569B5AEBDA721","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The roots should grow out of the agar studs between one and two weeks after germination. The level of media is adjusted as not to cover the studs completely, but rather be just below the plate holding the pipette tip studs<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh4g3kw.jpg\" \/><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh5g3kw.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"67EE2B3CB3124630B0F7A63903E19471","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh4g3kw.jpg\" \/><\/div>"}},{"id":1054726,"guid":"B4B0CBBC934C496C9A279F1D81EAD1A1","order_id":3,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh5g3kw.jpg\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866743,"guid":"8F91D660305E11EAA00BC547A53CE423","previous_id":866742,"previous_guid":"383E0870305E11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A56BFA00C1549919E725CAC6F0472E2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Prepare pipette studs"}},{"id":1054724,"guid":"059F194C5A1B4FFDBECD9B4358FBA84C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After two weeks of growth, the seedlings are transferred to larger hydroponics containers with 2L of solution per container <\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh6g3kw.jpg\" \/><\/div><\/div>"}},{"id":1054725,"guid":"108BC38731A844728A0C76F4E4CE6BA0","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/24ade3775508737138210a168177a0a1d3ca4b004ce7fb1ed98a69963474311a\/bwh6g3kw.jpg\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":3600,"critical":null,"critical_id":null,"duration":0},{"id":866744,"guid":"1AFE20F0305F11EAA00BC547A53CE423","previous_id":866743,"previous_guid":"8F91D660305E11EAA00BC547A53CE423","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2150FEAB75144FC69485713F1E73D5BD","order_id":1,"type_id":6,"title":"Section","sou