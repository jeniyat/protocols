ation":0},{"id":804059,"guid":"BEE6FD80C83211E99AB6ABA86DFC2EA8","previous_id":804058,"previous_guid":"BEE576E0C83211E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C8FC8813A0D944398D1CF9340A99BAF1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation of Hydra"}},{"id":1054724,"guid":"76DF8E68A19B4D15925B55BCCD31A912","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Incubate <\/span><span style = \"font-style:italic;\">Hydra<\/span><span> at 4\u00b0C for 30-60 min (can incubate several hours without obvious detriment\/benefit)<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":804060,"guid":"BEE83600C83211E99AB6ABA86DFC2EA8","previous_id":804059,"previous_guid":"BEE6FD80C83211E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9A318D2A61FB4A7883430AA9EBB523CB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation of Hydra"}},{"id":1054724,"guid":"D94AFD6944D347C29922D94CA51CAD9B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Resuspend <\/span><span style = \"font-style:italic;\">Hydra<\/span><span> in 1mL fresh 4\u00b0C DM<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":804061,"guid":"BEE92060C83211E99AB6ABA86DFC2EA8","previous_id":804060,"previous_guid":"BEE83600C83211E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BA1C1B6EF5E4416AA132DC3F4AF56BA3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation of Hydra"}},{"id":1054724,"guid":"07A636B0D972401B80040E6593EE89DE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Shear by glass Pasteur pipette ~10-15x until supernatant becomes slightly turbid, allow <\/span><span style = \"font-style:italic;\">Hydra<\/span><span> to settle, and discard supernatant <\/span><span style = \"font-weight:bold;\">[ST1- refer to guidelines]<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":804062,"guid":"BEEA0AC0C83211E99AB6ABA86DFC2EA8","previous_id":804061,"previous_guid":"BEE92060C83211E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"191A7DF7C2C749D99EFE9F31D2329C4B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation of Hydra"}},{"id":1054724,"guid":"CF7294F17E004B01A60C825509AE9BAA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Add 1mL fresh 4\u00b0C DM and shear by pipetting up and down with glass pasteur pipette ~1-1.5 min until solution becomes turbid, and approximately half of the available tissue is dissociated <\/span><span style = \"font-weight:bold;\">[ST2 - refer to guidelines]<\/span><span style = \"font-weight:bold;\"><\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":804063,"guid":"BEEB4340C83211E99AB6ABA86DFC2EA8","previous_id":804062,"previous_guid":"BEEA0AC0C83211E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4B1B0180629F497D9B7D2E4FA194D47B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation of Hydra"}},{"id":1054724,"guid":"4A56DEA66BB946F1B685511590F9780A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Let remaining large pieces settle. The supernatant contains the cells that will be collected and used for reaggregation<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":804065,"guid":"02DD5DE0C83311E99AB6ABA86DFC2EA8","previous_id":804063,"previous_guid":"BEEB4340C83211E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E27F37B0860045C4AB418D7625DBA119","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"F6D47C20B84E48BD903504F965869A1D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Aliquot supernatant (dissociated cell suspension from step 7) to new Eppendorf tubes at an approximate desired cell density <\/span><span style = \"font-weight:bold;\">[ST3 -refer to guidelines]<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804066,"guid":"02E17C90C83311E99AB6ABA86DFC2EA8","previous_id":804065,"previous_guid":"02DD5DE0C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E4B95C0EAED147B6807F35ADD002D225","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"4DF2A88D04AF473E9020192DA42DDC8E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Centrifuge aliquots in microcentrifuge at 800g for 6 min<\/span><span style = \"font-weight:bold;\"> [ST4 -refer to guidelines]<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804067,"guid":"02E32A40C83311E99AB6ABA86DFC2EA8","previous_id":804066,"previous_guid":"02E17C90C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FC902090F4394D1B92EDDEF76DC87350","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"3DAC331711834A179494DB43FF314EA8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Invert tubes for ~5-10 min to allow pellet to passively separate from the tube wall <\/span><span style = \"font-weight:bold;\">[ST5 -refer to guidelines]<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804068,"guid":"02E462C0C83311E99AB6ABA86DFC2EA8","previous_id":804067,"previous_guid":"02E32A40C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7C49AF12B53B402097D3F92320965947","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"B3DA981A9EAC44599D7A7A6D6F481815","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Transfer pellet to a 24-well plate containing a 50\/50 mixture of DM and HM+Penicillin\/Streptomycin (P\/S) or 100% DM <\/span><span style = \"font-weight:bold;\">[ST6 -refer to guidelines]<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804069,"guid":"02E59B40C83311E99AB6ABA86DFC2EA8","previous_id":804068,"previous_guid":"02E462C0C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"33CC103EA77340DB968D17E570DDD573","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"934ECA3DF6514AD18EFE4157CBAC3465","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Periodically during development, replace the medium with HM+P\/S<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804070,"guid":"02E6D3C0C83311E99AB6ABA86DFC2EA8","previous_id":804069,"previous_guid":"02E59B40C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"035AF6529F0A4A49AE54F02F37F6687C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"4D8713FF39DA446FB31EF7B18E09C690","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If starting from 50\/50 DM\/HM+P\/S, depending on the duration of the experiment, make the first medium exchange by 12hpa or 24hpa,  and make subsequent exchanges at ~24h intervals for the duration of the experiment.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804071,"guid":"02E80C40C83311E99AB6ABA86DFC2EA8","previous_id":804070,"previous_guid":"02E6D3C0C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BD67BD809C8C4AC39D563B11EF9AF9AA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"C734C4E3EE95442FA4C518F4F29BFCEA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>If starting with 100% DM, exchange into 50\/50 DM\/HM+P\/S  at ~4 hours post aggregation (hpa), exhange into HM+P\/S 100% at ~16hpa, and then continue to exange medium every ~24 hours  <\/span><span style = \"font-weight:bold;\">[ST7 - refer to guidelines]<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0},{"id":804072,"guid":"02E944C0C83311E99AB6ABA86DFC2EA8","previous_id":804071,"previous_guid":"02E80C40C83311E99AB6ABA86DFC2EA8","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"074097C5D619435D8F70FC9634303699","order_id":1,"type_id":6,"title":"Section","source":{"title":"Generating aggregates capable of developing into whole animals:"}},{"id":1054724,"guid":"3D1BE8B47CFA4D38932272BD4A3D346E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Enjoy your shiny, new <\/span><span style = \"font-style:italic;\">Hydra<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":null,"changed_on":1568420755}