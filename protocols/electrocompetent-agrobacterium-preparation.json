":620699,"guid":"7F5C3F7537BA42C3A9B2926168F86D78","previous_id":620698,"previous_guid":"C38631817D9443DE8F1DC3AEA0695C2F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6C29A3FAA78E45E0892951F30F76F1D5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EDDAE3D29C2C45CC9D17F62A5859188C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin cells at 5000 rpm for 15 minutes, resuspend (wash) in 1 volume cold,sterile H2O<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":620700,"guid":"D1973ED8E37E4B9E9C781C7DBA821C0C","previous_id":620699,"previous_guid":"7F5C3F7537BA42C3A9B2926168F86D78","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CA560A2DD81F49D299285B0DA41D658D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BA5B992708954276836C6855CD2F15FD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin cells at 5000 rpm for 15 minutes, resuspend (wash) in 0.5 volume cold,sterile H2O<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":620701,"guid":"A86FAE18D68F45A791D2A7E656A81DBF","previous_id":620700,"previous_guid":"D1973ED8E37E4B9E9C781C7DBA821C0C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F8D97D571BCE4B218DF018ED31244EC6","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"57474CDB145F4BF3BB0FC0811AC44E5E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin cells at 5000 rpm for 15 minutes, resuspend (wash) in 0.02 volume cold,sterile 10% (v\/v) gly