tion":0,"critical":null,"critical_id":null,"duration":0},{"id":856837,"guid":"BA4E5270188A11EA8977198D8A5E3265","previous_id":856835,"previous_guid":"AE9D7690188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"98E6C4DC81A3458F98C041C6D1359381","order_id":1,"type_id":6,"title":"Section","source":{"title":"Coat the plate:"}},{"id":1054724,"guid":"E60262FB536744899788F484C0132B0C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dilute Low-Endotoxin\/Azide-Free sterile unlabeled capture antibody to a final concentration of 0.5-4\u00b5g\/ml in sterile Coating Buffer and transfer 100\u00b5l\/well to the prepared ELISPOT plate.Note:\u00a0BioLegend's LEAF\u2122 and Ultra-LEAF\u2122 format antibodies are specifically designed for this assay.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856838,"guid":"BA522300188A11EA8977198D8A5E3265","previous_id":856837,"previous_guid":"BA4E5270188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1723A1C5CA474C00BC430ED7E74C2A2E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Coat the plate:"}},{"id":1054724,"guid":"95A0865D4E41456DB85B7A5E08397B02","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Store plates overnight in a humidified box at 4\u00b0C or at 37\u00b0C for \u22654 hours in a humidified atmosphere.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856840,"guid":"C8A6A7A0188A11EA8977198D8A5E3265","previous_id":856838,"previous_guid":"BA522300188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"94B70C84B78B4E0CBD5C27CD0DC6A1C2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Block the Plate:"}},{"id":1054724,"guid":"20E0AE0FBEC44A8FAD509A1BFD28A0B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash the plate 3 times with 200 \u00b5l\/well of sterile PBS, gently tapping plates dry on a clean paper towel between each wash.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856843,"guid":"C8AFCF60188A11EA8977198D8A5E3265","previous_id":856847,"previous_guid":"DB97BED0188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A485A5487D494CDF811C9347BD0104AB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Block the Plate:"}},{"id":1054724,"guid":"CC2E071B87F74742A39087698FDE6662","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 200\u00b5l\/well of sterile Blocking Buffer.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856844,"guid":"C8B32AC0188A11EA8977198D8A5E3265","previous_id":856843,"previous_guid":"C8AFCF60188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"18A131C2AD5C4756B5AA465811BAC95D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Block the Plate:"}},{"id":1054724,"guid":"5244B150E3D14C778DC57B663A467096","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Seal plate and incubate at room temperature for \u2265 1 hour.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856845,"guid":"C8B6AD30188A11EA8977198D8A5E3265","previous_id":856844,"previous_guid":"C8B32AC0188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"21BC854402364E789AE22F1DBF47C6D9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Block the Plate:"}},{"id":1054724,"guid":"013F329EB75D48639F6F131E48F22027","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Repeat step 5.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856846,"guid":"D35B2220188A11EA8977198D8A5E3265","previous_id":856840,"previous_guid":"C8A6A7A0188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"57B0398F4ABE4FA09CEBBD2EE1629D7D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Block the Plate:"}},{"id":1054724,"guid":"8FA2474B19EE427A9988C4AEA83E0897","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Since ELISPOT plates are more delicate than ELISA plates, they should be gently tapped and washed manually.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856847,"guid":"DB97BED0188A11EA8977198D8A5E3265","previous_id":856846,"previous_guid":"D35B2220188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F3C3BAD779846589458721D0E2C9198","order_id":1,"type_id":6,"title":"Section","source":{"title":"Block the Plate:"}},{"id":1054724,"guid":"729572E019DB4BF38944455CB4545FBF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Do not use an automatic plate washer, since this could compromise the integrity of the PVDF membrane.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856849,"guid":"E6039790188A11EA8977198D8A5E3265","previous_id":856845,"previous_guid":"C8B6AD30188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"29CFC38A6FD541CAAB410ACE85B7CA38","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"2DFD88FA5F8846AF90AF541B87ECBC10","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 100\u00b5l\/well of appropriate sterile antigen or mitogen solution diluted in appropriate sterile tissue culture (TC) medium.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856850,"guid":"E60915D0188A11EA8977198D8A5E3265","previous_id":856849,"previous_guid":"E6039790188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"35D3D5E4ACA140B89A69607A421786D7","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"E1559FB3F16E42A181A6C1FED8B9C7CA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Add 100\u00b5l\/well of cells diluted in sterile TC medium. Use 5 x 10<\/span><span style = \"vertical-align:super;\">4<\/span><span>\u00a0to 5 x 10<\/span><span style = \"vertical-align:super;\">5 <\/span><span>cells\/well.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856854,"guid":"E61DAF40188A11EA8977198D8A5E3265","previous_id":856860,"previous_guid":"0502C5D0188B11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"71A7BCA69CC84391BF1DF6B1F6F38BE8","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"63CEAAF571FC480EA6D62F6FC3DB1380","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Seal the plate and incubate at 37\u00b0C with 5% CO<\/span><span style = \"vertical-align:sub;\">2<\/span><span> in a humidified atmosphere for the optimal stimulation period.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856858,"guid":"FBE53F00188A11EA8977198D8A5E3265","previous_id":856850,"previous_guid":"E60915D0188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D8AF43AC431A440794063251EF9C7521","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"7D3EAC3BFBB444BBB557B5F9DD8B0643","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The minimum number of cells should be determined in preliminary experiments.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856859,"guid":"00F49090188B11EA8977198D8A5E3265","previous_id":856858,"previous_guid":"FBE53F00188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"90166DB275B74D74BF1AF21F236D4834","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"A2713E81BA2C46D2AF068BE2FF6D7A50","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">When determining the optimal number of cells to use, keep in mind the expected levels of expression of the target protein. If the expression is expected to be low, use a higher number of cells.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856860,"guid":"0502C5D0188B11EA8977198D8A5E3265","previous_id":856859,"previous_guid":"00F49090188B11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"870DE0B255174333B117FC65D5236CF2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"9D46A633AC334DAC986354636055A0E8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If the cells can withstand the environment, use a serum-free media. Serum contains proteins that can affect results or give a high background or nonspecific signal.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856861,"guid":"1C8C9F00188B11EA8977198D8A5E3265","previous_id":856854,"previous_guid":"E61DAF40188A11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9298D09F2C18466D996C7AE809AF5160","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"A384112F026D4169AF58ECA42D7D20D6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">BioLegend recommends a 24 hour incubation for IFN\u03b3, IL-2, and TNF\u03b1; and a 48 hour incubation for IL-4, IL-5, and IL-10 for most activation conditions.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856862,"guid":"224E9EC0188B11EA8977198D8A5E3265","previous_id":856861,"previous_guid":"1C8C9F00188B11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C82DB9D0E6304AAF9FC85A9CFF69CE23","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"D428A98475EA431F8597103B424D6E51","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Do not shake or move the plates while the cells are culturing. This will lead to spots that are not well-defined.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856863,"guid":"2893D840188B11EA8977198D8A5E3265","previous_id":856862,"previous_guid":"224E9EC0188B11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39E918FD50A94ED1860625CA451874BF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Set-Up Tissue Culture and Add Antigen or Mitogen:"}},{"id":1054724,"guid":"BC7B2971FEBB4D859EBB485E16403689","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If your cells take more than 48 hours to respond to stimulation, they can be treated with the stimulant in a separate 96-well plate prior to transferring to the ELISPOT plate.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":856866,"guid":"43696490188C11EA8977198D8A5E3265","previous_id":856863,"previous_guid":"2893D840188B11EA8977198D8A5E3265","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"00D6D24E9FCF43A8B1F7216380D0ABD9","order_id":1,"type_id":6,"title":"Section","source":{"titl