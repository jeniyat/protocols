order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c7127b51f594a2676dc282d091a933177f169025308344661c9487d675e5c71d\/btjx8ykw.png\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":835051,"guid":"9D2B5F90F75F11E9B8FB35341E92B80B","previous_id":835049,"previous_guid":"B7F0AA20F75E11E9B8FB35341E92B80B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0EF237A5E06E4E57AD6A3DE5A68AF707","order_id":1,"type_id":6,"title":"Section","source":{"title":"Manual ROIs"}},{"id":1054724,"guid":"B6169EE755034B65A2D45E71C344160D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Identify the ROI you want to segment on your brain.<\/div><div class = \"text-block\">There are resources on the internet, and in lab to help identify the tracts. For this protocol a Brain Atlas of the Ferret was utilized.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":838452,"guid":"B763B280FCE711E986E6F3DF1B4B060D","previous_id":835050,"previous_guid":"927DC2E0F75F11E9B8FB35341E92B80B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B797E349B7EC408A8AC5C4701125213B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Manual ROIs"}},{"id":1054724,"guid":"123253C7FC5F402FA3E1E941BADB3DAA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Select the \"Polygon Mode\" and on your ROI trace an outline.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c7127b51f594a2676dc282d091a933177f169025308344661c9487d675e5c71d\/btjz8ykw.png\" \/><\/div><div class = \"text-block\">Once you are happy with your shape select the accept button in the bottom right hand corner.<\/div><\/div>"}},{"id":1054725,"guid":"E92EA73318A54F95967B609D8777BC33","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c7127b51f594a2676dc282d091a933177f169025308344661c9487d675e5c71d\/btjz8ykw.png\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":838566,"guid":"6DDFCBB0FCEE11E986E6F3DF1B4B060D","previous_id":838452,"previous_guid":"B763B280FCE711E986E6F3DF1B4B060D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E2FB705F283B4D98BCFA9EFC9CD234D9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Manual ROIs"}},{"id":1054724,"guid":"949810A6FB624C809C507841A0237226","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Continue the previous step through the slices till your ROI is finished. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":838572,"guid":"95799FB0FCEF11E986E6F3DF1B4B060D","previous_id":838566,"previous_guid":"6DDFCBB0FCEE11E986E6F3DF1B4B060D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"37E0EE5DEE1C4C229480D45CEE50BBFF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Manual ROIs"}},{"id":1054724,"guid":"F3FDC3F5DAA94A299FB479B7F3108D1A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Lastly, save your ROI as a NIFTI file, and open it in ITK-SNAP to ensure there are no mistakes.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c7127b51f594a2676dc282d091a933177f169025308344661c9487d675e5c71d\/btj38ykw.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"C9DBFDF959094972A8C07B3A303DE2E2","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c7127b51f594a2676dc282d091a933177f169025308344661c9487d675e5c71d\/btj38ykw.png\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":838574,"guid":"29A46120FCF011E986E6F3DF1B4B060D","previous_id":839293,"previous_guid":"5E40E760FF4C11E9B3623D9B19C4DF90","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"75F2BE3CA1174614BC4003F298F42160","order_id":1,"type_id":6,"title":"Section","source":{"title":"Auto ROIs"}},{"id":1054724,"guid":"8459E373DAC9448198C47A072667397E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open up ITK-SNAP by typing:<\/div><\/div>"}},{"id":1054725,"guid":"16EC18B0001B11EA9253778E2C0EEA7D","order_id":2,"type_id":15,"title":"command","source":{"id":6655,"name":"cd \/rsgrps\/hutchinsone\/Singularity_Containers\nmodule load singularity\nsingularity run nklab-neurotools-v0.4.sif itksnap","command_name":"","command":"","os_name":"","os_version":""}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":839293,"guid":"5E40E760FF4C11E9B3623D9B19C4DF90","previous_id":838572,"previous_guid":"95799FB0FCEF11E986E6F3DF1B4B060D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"965A1DD409534FD2B8B2D899C72AC9E2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Manual ROIs"}},{"id":1054724,"guid":"B1E2