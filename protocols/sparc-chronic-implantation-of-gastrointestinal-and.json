nents":[{"id":1054723,"guid":"04F10A509FE742A1AC83A40E129010C4","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparation: Before first incision"}},{"id":1054724,"guid":"70BEAC5FF2214619B7BB8045F7A7BF0C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Induce anesthesia with 20mg\/kg ketamine (i.m.).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#f8e71c","critical":null,"critical_id":null,"duration":0},{"id":795238,"guid":"ED956BC2C0AB49D5B54881828C620015","previous_id":795275,"previous_guid":"7822EAF190174CD69D628E47FC54617E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B91DEAFF00B24B2A9A1F89B27CF65270","order_id":1,"type_id":6,"title":"Section","source":{"title":"First incisions and Tunneling"}},{"id":1054724,"guid":"7DF3770E03CD4A8D881D24DF8DF82B75","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make incisions in skin on the abdomenal midline (~4 cm) and dorsal neck (~3 cm).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#f5a623","critical":null,"critical_id":null,"duration":0},{"id":795239,"guid":"3EFE1B43D49B435CA2C99F2C68355432","previous_id":795238,"previous_guid":"ED956BC2C0AB49D5B54881828C620015","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31182C8663E94CB8AA7BE3BB9763D304","order_id":1,"type_id":6,"title":"Section","source":{"title":"First incisions and Tunneling"}},{"id":1054724,"guid":"A98D87A096634066ACEDFAEA9F124803","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use a trocar to tunnel under skin from the dorsal neck, along the back, and to the left side to exit at the abdominal incision.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795240,"guid":"96B2BE013A7F4DBBBDAE5D55841FCBB5","previous_id":795239,"previous_guid":"3EFE1B43D49B435CA2C99F2C68355432","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9DA73C2C3EFF4012A72580591B81309C","order_id":1,"type_id":6,"title":"Section","source":{"title":"First incisions and Tunneling"}},{"id":1054724,"guid":"681CDF8D007948698D3F5BE99040A4B6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Advance electrode leads and gastric catheter through tunnel with trocar and then remove trocar leaving the leads under the skin.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795241,"guid":"C89BB13B5A494603AB3EF6BE6BF9E240","previous_id":795270,"previous_guid":"AAC2C8D3BFE049FE86322855BEFB04ED","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5546CE6EBB2F4FC0BD788E7B46B28D02","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"7C2678A841434A05961E79D3FE16CF3A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the animal on its back.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#50e3c2","critical":null,"critical_id":null,"duration":0},{"id":795242,"guid":"A9F1739A3EB4463F8A515D13C0D95B12","previous_id":795271,"previous_guid":"72847E2C894F4ADB8BD116A0E8E4E318","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"17D24D2A172D4373BB0B5117F431C98D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"A8E25509DF90477F91E0314A619E7ED6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open abdominal cavity using a 3-point retractor (Balfour pediatric retractor; George Tiemann), applied to the lateral edges of the abdominal incision and sternum. This retractor is elevated using a vertical post attached to a surgical board under the animal.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795243,"guid":"93EC29A33C664EF6863E1FE8C0FFF9DE","previous_id":795242,"previous_guid":"A9F1739A3EB4463F8A515D13C0D95B12","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8A8D0FE37DCC4EA59FA692251FC46E7C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"FC8D80AF37F94A2288A0F7F3C01BDF4D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make incision (~0.5 cm) on the left lateral edge of the gastric fundus and insert gastric catheter with the tip resting in the antrum. Place a purse-string suture around the incision and tied. And, additional knot is made around the catheter and the free ends of this knot and purse-string suture are tied together. Finally surgical glue (Vetbond) is applied to the incision area.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795244,"guid":"7056C381F1984A8391D88446A1B111C7","previous_id":795243,"previous_guid":"93EC29A33C664EF6863E1FE8C0FFF9DE","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B48B07ABDD6E40FE95E3E49F830E5050","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"F9085A43BE734C878E901A0025DF1973","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In some experiments, a cuff electrode is placed on either the ventral or dorsal abdominal vagus nerve trunk. Suture a ground electrode lead to connective tissue on the greater curvature of the stomach.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795245,"guid":"612EBE62FFC04D9DAFD907A837A31C93","previous_id":795302,"previous_guid":"A52A6340BBA411E9B48823A83EF78C91","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E8977C17BDC840FC9DDDE8EE7F55A5AB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"E159731B7F2C4D4CA2F4BFDEC5AAA416","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place planar electrodes on surface of stomach and duodenum using 8-0 silk suture.  Suture a ground electrode lead to connective tissue on the greater curvature of the stomach<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795246,"guid":"DE52F0D21259419EA3422C4BFB68C447","previous_id":795245,"previous_guid":"612EBE62FFC04D9DAFD907A837A31C93","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BFAEC2E945CF41A7B5B28638ED4590D0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"C8E8C1545EAF4F8687DC23C416553B8F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Administer abdominal lavage with antibiotic (cefalexin, 1% w\/v) in saline (0.9%, 100 ml).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795247,"guid":"E52C78F6210048D19ED1CE8FCC7A6402","previous_id":795246,"previous_guid":"DE52F0D21259419EA3422C4BFB68C447","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"360DD6D34D7B46F4A6B3AC644DFD555E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"62FD818DDF754358939ED911AC17075F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Test electrode impedances.<\/div><\/div>"}},{"id":1054725,"guid":"7AC51890BBB611E98F5439980EA912C0","order_id":2,"type_id":28,"title":"equipment","source":{"brand":"Ripple LLC","sku":"Grapevine Neural Interface Processor and a Nano2 r","specifications":"","can_edit":false,"name":"Grapevine Neural Interface Processor and a Nano2 recording headstage","type":"electrophysiological recording and stimulation","link":null,"file":null,"image":null}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795248,"guid":"9749F22D5837414B90BA3BEDF9566915","previous_id":795247,"previous_guid":"E52C78F6210048D19ED1CE8FCC7A6402","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C4AEE9052CD545919D724A2B6CEC1A44","order_id":1,"type_id":6,"title":"Section","source":{"title":"Abdominal Surgery: Placement of electrodes and gastric tube"}},{"id":1054724,"guid":"7BBBA1CE74A6432394328A3FACFCD4BD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Close abdomen using absorbable suture for muscle (silk, 2-0) and monofilament (3-0) for the skin.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":795249,"guid":"E4746C3D402B4C678A354B898D568431","previous_id":795248,"previous_guid":"9749F22D5837414B90BA3BEDF9566915","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"55F075433C874C