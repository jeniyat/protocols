iption":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c2afa03162cf06e31b1e105e05ba6e23f1575a0aa042f1b395a783b8952f151d\/bu98bahx7.jpg\" \/><\/div>"}},{"id":1054726,"guid":"B7BCC1EE7E8E45B8A19129EF66543D40","order_id":3,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c2afa03162cf06e31b1e105e05ba6e23f1575a0aa042f1b395a783b8952f151d\/bu99bahx7.jfif\" \/><\/div>"}},{"id":1054727,"guid":"1ABE693140A04A1E9CD6E269FACBBC5B","order_id":4,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/c2afa03162cf06e31b1e105e05ba6e23f1575a0aa042f1b395a783b8952f151d\/bvasbahx7.gif\" \/><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#fd2959","section_duration":9000,"critical":null,"critical_id":null,"duration":0},{"id":855313,"guid":"F2EECB10178011EA92457BFEA12A1421","previous_id":855290,"previous_guid":"13B3A350176F11EA92457BFEA12A1421","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"66C32EC5A0414DBB906B3D451D44305D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Performing Microtomy"}},{"id":1054724,"guid":"CE52DFD0942A46FFA2660A7802A49142","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After your paraffin ribbon has floated on the waterbath for approximately 20-45 seconds, use your forceps or wooden stick applicator, select the section you wish to keep for affixing to the slide. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#fd2959","section_duration":9000,"critical":null,"critical_id":null,"duration":0},{"id":855329,"guid":"BB8FF8E0178711EA92457BFEA12A1421","previous_id":855313,"previous_guid":"F2