dges":[],"research_interests":null}],"versions":[],"groups":[],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":709918,"guid":"6B887FD033AC11E9A1AA4FE2540145C4","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8DB4059E7E344AA0BFF751195939A2F0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Washing Coverslips"}},{"id":1054724,"guid":"95D2971C80CB42819A4E9D1F9EEA9E6B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Boil cloverslips in 1M HCL for 5 minutes, allow to cool.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\"><span>Wash 3X with ddH<\/span><span style = \"vertical-align:sub;\">2<\/span><span>O.<\/span><\/li><li style = \"counter-reset:ol0;\">Wash for 30 min in 100% ethanol.<\/li><li style = \"counter-reset:ol0;\"> <\/li><li style = \"counter-reset:ol0;\"><span>Wash 3X with ddH<\/span><span style = \"vertical-align:sub;\">2<\/span><span>O. <\/span><\/li><li style = \"counter-reset:ol0;\">Cover flask with aluminum foil and autoclave.<\/li><\/ol><\/div><div class = \"text-block\">Wash for 30 min in 100% ethanol.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\"><span>Wash 3X with ddH<\/span><span style = \"vertical-align:sub;\">2<\/span><span>O. <\/span><\/li><li style = \"counter-reset:ol0;\">Cover flask with aluminum foil and autoclave.<\/li><\/ol><\/div><\/div>"}},{"id":1054725,"guid":"1E69589033AD11E9B10EEFFABF964D91","order_id":2,"type_id":4,"title":"duration","source":{"duration":300,"title":""}},{"id":1054726,"guid":"2E7CF25033AD11E9B10EEFFABF964D91","order_id":3,"type_id":4,"title":"duration","source":{"duration":1800,"title":""}}],"cases":null,"data":null,"section":null,"section_color":"#A492FF"},{"id":709923,"guid":"4F287B9033AE11E9B10EEFFABF964D91","previous_id":709918,"previous_guid":"6B887FD033AC11E9A1AA4FE2540145C4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A1BF91300D24D31BD66EEDE7A52180D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Coating Coverslips"}},{"id":1054724,"guid":"349E163CF3C54A2D82281D8E4E1482E2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Distribute 2 coverlips into each 35 x 10 mm petri dish with flamed forceps.<\/li><li style = \"counter-reset:ol0;\"> <\/li><li style = \"counter-reset:ol0;\">Dispense approximately 175 \u00b5l of 0.1 mg\/ml Poly-D-Lysine on each coverslip and incubate for 1 hour.<\/li><li style = \"counter-reset:ol0;\"> <\/li><li style = \"counter-reset:ol0;\"><\/li><li style = \"counter-reset:ol0;\"><span>Wash 3X with ddH<\/span><span style = \"vertical-align:sub;\">2<\/span><span>O.<\/span><\/li><li style = \"counter-reset:ol0;\">Allow to air dry at least 30 minutes.<\/li><li style = \"counter-reset:ol0;\"> <\/li><li style = \"counter-reset:ol0;\">Dilute laminin in MEM (5 \u00b5g\/ml final concentration) and coat coverslips with 100 \u00b5l of solution.<\/li><li style = \"counter-reset:ol0;\">Cover petri dishes to prevent evaporation of the solution. <\/li><\/ol><\/div><div class = \"text-block\">Dispense approximately 175 \u00b5l of 0.1 mg\/ml Poly-D-Lysine on each coverslip and incubate for 1 hour.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\"><span>Wash 3X with ddH<\/span><span style = \"vertical-align:sub;\">2<\/span><span>O.<\/span><\/li><li style = \"counter-reset:ol0;\">Allow to air dry at least 30 minutes.<\/li><li style = \"counter-reset:ol0;\"> <\/li><li style = \"counter-reset:ol0;\">Dilute laminin in MEM (5 \u00b5g\/ml final concentration) and coat coverslips with 100 \u00b5l of solution.<\/li><li style = \"counter-reset:ol0;\">Cover petri dishes to prevent evaporation of the solution. <\/li><\/ol><\/div><div class = \"text-block\">Allow to air dry at least 30 minutes.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Dilute laminin in MEM (5 \u00b5g\/ml final concentration) and coat coverslips with 100 \u00b5l of solution.<\/li><li style = \"counter-reset:ol0;\">Cover petri dishes to prevent evaporation of the solution. <\/li><\/ol><\/div><\/div>"}},{"id":1054725,"guid":"24C00C5033AF11E9B10EEFFABF964D91","order_id":2,"type_id":24,"title":"temperature","source":{"concentration":37,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"210B8FD033AF11E9B10EEFFABF964D91","order_id":3,"type_id":4,"title":"duration","source":{"duration":3600,"title":""}},{"id":1054727,"guid":"48994B5033AF11E9B10EEFFABF964D91","order_id":4,"type_id":4,"title":"duration","source":{"duration":108000,"title":""}}],"cases":null,"data":null,"section":null,"section_color":"#A492FF"},{"id":709978,"guid":"58092D3033BE11E9B10EEFFABF964D91","previous_id":709923,"previous_guid":"4F287B9033AE11E9B10EEFFABF964D91","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F0E378CBE7943A5A82AC7F2769BC22B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissociation Media Preparation"}},{"id":1054724,"guid":"2664327219694C0AA822A27DBDD012CD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepared fresh day of dissection.<\/div><div class = \"text-block\">Mix the following solutions in order, otherwise papain will not dissolve<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">15.5 ml Cysteine water<\/li><li style = \"counter-reset:ol0;\">400 units papain <\/li><li style = \"counter-reset:ol0;\">4 ml DISCO<\/li><li style = \"counter-reset:ol0;\">20 \u00b5l kynurenic acid<\/li><\/ol><\/div><div class = \"text-block\">Adjust pH to 7.4 with 5N HCl and sterile filter into Nunc vial<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#94EBFF"},{"id":709981,"guid":"E99CD39033BF11E9B10EEFFABF964D91","previous_id":709984,"previous_guid":"CBA77EC033C011E9B10EEFFABF964D91","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"10A1962B46CB43038C6084D6537A959A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Dissection"}},{"id":1054724,"guid":"1799FBF739474006BEA5AF52DF4A6F5B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">All orientation instructions assumes user is right-handed.<\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Place 10 cm petri dishes on ice and fill with Gey's Balanced Salt Solution (GBSS).<\/li><li style = \"counter-reset:ol0;\">Remove post-natal day 0 pups from mother in container with bedding and place on heating pad (set to low) until ready to use.<\/li><li style = \"counter-reset:ol0;\">Prepare a trough in ice and line with aluminum foil.<\/li><li style = \"counter-reset:ol0;\">Place pups on ice.<\/li><li style = \"counter-reset:ol0;\">Once movement has slowed and pups stop moving, rapidly decapitate and transfer to 10 cm petri dish.<\/li><li style = \"counter-reset:ol0;\">Cut skin to expose skull and remove the skull by cutting around the base with 3 mm Vanna Scissors.<\/li><li style = \"counter-reset:ol0;\">Using a spatula, gently push the brain from the skull from the front into GBSS.<\/li><li style = \"counter-reset:ol0;\">Place brain in center of stage, ventral surface upwards and anterior pointing left. <\/li><li style = \"counter-reset:ol0;\">Using 5A style forceps angled downward, pierce the brain through the rostral hypothalamus and pin to stage.<\/li><li style = \"counter-reset:ol0;\">Make first cut caudal to the midbrain flexture.<\/li><li style = \"counter-reset:ol0;\">Make second cut rostral to midbrain flexture, including caudal hypothalamus.<\/li><li style = \"counter-reset:ol0;\">Take resulting slice and orient ventral surface to the right.<\/li><li style = \"counter-reset:ol0;\">Hold slice in place by inserting forceps into slice at the acqueduct.<\/li><li style = \"counter-reset:ol0;\">Remove the hypothalamus by cutting the ventral portion.<\/li><li style = \"counter-reset:ol0;\">Isolate ventral midbrain by cutting approximately halfway between previous cut and acqueduct.<\/li><li style = \"counter-reset:ol0;\">Remove left and right meninges. <\/li><li style = \"counter-reset:ol0;\"><span>Dissect ventral midbrain into 1 mm<\/span><span style = \"vertical-align:super;\">3<\/span><span> sections and transfer to dissociation media using sterile transfer pipette. <\/span><\/li><\/ol><\/div><div class = \"text-block\">Incubate in dissociation media for 20 minutes.<\/div><\/div>"}},{"id":1054725,"guid":"5451A980352911E9859C87FA408498F3","order_id":2,"type_id":4,"title":"duration","source":{"duration":1200,"title":""}}],"cases":null,"data":null,"section":null,"section_color":"#A492FF"},{"id":709982,"guid":"ED369C2033BF11E9B10EEFFABF964D91","previous_id":709981,"previous_guid":"E99CD39033BF11E9B10EEFFABF964D91","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DD7BDD91476F44F28AF0FE7C5A838442","order_id":1,"type_id":6,"title":"Section","source":{"title":"Neuronal Media"}},{"id":1054724,"guid":"15C3984B6935485F8EA5611B61C29302","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Neuronal Media (Prepare fresh)<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">25 ml Neurobasal Plus<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">0.5 ml B-27 Plus<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">250 \u00b5l L-glutamine<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">For day one of plating add:<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">40 ul GDNF (1 ng\/ml)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">20 ul kynurenic acid<\/li><\/ul><\/div><div class = \"text-block\">For day one of plating add:<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">40 ul GDNF (1 ng\/ml)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">20 ul kynurenic acid<\/li><\/ul><\/div><div class = \"text-block\">Warm to 37\u00baC.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#94EBFF"},{"id":709983,"guid":"F032EC3033BF11E9B10EEFFABF964D91","previous_id":711073,"previous_guid":"A0FF1700358711E9859C87FA408498F3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2283E1C3BFD84A4FBE0CEDC961E0EF15","order_id":1,"type_id":6,"title":"Section","source":{"title":"Maintenance"}},{"id":1054724,"guid":"312470274EA94575A4752179618030D1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Every 4 days following plating, remove 1 ml of old media and replace with 1 ml of fresh Neuronal Media. Media can be changed more often, if necessary. <\/div><\/div>"}},{"id":1054725,"guid":"5B2277C3E3F6407B9FE58326CF2F9859","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">DIV 9-12 is the ideal window for general neuronal health and activity.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null},"comments":[]}}],"cases":null,"data":null,"section":null,"section_color":"#84CE84"},{"id":709984,"guid":"CBA77EC033C011E9B10EEFFABF964D91","previous_id":709978,"previous_guid":"58092D3033BE11E9B10EEFFABF964D91","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A37A683E0BE24223BDB56617B2C7131B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Pre-surgical preparation"}},{"id":1054724,"guid":"35F857A170E64889AB184EEE390B60AE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Clean 10 cm glass petri dish with 70% ethanol and allow to dry<\/li><li style = \"counter-reset:ol0;\">Using VetBond, glue a single Sylgard stage in the middle of the dish and allow to cure<\/li><li style = \"counter-reset:ol0;\">Warm water bath to 37\u00baC.<\/li><li style = \"counter-reset:ol0;\">Thaw Glia Media in water bath.<\/li><li style = \"counter-reset:ol0;\"><span>Transfer Dissociation media to water bath and oxygenate with 95% O<\/span><span style = \"vertical-align:sub;\">2 <\/span><span>5% CO<\/span><span style = \"vertical-align:sub;\">2<\/span><span>. Flow rate approximately 1 bubble per second.<\/span><\/li><\/ol><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#FFED92"},{"id":711073,"guid":"A0FF1700358711E9859C87FA408498F3","previous_id":709982,"previous_guid":"ED369C2033BF11E9B10EEFFABF964D91","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0C48084D27894C30A72622B9E5F16CF6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Plating"}},{"id":1054724,"guid":"A361B102DD974E7CA3EF4475D6528A13","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Following incubation, transfer tissue to biosafety cabinet. <\/li><li style = \"counter-reset:ol0;\">Add 5 ml of Glia Media to a 15 ml tube and transfer tissue, ensuring to transfer as little dissociation media as possible.<\/li><li style = \"counter-reset:ol0;\">Wash tissue 2-3x with 10 ml Glia Media, allowing tissue to settle between solution removal.<\/li><li style = \"counter-reset:ol0;\">After final wash, resuspend tissue in 5 ml Glia Media.<\/li><li style = \"counter-reset:ol0;\">Starting with large bore pipette, triturate tissue until it no longer decreases in size (10-20X) before switching to medium then small bore pipettes. Final appearance will be opalescent, indicating cells are in suspension. Pipette slowly to minimize the entrance of air bubbles and significant shear force. <\/li><li style = \"counter-reset:ol0;\">Centrifuge cell suspension for 5 minutes at 1550 RPM at room temperature.<\/li><li style = \"counter-reset:ol0;\">Remove supernatant and resuspend in 1 ml of Glia Media and pass through a 70 \u00b5m cell strainer.<\/li><li style = \"counter-reset:ol0;\">Aspirate 10 \u00b5l of cell suspension (ensure it does not settle) and mix thoroughly with 10 \u00b5l Trypan Blue Solution and count number of live cells \/ \u00b5l.<\/li><li style = \"counter-reset:ol0;\">Adjust cell concentration to 80,000 cells per 100 \u00b5l with Glia Media.<\/li><li style = \"counter-reset:ol0;\">Pipette 100 \u00b5l of suspension directly into the laminin coating solution on each coverslip, slowly and in a clockwise motion to ensure homogenous plating.<\/li><li style = \"counter-reset:ol0;\"><span>Allow cells to settle for 2 hours in incubator set to 37\u00baC with 5% CO<\/span><span style = \"vertical-align:sub;\">2<\/span><span>. <\/span><\/li><li style = \"counter-reset:ol0;\"> <\/li><li style = \"counter-reset:ol0;\"><\/li><li style = \"counter-reset:ol0;\">Gently flood dishes with 2 ml of Neuronal Media supplemented with GDNF and kynurenic acid.<\/li><li style = \"counter-reset:ol0;\">Transduce neurons with AAV5-hSyn-GCaMP6f (number of genome copies per cell = 30,000).<\/li><\/ol><\/div><div class = \"text-block\"><span>Allow cells to settle for 2 hours in incubator set to 37\u00baC with 5% CO<\/span><span style = \"vertical-align:sub;\">2<\/span><span>. <\/span><\/div><div class = \"text-block\"><ol style = \"list-style-type: decimal;\"><li style = \"counter-reset:ol0;\">Gently flood dishes with 2 ml of Neuronal Media supplemented with GDNF and kynurenic acid.<\/li><li style = \"counter-reset:ol0;\">Transduce neurons with AAV5-hSyn-GCaMP6f (number of genome copies per cell = 30,000).<\/li><\/ol><\/div><\/div>"}},{"id":1054725,"guid":"DCC46C10358A11E9859C87FA408498F3","order_id":2,"type_id":4,"title":"duration","source":{"duration":7200,"title":""}}],"cases":null,"data":null,"section":null,"section_color":null}],"materials":[{"id":118833,"mol_weight":0,"name":"200 mM L-Glutamine","linfor":null,"url":"","sku":"G7513","public":0,"vendor":{"name":"Sigma","affiliation":null,"username":null,"link":"www.sigma.com","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":114},"can_edit":0,"stats":{"total_protocols":0}},{"id":118834,"mol_weight":0,"name":"45% D-( )-Glucose","linfor":null,"url":"","sku":"G8769","public":0,"vendor":{"name":"Sigma","affiliation":null,"username":null,"link":"www.sigma.com","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":114},"can_edit":0,"stats":{"total_protocols":0}},{"id":118926,"mol_weight":0,"name":"Sylgard 184","linfor":null,"url":"http:\/\/www.dowcorning.com\/applications\/search\/default.aspx?R=131EN","sku":"","public":0,"vendor":{"name":"Dow Corning","affiliation":null,"