>"}}],"cases":null,"data":null,"section":null,"section_color":"#FFED92"},{"id":713297,"guid":"31DE848CB1E341BCBBB10B0A62AF0557","previous_id":713302,"previous_guid":"96D2BE4B330D4A0690B52A4E9BCDA042","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1F6779BAF5664EEC9C101F289BFB1ADE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Spin Down"}},{"id":1054724,"guid":"DD8D2FA38C904673837477BD03933D77","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge the cell suspension on 1.5 kRPM for <\/div><div class = \"text-block\">. <\/div><\/div>"}},{"id":1054725,"guid":"D4A00E402A4511E9BEAAD50A6849A494","order_id":2,"type_id":4,"title":"duration","source":{"duration":180,"title":""}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713298,"guid":"69A5F64DF0264046B863D11AE2B34FF9","previous_id":713297,"previous_guid":"31DE848CB1E341BCBBB10B0A62AF0557","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"865E6ACEB0C74F4AA8A181DEDABBE827","order_id":1,"type_id":6,"title":"Section","source":{"title":"Resuspend and Reseed"}},{"id":1054724,"guid":"0D2F16658B464E69AE50D0BB51CE18B1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove supernatant, dispose into waste beaker.<\/div><\/div>"}},{"id":1054725,"guid":"6B45C1F562E74D52B4E7B71318D6F750","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">You can leave a small amount with the serological pipette, the rest will be taken off in the next step.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null},"comments":[]}}],"cases":null,"data":null,"section":null,"section