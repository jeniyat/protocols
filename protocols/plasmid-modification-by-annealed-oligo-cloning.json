der_id":2,"type_id":3,"title":"amount","source":{"amount":5,"unit":"\u00b5l","title":""}},{"id":1054726,"guid":"285D90D0CEAF11E9B8BC3378202BBE51","order_id":3,"type_id":3,"title":"amount","source":{"amount":45,"unit":"\u00b5l","title":""}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","critical":null,"critical_id":null,"duration":0},{"id":776498,"guid":"35AB8CB09F6111E997BE7F85BA9969CB","previous_id":776497,"previous_guid":"35971A509F6111E997BE7F85BA9969CB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8C18E26B99AA47CAB16F46F964DFDD70","order_id":1,"type_id":6,"title":"Section","source":{"title":"Ligitation"}},{"id":1054724,"guid":"536761E062194682B9A4B02007B9AEB9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mix the annealed oligos with cut vector in molar ratios (vector:insert) between 4:3 and 1:6 in a standard ligation reaction (ex. to ligate an annealed oligo insert of 50bp in length into a 5kb vector, mix 100ng of the vector with <\/div><div class = \"text-block\">-<\/div><div class = \"text-block\"> of annealed oligos).<\/div><\/div>"}},{"id":1054725,"guid":"2CE0BD10CEB111E9B8BC3378202BBE51","order_id":2,"type_id":3,"title":"amount","source":{"amount":750,"unit":"\u00b5g","title":""}},{"id":1054726,"guid":"371691B0CEB111E9B8BC3378202BBE51","order_id":3,"type_id":3,"title":"amount","source":{"amount":6,"unit":"ng","title":""}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","critical":null,"critical_id":null,"duration":0},{"id":776499,"guid":"35AB8CB19F6111E997BE7F85BA9969CB","previous_id":776498,"previous_guid":"35AB8CB09F6111E997BE7F85BA9969CB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"54E2BF2DE8FC45AD815559BCF9BBEEFE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Ligitation"}},{"id":1054724,"guid":"96B4F06E302D4BB8AE0FCC755A821005","order_id":1,"type_id":1,"title":"description"