}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":821604,"guid":"B9318610E56C11E9865A6B6288333E0B","previous_id":821603,"previous_guid":"B93074A0E56C11E9865A6B6288333E0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ED2DF61693D749E6996222B059578F24","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E8A90912185B41FA983B7D4EA47FFA53","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Discard the supernatant. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":821605,"guid":"B9327070E56C11E9865A6B6288333E0B","previous_id":821604,"previous_guid":"B9318610E56C11E9865A6B6288333E0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"44214F4ECA00451FB468DE21DFCBC505","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7D916AFD9A864BF4BFA8C0A0145C3E31","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 2.5mL of MojoSort\u2122 Buffer.<\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Note: <\/span><span>If you observe aggregates, filter the suspension. To maximize yield, you can disrupt the aggregates by pipetting the solution up and down. <\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":821606,"guid":"B9335AD0E56C11E9865A6B6288333E0B","previous_id":821605,"previous_guid":"B9327070E56C11E9865A6B6288333E0B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"187A499E2BAD42C8A2C4B3950D6D2FB6","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CE8411A895A444ED9DC6E7EC27BD02BD","order_id":1,"type_id":1,"title"