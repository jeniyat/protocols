{"id":21218,"title":"Vandy - Hyperinsulinemic-euglycemic Clamp","title_html":"Vandy - Hyperinsulinemic-euglycemic Clamp","image":{"source":"https:\/\/s3.amazonaws.com\/pr-journal\/bea4a.png","placeholder":"https:\/\/s3.amazonaws.com\/pr-journal\/bea4a.png"},"doi":"dx.doi.org\/10.17504\/protocols.io.yyafxse","doi_status":2,"uri":"vandy-hyperinsulinemic-euglycemic-clamp-yyafxse","type_id":1,"published_on":1558013609,"version_id":0,"created_on":1552059505,"categories":null,"creator":{"name":"Lili Liang","affiliation":null,"username":"lili-liang","link":"https:\/\/mmpc.org\/shared\/document.aspx?id=239&docType=Protocol","image":{"source":"\/img\/avatars\/009.png","placeholder":"\/img\/avatars\/009.png"},"badges":[{"id":4,"image":{"source":"\/img\/badges\/gold.svg","placeholder":"\/img\/badges\/gold.svg"},"name":"Gold power author!"}],"research_interests":null},"journal":null,"journal_name":null,"journal_link":null,"public":1,"has_versions":0,"link":"https:\/\/mmpc.org\/shared\/document.aspx?id=239&docType=Protocol","total_collections":0,"number_of_steps":17,"authors":[{"name":"Li Kang","affiliation":"Vanderbilt University","username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null}],"versions":[],"groups":[{"id":987,"uri":"mouse-metabolic-phenotyping-centers","title":"Mouse Metabolic Phenotyping Centers","image":{"source":"https:\/\/s3.amazonaws.com\/pr-journal\/bajm36cw.png","placeholder":"https:\/\/s3.amazonaws.com\/pr-journal\/bajm36cw.png"},"tech_support":{"email":"info@mmpc.org","phone":"","hide_contact":0,"use_email":1},"is_member":1,"request":{"id":987,"uri":"mouse-metabolic-phenotyping-centers","title":"Mouse Metabolic Phenotyping Centers","image":{"source":"https:\/\/s3.amazonaws.com\/pr-journal\/bajm36cw.png","placeholder":"https:\/\/s3.amazonaws.com\/pr-journal\/bajm36cw.png"},"tech_support":{"email":"info@mmpc.org","phone":"","hide_contact":0,"use_email":1},"is_member":1,"description":null,"research_interests