,\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"bvvoa\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"9au81\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[],\"guid\":\"2FFF8A50368411E9B6F7A1EB175181FB\",\"id\":711481,\"previous_guid\":\"28A118A0368411E9B6F7A1EB175181FB\",\"section\":null}","section":null,"section_color":null},{"id":711482,"guid":"3074E160368411E9B6F7A1EB175181FB","previous_id":711481,"previous_guid":"2FFF8A50368411E9B6F7A1EB175181FB","modified_on":0,"protocol_id":20693,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"bev20\",\"text\":\"Gently layer the diluted blood from step 3 onto the Ficoll from step 2. This can be done free-hand or using a graduated pipette and pipette man\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[],\"guid\":\"3074E160368411E9B6F7A1EB175181FB\",\"id\":711482,\"previous_guid\":\"2FFF8A50368411E9B6F7A1EB175181FB\",\"section\":null}","section":null,"section_color":null},{"id":711483,"guid":"30BE4670368411E9B6F7A1EB175181FB","previous_id":711482,"previous_guid":"3074E160368411E9B6F7A1EB175181FB","modified_on":0,"protocol_id":20693,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"4lndv\",\"text\":\"Once the blood has been layered and tubes secured, remove from hood and insert into benchtop centrifuge. \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"d31hp\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"5u9li\",\"text\":\"\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[],\"guid\":\"30BE4670368411E9B6F7A1EB175181FB\",\"id\":711483,\"previous_guid\":\"3074E160368411E9B6F7A1EB175181FB\",\"section\":null}","section":null,"section_color":null},{"id":711485,"guid":"43BB6B90368411E9B6F7A1EB175181FB","previous_id":711520,"previous_guid":"D2749D90368C11E9B6F7A1EB175181FB","modified_on":0,"protocol_id":20693,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"3abs1\",\"text\":\"Once centrifuge has stopped, remove tubes and place in hood for further processing.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[],\"guid\":\"43BB6B90368411E9B6F7A1EB175181FB\",\"id\":711485,\"previous_guid\":\"30BE4670368411E9B6F7A1EB175181FB\",\"section\":null}","section":null,"section_color":null},{"id":711486,"guid":"4407B6D0368411E9B6F7A1EB175181FB","previous_id":711518,"previous_guid":"BBF381D0368C11E9B6F7A1EB175181FB","modified_on":0,"protocol_id":20693,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"3svl0\",\"text\":\"\\u00a0Wash isolated PBMCs twice in 50mL of PBS. Centrifuge cell suspension at 400xg for 10 min at room temperature with the brake and accelerator set to max.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[],\"guid\":\"4407B6D0368411E9B6F7A1EB175181FB\",\"id\":711486,\"previous_guid\":\"43BB6B90368411E9B6F7A1EB175181FB\",\"section\":null}","section":null,"section_color":null},{"id":711487,"guid":"4452A280368411E9B6F7A1EB175181FB","previous_id":711486,"previous_guid":"4407B6D0368411E9B6F7A1EB175181FB","modified_on":0,"protocol_id":20693,"components":[],"cases":null,"data":"{\"blocks\":[{\"key\":\"5m50n\",\"text\":\"Dilute cells in 1:10 ratio with PBS and trypan blue. Count the cells using haemocytometer and microscope. Record observed cell number and cell viability\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[],\"guid\":\"4452A280368411E9B6F7A1EB175181FB\",\"id\":711487,\"previous_guid\":\"4407B6D0368411E9B6F7A1EB175181FB\