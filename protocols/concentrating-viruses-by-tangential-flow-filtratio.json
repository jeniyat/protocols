er_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[]},"created_on":0,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":0,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":0,"can_transfer":0,"can_download":1,"is_locked":0},"guid":"88A53951B9DA11E99CA40242AC110005","steps":[{"id":219123,"guid":"C5534C870EAB43D6BF0416B0CBC058E3","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"5bq93\",\"text\":\"Centrifuge lysate in the Sorvall Lynx 4000 centrifuge at 5,000 rpm, 5 min, 4\\u00baC. Discard the pellets.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219190,"guid":"30B107D93C7F4E15ACD2476D23D961A2","previous_id":219123,"previous_guid":"C5534C870EAB43D6BF0416B0CBC058E3","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"mqeo\",\"text\":\"Add Triton X-100 to the lysate supernatants for a final concentration of 1% (from a 10 or 20% stock).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219243,"guid":"43F7D6A42F2F4BF1BA3CB14747B8C199","previous_id":219190,"previous_guid":"30B107D93C7F4E15ACD2476D23D961A2","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"11sk3\",\"text\":\"Centrifuge the lysate in the Sorvall WX Ultra Series\\u00a0ultracentrifuge\\u00a0at 17,000 rpm, 50 min, 4\\u00baC. Discard the supernatants.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219333,"guid":"F6C212BD71CF440C8D19BD757FA4C646","previous_id":219243,"previous_guid":"43F7D6A42F2F4BF1BA3CB14747B8C199","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"3anis\",\"text\":\"Resuspend the virus pellets with a small volume of 50 mM Tris-HCl, pH 7.8\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"fp9dc\",\"text\":\"*Approximately 1.0 mL per 100 mL lysate\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219395,"guid":"E5B9313C372B410EB21FFD1B90BBD193","previous_id":219333,"previous_guid":"F6C212BD71CF440C8D19BD757FA4C646","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"fjai1\",\"text\":\"Layer the virus suspension onto 100-400 mg\\\/mL (10-40%) linear sucrose gradients equilibriated with 50 mM Tris-HCl made in polypropylene\\u00a0tubes (layer approximately 3-4 mL per gradient).\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"2v34g\",\"text\":\" \",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]},{\"key\":\"bdj5\",\"text\":\"*To make sucrose stocks, add 10-40% sucrose to Tris-HCl and autoclave.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219548,"guid":"63433B236F164B958C9D6C6AE51E9F93","previous_id":219395,"previous_guid":"E5B9313C372B410EB21FFD1B90BBD193","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"6unat\",\"text\":\"Centrifuge the gradients in the ultracentrifuge\\u00a0at 20,000 rpm, 20 min, 4\\u00baC. The virus will be the major band about 1\\\/2 to 2\\\/3 deep in the gradient.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219631,"guid":"E66D5334D10640FBAA496FAE2A068253","previous_id":219548,"previous_guid":"63433B236F164B958C9D6C6AE51E9F93","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"fli28\",\"text\":\"Remove the virus bands from the gradients with sterile needles and transfer to 30 mL polypropylene centrifuge tubes. Split the virus from 3 gradients between two tubes. Slowly dilute the virus to the tube volume with 50 mM Tris-HCl. Centrifuge the tubes in the ultracentrifuge\\u00a0at 27,000 rpm, 3 hours, 4\\u00baC. Discard the supernatants.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":219777,"guid":"09A6695480084E9D82E62717D977E865","previous_id":219631,"previous_guid":"E66D5334D10640FBAA496FAE2A068253","modified_on":0,"protocol_id":4633,"components":[],"cases":[],"data":"{\"blocks\":[{\"key\":\"7g77n\",\"text\":\"Resuspend the virus pellets with a small volume of 50 mM Tris-HCl. Store the virus at 4\\u00baC. Do not freeze. Filter sterilization using a 0.45 um cellulose acetate filter is recommended.\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"{\"blocks\":[{\"key\":\"bfpb\",\"text\":\"Adapted from: Van Etten, J. (n.d.). Titering of Chlorella\\u00a0Viruses. Retrieved from http:\\\/\\\/ncv.unl.edu\\\/vanettenlab\\\/\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[{\"offset\":48,\"length\":10,\"style\":\"italic\"}],\"entityRanges\":[],\"data\":[]}],\"entityMap\":[]}","changed_on":1525975211}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":868973,"guid":"7803BBE602A241CEB483376DBF792493","previous_id":868972,"previous_guid":"B74A0D4E88634392A7BDCD7A2026A31F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F8F95A66C1F4D19B8E12C7F4BF483FA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Titering"}},{"id":1054724,"guid":"1FE8E816304C461293B6BF67E8EA45E9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Titer the sample by plaque assay and determine the\u00a0actual concentration compared to the theoretical.<\/div><div class = \"text-block\"><span>*Theoretical concentration = (V<\/span><span style = \"vertical-align:sub;\">1<\/span><span>*C<\/span><span style = \"vertical-align:sub;\">1<\/span><span>) = (V<\/span><span style = \"vertical-align:sub;\">2<\/span><span>*C<\/span><span style = \"vertical-align:sub;\">2<\/span><span style = \"vertical-align:sub;\">)<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":868974,"guid":"4B974C93B2D84E17BD9E6C59D9A153EA","previous_id":868973,"previous_guid":"7803BBE602A241CEB483376DBF792493","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AADC02F8610E465AB63DFD0186740DBA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Machine Cleaning"}},{"id":1054724,"guid":"67E738FD063A4020A694F7E21AD1E42D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Clean the TFF system with NaOH in the same process as in the preparation steps. Rinse with MilliQ.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":868975,"guid":"7353FF4BCF0E43D2A057D461FE8645E4","previous_id":868974,"previous_guid":"4B974C93B2D84E17BD9E6C59D9A153EA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7A6A758E52B6406CB7037F19A11A0461","order_id":1,"type_id":6,"title":"Section","source":{"title":"Machine Cleaning"}},{"id":1054724,"guid":"B0460D95445F4FE191824B3C8E031686","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using a syringe filter, fill the Pellicon filter with 0.1 N NaOH, 4\u00b0C until the next use.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"section_duration":0,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">Ultracentrifugation in the WX Ultra Series can support only 30 mL samples at a time, and therefore do not significantly concentrate viruses. Tangential flow filtration enables viral concentration of large volumes that can then be purified using sucrose density gradients.\u00a0<\/div><div class = \"text-block\">Contact Dr. Steven Wilhelm (wilhelm@utk.edu) or Samantha Coy (srose16@vols.utk.edu) for additional information regarding this protocol.<\/div><\/div>","changed_on":1578930763}