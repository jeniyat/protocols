er_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[]},"created_on":1575594006,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"2538FBA18E604FA48DEA8B7656CDB52A","steps":[{"id":855814,"guid":"4F90AD3017C111EA9DBBC50BC196CC31","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"41CA69EC29E64B33B83334000561D759","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"0C73416B09C34956A0E8A86AE8E0C7F5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Culture between 1-15 million cells. Collect cells by spinning down at 500xg at 4\u00b0C for 5 minutes. Wash cells with PBS at room temperature.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855815,"guid":"4F931E3017C111EA9DBBC50BC196CC31","previous_id":855814,"previous_guid":"4F90AD3017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"591023CC636149208B782187C6DB21EB","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"0259AF2C9B234F82B4EEC64B0E53CA4F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the PBS and add freshly made basic cell culture media (it should not contain any serum or large molecular weight proteins to avoid any interferences from those proteins) containing 1% formaldehyde (CH2O) to cross-link the DNA-protein complexes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855816,"guid":"4F95B64017C111EA9DBBC50BC196CC31","previous_id":855815,"previous_guid":"4F931E3017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D8877B4E31DD48A1A883BCFE3879E2DE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"BDA053E8372F4E4192127937522EB305","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate for 10 minutes at room temperature with gentle agitation on a rocking platform.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855817,"guid":"4F9938B017C111EA9DBBC50BC196CC31","previous_id":855816,"previous_guid":"4F95B64017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5D3FD802E1954E6E90C8A5F5CE197D8B","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"BEF48D15CCCB49AEAF7061ADC72A5353","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the fixation solution by spinning down at 500xg at 4\u00b0C for 5 minutes. To quench the reaction, for cell count of 1-5 million, add 3mL of 0.65M glycine solution; for cell count of 5-10 million, add 4mL of 0.65M glycine solution; for cell count of 10-15 million, add 5mL of 0.65M glycine solution.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855818,"guid":"4FA1761017C111EA9DBBC50BC196CC31","previous_id":855817,"previous_guid":"4F9938B017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5E9A99117B754D8EA94FD4E1EA8D693E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"D04AE4380D9E49EEAA844A2C4C06E9C4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate at room temperature for 5 minutes with gentle agitation on a rocking platform.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855819,"guid":"4FA398F017C111EA9DBBC50BC196CC31","previous_id":855818,"previous_guid":"4FA1761017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"677B98D8C55743D69411A48D6803F3A6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"8026908E03F5405AA4EDC4E69DF6D7AD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the glycine solution by spinning down at 500xg at 4\u00b0C for 5 minutes. Discard the supernatant. The pellet can be frozen at -80\u00b0C after adding 1\u00b5L of Protease Inhibitor Cocktail (PIC).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855820,"guid":"4FA82CD017C111EA9DBBC50BC196CC31","previous_id":855819,"previous_guid":"4FA398F017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7EF6D9CC8CB14F01951647DE1A5A37EF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"C6DE381B539645718E4BE00EB2D9A2CF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">To lyse the cells, for cell count of 1-5 million, add 0.4mL of Hypotonic Buffer; for cell count of 5-10 million, add 0.8mL of Hypotonic Buffer; for cell count of 10-15 million, add 1mL of Hypotonic Buffer to the cell pellet. Resuspend the cells and incubate at 4\u00b0C for 10 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855821,"guid":"4FAAEBF017C111EA9DBBC50BC196CC31","previous_id":855820,"previous_guid":"4FA82CD017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8869C46B24C440A6B19C41BEC240A7DF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"A006B722A41148D99C82284F4A3EB8CA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge the hypotonic slurries at 5,000xg at 4\u00b0C for 5 minutes to collect the nuclei.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855822,"guid":"4FAE6E6017C111EA9DBBC50BC196CC31","previous_id":855821,"previous_guid":"4FAAEBF017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6BA10E532E60414DB17F509231EF4816","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"CCA868CAF9264C04B3931A4386D399AF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For cell count of 1-5 million, add 0.3mL of Digestion Buffer; for cell count of 5-10 million, add 0.4mL of Digestion Buffer; for cell count of 10-15 million, add 0.5mL of Digestion Buffer to the nuclei, immediately followed by adding 2\u00b5L of PIC to each sample.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855823,"guid":"4FB217E017C111EA9DBBC50BC196CC31","previous_id":855822,"previous_guid":"4FAE6E6017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B7A1940C5C0043AABC2E3E8B77D8DF08","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"A827080E3E0F4E2692AB84A60B26093E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add Micrococcal Nuclease to each sample to digest the DNA. Mix by inverting the tube several times and incubate at 37\u00b0C for 20 minutes. Mix by inversion every 3-5 minutes. The amount and incubation time of Micrococcal Nuclease required to digest the genomic DNA to an optimal 150 900 bp length may need to be determined empirically for individual cell types.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855824,"guid":"4FB5C16017C111EA9DBBC50BC196CC31","previous_id":855823,"previous_guid":"4FB217E017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9E75E313C36F42CA93BC26CE00DF9F3D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"54F481FCD02D4EF29E3E32CDA83375DE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Stop digestion by adding 10\u00b5L of 0.5M EDTA per sample and place the sample on ice.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855825,"guid":"4FBA554017C111EA9DBBC50BC196CC31","previous_id":855824,"previous_guid":"4FB5C16017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7248FFD5783B4259ADF06FC3A0B1F89E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"3C9049E2EED24CE5A84FEB948BA63E3A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pellet nuclei by centrifugation at 12,000xg at 4\u00b0C for 1 minute. Discard the supernatant.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855826,"guid":"4FBE9B0017C111EA9DBBC50BC196CC31","previous_id":855825,"previous_guid":"4FBA554017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4F8789FC84F1465287F9EAEDBC5B2BB0","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"CD239610611F41F7A1A7B3DC713A411A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend nuclear pellet in Lysis Buffer and incubate the sample on ice for 10 minutes to lyse the nuclei. Alternatively, sonicate to shear the DNA. The time and strength for sonication may need to be determined empirically.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855827,"guid":"4FC355F017C111EA9DBBC50BC196CC31","previous_id":855826,"previous_guid":"4FBE9B0017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4755DBB35B9C49AF899FFA5DD63DCB8A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"4C60D8E4F3AF4F738B456C8540E6A044","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge the sample at 16,000xg at 4\u00b0C for 10 minutes. Transfer the supernatant to a clean dry microcentrifuge tube.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855828,"guid":"4FC837F017C111EA9DBBC50BC196CC31","previous_id":855827,"previous_guid":"4FC355F017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E6304B5BC2BE448ABA4B47C5EE066055","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"2472BF230E574D1087BDC6518B953778","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1\u00b5L of PIC to each sample and mix.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855829,"guid":"4FCDDD4017C111EA9DBBC50BC196CC31","previous_id":855828,"previous_guid":"4FC837F017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1C08A99BA1CC4C6A9417A87A1376EB79","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Sample Preparation"}},{"id":1054724,"guid":"EC31827354D34FD18F8FBF4AF210D19D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Chromatin sample is now ready for ChIP Assay. If the sample is not to be used immediately, store at -80\u00b0C. It is recommended that the shearing efficiency is analyzed at this stage to ensure that 150-900bp fragments are obtained during shearing the DNA to increase the ChIP efficiency.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855831,"guid":"667B121017C111EA9DBBC50BC196CC31","previous_id":855829,"previous_guid":"4FCDDD4017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EEDE7414A00847B5AC112B971F4465F1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Addendum: Shearing Efficiency Analysis"}},{"id":1054724,"guid":"8DF722F040DD4DBC8D13FA9C18BDC926","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">i. Take 50\u00b5L aliquot of sheared chromatin from each sample into a new microcentrifuge. Add 5\u00b5L of 1M NaHCO3, 5\u00b5L of 5M NaCl and 50\u00b5L of Distilled Water. Mix thoroughly and incubate at 65\u00b0C (heat block) for two hours or incubation can be extended for overnight.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855832,"guid":"66823E0017C111EA9DBBC50BC196CC31","previous_id":855831,"previous_guid":"667B121017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"83DFC97CA08149068E58DE4FFCC93B29","order_id":1,"type_id":6,"title":"Section","source":{"title":"Addendum: Shearing Efficiency Analysis"}},{"id":1054724,"guid":"C97846EE45384B3DB62BD009E798EE64","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">ii. Remove the sample from heat block, add 1\u00b5L of Proteinase K to each sample, vortex briefly and perform a spin down at 500xg at 4\u00b0C for 5 minutes. Incubate at 37\u00b0C for one hour.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855833,"guid":"6688CDB017C111EA9DBBC50BC196CC31","previous_id":855832,"previous_guid":"66823E0017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ECEB41282FF341F7A90931F8EE239D09","order_id":1,"type_id":6,"title":"Section","source":{"title":"Addendum: Shearing Efficiency Analysis"}},{"id":1054724,"guid":"C152AAF87A194FD0BFF33BB6B17D3C3F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">iii. Add 2\u00b5L of Proteinase K Stop Solution to each sample, vortex briefly and perform a short spin down.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855834,"guid":"668FD29017C111EA9DBBC50BC196CC31","previous_id":855833,"previous_guid":"6688CDB017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AB360FB202AF437997EB6367F8338D6D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Addendum: Shearing Efficiency Analysis"}},{"id":1054724,"guid":"2E070506C13E4017AB7D9EF029D31AB4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">iv. Purify the DNA by QIAquick\u2122 PCR purification kit, according to manufacturer\u2019s manual.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855835,"guid":"6696FE8017C111EA9DBBC50BC196CC31","previous_id":855834,"previous_guid":"668FD29017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4EF0542E2AAF4DBAA499BEED8A343D84","order_id":1,"type_id":6,"title":"Section","source":{"title":"Addendum: Shearing Efficiency Analysis"}},{"id":1054724,"guid":"A9301CFBFFD54D6B85B5CAE63F4B2210","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">v. Quantify the DNA in the sample using a Nanodrop. This will be used to determine the volume of chromatin to load in ChIP assay.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855836,"guid":"669E9FA017C111EA9DBBC50BC196CC31","previous_id":855835,"previous_guid":"6696FE8017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E912E23773724464A7EB00C9D6EE1C7A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Addendum: Shearing Efficiency Analysis"}},{"id":1054724,"guid":"0425BFF2DC5A40DABFC10A8486E8878B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">vi. To ensure 150-900bp fragments have been obtained during shearing the DNA, run each DNA sample on a 1.3-1.5% agarose gel and visualize against a marker of known size DNA fragments (e.g., 100bp ladder).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855838,"guid":"7CE3EAE017C111EA9DBBC50BC196CC31","previous_id":855836,"previous_guid":"669E9FA017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AD02D39E5A604673BAFE00ED2A68CD7D","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Immunoprecipitation Assay"}},{"id":1054724,"guid":"92CA499ACC5349AD8F5E0138A9FE5225","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mix the Chromatin Sample, Protease Inhibitor Cocktail (PIC), optimal quantity of BioLegend\u2019s Go-ChIP-GradeTM\u00a0Purified antibodies and add Column Conditioning Buffer to make final 1mL slurries. Gently rotate at 4\u00b0C for one hour or incubation can also be extended to overnight.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855839,"guid":"7CEBDA2017C111EA9DBBC50BC196CC31","previous_id":855838,"previous_guid":"7CE3EAE017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7755593AE832429ABEB8FACF746FAF23","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Immunoprecipitation Assay"}},{"id":1054724,"guid":"A3C9D35AFE8B4F7C8ADA02BB64C1A9B2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare the high-throughput (HT) Protein A or G 96 well plate or Spin Column by adding 600\u00b5L of Column Conditioning Buffer in each well or column and allow it to flow through via gravity (~15 minutes).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855840,"guid":"7CF465A017C111EA9DBBC50BC196CC31","previous_id":855839,"previous_guid":"7CEBDA2017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8A0076C605024A2E8F20ADE450683AF9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Immunoprecipitation Assay"}},{"id":1054724,"guid":"0B84857C6EBC41A99C1C972232431E93","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Discard the flow-through and repeat steps 18 and 19.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855841,"guid":"7CFCF12017C111EA9DBBC50BC196CC31","previous_id":855840,"previous_guid":"7CF465A017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A85A8F80E92140A2BE56114DEC176F3F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Immunoprecipitation Assay"}},{"id":1054724,"guid":"49EB542B96034FCF8F0354126E5F2A82","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the slurries from the rotator following 4\u00b0C incubation and briefly spin down to remove residual liquid from the caps.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855842,"guid":"7D068E1017C111EA9DBBC50BC196CC31","previous_id":855841,"previous_guid":"7CFCF12017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A807362844094ECBB3F697D7802F017E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Immunoprecipitation Assay"}},{"id":1054724,"guid":"3FF653072C024589A50E8281BFD9D45F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Load the entire 1mL slurries and allow to flow completely through the high-throughput (HT) Protein A or G 96 well plate or Spin Column at room temperature (approximate 15-20 minutes).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":855843,"guid":"7D102B0017C111EA9DBBC50BC196CC31","previous_id":855842,"previous_guid":"7D068E1017C111EA9DBBC50BC196CC31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F20ABEFB88474CCC8EDA03DC3F0FE470","order_id":1,"type_id":6,"title":"Section","source":{"title":"Chromatin Immunoprecipitation Assay"}},{"id":1054724,"guid":"393838A65F19411796BA9808CDF72971","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"