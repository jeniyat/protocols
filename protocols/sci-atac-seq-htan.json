d":1054725,"guid":"8B9039AE74FF450F9CFAD1533E868EF1","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/be6w4e96.png\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":"#ccece6","duration":0},{"id":754632,"guid":"B1750C1946D24722938930879B29D273","previous_id":754631,"previous_guid":"820A524EBEB248F1ACA8A404D7274105","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9DAE51084C32465BBF867152166EA723","order_id":1,"type_id":6,"title":"Section","source":{"title":"Nuclear Isolation"}},{"id":1054724,"guid":"D633DAC7D42B419C93DC1E2D280EF0E4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Generalized nuclear isolation steps:<\/div><div class = \"text-block\">1. Pellet <\/div><div class = \"text-block\"> liquid cultured cells in<\/div><div class = \"text-block\">centrifuge at 500 rcf  for <\/div><div class = \"text-block\">2. Aspirate pellet and resuspend in <\/div><div class = \"text-block\">3. Incubate on ice for<\/div><div class = \"text-block\">3. Pellet liquid cultured cells in<\/div><div class = \"text-block\">centrifuge at 500 rcf  for <\/div><div class = \"text-block\">4. Aspirate pellet and resuspend <\/div><div class = \"text-block\">5. Run total volume through 35 um filter <\/div><div class = \"text-block\">6. Add DAPI to final concentration of 15 ug\/mL (ie <\/div><div class = \"text-block\">for every 1 mL of sample).<\/div><\/div>"}},{"id":1054725,"guid":"844CE5D4FB9248F68D988390CBBFCD5D","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Note<\/span><\/div><div class = \"text-block\">Isolation of nuclei is dependent on the sample being used. Tissue should follow a dounce homogenization protocol, while liquid cell cutures can be pelleted and resuspended directly in NIB. <\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"comments":[]}},{"id":1054726,"guid":"8BF51E8051AD11E99664FBDCB7D2F9C3","order_id":3,"type_id":3,"title":"amount","source":{"amount":10,"unit":"ml","title":""}},{"id":1054727,"guid":"AA4A6DA051AC11E99664FBDCB7D2F9C3","order_id":4,"type_id":24,"title":"temperature","source":{"concentration":4,"unit":"\u00b0C","title":""}},{"id":1054728,"guid":"981C89F051AD11E99664FBDCB7D2F9C3","order_id":5,"type_id":4,"title":"duration","source":{"duration":600,"title":"min"}},{"id":1054729,"guid":"B8FAFF8051AD11E99664FBDCB7D2F9C3","order_id":6,"type_id":3,"title":"amount","source":{"amount":2,"unit":"ml","title":"NIB"}},{"id":1054730,"guid":"D3B3D18051AD11E99664FBDCB7D2F9C3","order_id":7,"type_id":4,"title":"duration","source":{"duration":600,"title":"min"}},{"id":1054731,"guid":"AA4A6DA051AC11E99664FBDCB7D2F9C3","order_id":8,"type_id":24,"title":"temperature","source":{"concentration":4,"unit":"\u00b0C","title":""}},{"id":1054732,"guid":"CBFF027051AD11E99664FBDCB7D2F9C3","order_id":9,"type_id":4,"title":"duration","source":{"duration":300,"title":"min"}},{"id":1054733,"guid":"EEA5DBF051AD11E99664FBDCB7D2F9C3","order_id":10,"type_id":3,"title":"amount","source":{"amount":500,"unit":"\u00b5l","title":"NIB"}},{"id":1054734,"guid":"F8F361D051AE11E99664FBDCB7D2F9C3","order_id":11,"type_id":3,"title":"amount","source":{"amount":3,"unit":"\u00b5l","title":"5 mg\/mL DAPI"}}],"cases":null,"data":null,"section":null,"section_color":"#99d8c9","duration":0},{"id":754633,"guid":"D496E98F702E4456A58403927D5F8754","previous_id":754632,"previous_guid":"B1750C1946D24722938930879B29D273","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6C8E9F393236482E851526E573F625C6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Perform 1st sort"}},{"id":1054724,"guid":"F2A08B467D984B3B9F016F93ADCDE2A5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">1. Per well use <\/div><div class = \"text-block\">and <\/div><div class = \"text-block\">from Illumina<\/div><div class = \"text-block\">2. Sort 2,000-5,0000 nuclei into each well using fuoresence activated cell sorting (FACS)<\/div><\/div>"}},{"id":1054725,"guid":"B109A4C054AF11E99ECF698FBEE9B4E0","order_id":2,"type_id":3,"title":"amount","source":{"amount":5,"unit":"\u00b5l","title":"NIB"}},{"id":1054726,"guid":"B73B082054AF11E99ECF698FBEE9B4E0","order_id":3,"type_id":3,"title":"amount","source":{"amount":5,"unit":"\u00b5l","title":"TD buffer (2X)"}},{"id":1054727,"guid":"9000C55C4C414198A6CEE2F2FD2CF2D8","order_id":4,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Nuclei count may vary by prep, but ensure that numbers <\/span><span style = \"font-style:italic;\">within<\/span><span> a prep are constant, ie all wells receive the same number of cells. Keep in ice or in 96-well plate chiller the whole time.<\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"comments":[]}}],"cases":null,"data":null,"section":null,"section_color":"#66c2a4","duration":0},{"id":754634,"guid":"0B21643531714F2B933329167D550B41","previous_id":754633,"previous_guid":"D496E98F702E4456A58403927D5F8754","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"47463C06D6F54534BCAC5EEF68F2C58E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Tagmentation following 1st Sort"}},{"id":1054724,"guid":"B5DE8FC13DC0440289148F3DF33695C2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin down 8-well strips in table top centrifuge immediately after sorting finishes.<\/div><div class = \"text-block\">\tStore covered and on ice until the full plate is complete.<\/div><div class = \"text-block\">\tNote: to cover strips as they finish sorting, I tend to tear up aluminum plate covers into single-strip width.<\/div><div class = \"text-block\">\tAddition of Pitstop2 in 2X TD Buffer<\/div><div class = \"text-block\">\tMake sure to test the volume in a well following sort. Sorting a high count of nuclei (2-5K\/well increases to volume by 5-10uL, respectively). If unadjusted, this will lower your TD buffer concentration and decrease Tn5 activity.<\/div><div class = \"text-block\">\tIn 2X TD Buffer, add Pitstop2 reagent before addition into wells.<\/div><div class = \"text-block\">\tExample: Pitstop2 addition for a 5K nuclei sort (adding 10uL to each well which contains 10uL [NIB+2X TD Buffer] and 10uL of sorted nuclei; 30uL Final Volume).<\/div><div class = \"text-block\">\tDetermining concentration of Pitstop2 for TD buffer addition:<\/div><div class = \"text-block\">a.\tC1V1=C2V2<\/div><div class = \"text-block\">b.\t(30uL Total Volume per well)(70uM Final Pitstop Conc.)=(10uL 2X TD Buffer)(X Pitstop2 Concentration)<\/div><div class = \"text-block\">\u2663\tX=210uM<\/div><div class = \"text-block\">a.\tDetermining volume of Pitstop2 to add to 2X TD Buffer mastermix:<\/div><div class = \"text-block\">\u2663\tC1V1=C2V2<\/div><div class = \"text-block\">\u2663\t(210uM working concentration)(100uL TD mastermix) = (3000uM Pitstop2 stock)(X Pitstop2 volume)<\/div><div class = \"text-block\">i.\tX= 7uL Pitstop2 stock for each 93uL 2X TD buffer<\/div><div class = \"text-block\">\u2022\tAdd 1uL 8uM uniquely indexed transposome to each well<\/div><div class = \"text-block\">\u2022\tSeal plate and incubate at 55C for 15 minutes with gentle shaking (on eppendorf thermomixer, ~300 rpm).<\/div><div class = \"text-block\">\u2022\tPlate plate on ice immediately to stop reaction. Keep samples on ice to prevent over-transposistion and nuclei lysis.<\/div><div class = \"text-block\">\u2022\tPool all wells, while maintaining everything on ice.<\/div><div class = \"text-block\">\u2022\tAdd 2uL\/per mL pooled sample of DAPI (5mg\/mL) and bring to sorter for second sort.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754635,"guid":"7F898F11E8ED448EBD8ECCE01844006E","previous_id":754634,"previous_guid":"0B21643531714F2B933329167D550B41","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"61A28CE13FB64B54B577AE8EA3FB41F4","order_id":1,"type_id":6,"title":"Section","source":{"title":"2nd Sort Plate Setup: Preparing Xie Buffer"}},{"id":1054724,"guid":"391F650B04AF41AFB7D6D1B6E2DA4FAE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Preparing Second Plate Xie Buffer (8.5uL\/well):<\/div><div class = \"text-block\">Volume<\/div><div class = \"text-block\">Reagent:<\/div><div class = \"text-block\">6 mL<\/div><div class = \"text-block\">0.05M Tris-HCl pH 7.8 <\/div><div class = \"text-block\">40 uL<\/div><div class = \"text-block\">0.5 EDTA<\/div><div class = \"text-block\">200 uL<\/div><div class = \"text-block\">1M KCl<\/div><div class = \"text-block\">200 uL<\/div><div class = \"text-block\">10% Triton X-100<\/div><div class = \"text-block\">300 uL<\/div><div class = \"text-block\">Qiagen Protease<\/div><div class = \"text-block\">3.26 mL<\/div><div class = \"text-block\">H2O<\/div><div class = \"text-block\">10 mL<\/div><div class = \"text-block\">Total<\/div><div class = \"text-block\">\u2022\tAdd 2.5 uL of 10 uM i5 Indexed PCR Primer and 2.5 uL of 10 uM i7 Indexed PCR Primer prior to sort.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754636,"guid":"B6570D8FA8C542C6BCD719DE85913BE0","previous_id":754635,"previous_guid":"7F898F11E8ED448EBD8ECCE01844006E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FF83AEAF03364A87A6B308D1D58EE80C","order_id":1,"type_id":6,"title":"Section","source":{"title":"2nd Sort Protocol"}},{"id":1054724,"guid":"8345FC2469DB40DD9F401AB61AE8A319","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">\u2022\tThaw RT-PCR reagents on ice before second sort<\/div><div class = \"text-block\">\u2022\tSort X nuclei per well (X is dependent on number of wells tagmented in first sort, as a linear trend)<\/div><div class = \"text-block\">\u2022\t1 plate = 22 nuclei\/well<\/div><div class = \"text-block\">\u2022\t1.5 plates = 33 nuclei\/well<\/div><div class = \"text-block\">\u2022\t2 plates = 44 nuclei\/well etc...<\/div><div class = \"text-block\">\u2022\tUsing same gates as first sort:<\/div><div class = \"text-block\">\u2022\tSort X nuclei per well with modified sort settings:<\/div><div class = \"text-block\">o\t\"Single cell\" rather than \"Normal\"<\/div><div class = \"text-block\">o\tThis leads to a higher abort count (less efficient sorting) but is far more precise<\/div><div class = \"text-block\">\u2022\tAs each 8-well strip (containing the Xie buffer) is completed, replace with a new prepared strip.<\/div><div class = \"text-block\">\u2022\tSpin down the completed 8-well stip in table-top centrifuge beside the machine.<\/div><div class = \"text-block\">\u2022\tCover the strips and store on ice until all sorting is completed.<\/div><div class = \"text-block\">\u2022\tNote: to cover strips as they finish sorting, I tend to tear up aluminum plate covers into single-strip width.<\/div><div class = \"text-block\">\u2022\tKeep sorted samples on ice to prevent transposases cross-reacting with other nuclei.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754637,"guid":"5197C665416C45A0A2969544FB3C9ED9","previous_id":754636,"previous_guid":"B6570D8FA8C542C6BCD719DE85913BE0","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BA5D57DE24214E2381796B21EA4DC646","order_id":1,"type_id":6,"title":"Section","source":{"title":"Transposase Denaturation and PCR setup"}},{"id":1054724,"guid":"3F778D68DFA54A3FB3BEAAB70B32E3EC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">\u2022\tSpin down 8-well strip tubes<\/div><div class = \"text-block\">\u2022\tCover plate and hold on ice until sorting is complete.<\/div><div class = \"text-block\">\u2022\tDigest remaining Tn5 and then denature the Qiagen serine protease on Eppendorf Thermocyclers: <\/div><div class = \"text-block\">\u2022<\/div><\/div>"}},{"id":1054725,"guid":"7F536BB08BD111E98C086D6BA05CC8F5","order_id":2,"type_id":24,"title":"temperature","source":{"concentration":55,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"8D61BF908BD111E98C086D6BA05CC8F5","order_id":3,"type_id":4,"title":"duration","source":{"duration":1200,"title":""}},{"id":1054727,"guid":"9C8CF5208BD111E98C086D6BA05CC8F5","order_id":4,"type_id":24,"title":"temperature","source":{"concentration":70,"unit":"\u00b0C","title":""}},{"id":1054728,"guid":"A35234108BD111E98C086D6BA05CC8F5","order_id":5,"type_id":4,"title":"duration","source":{"duration":1800,"title":""}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754638,"guid":"C2382316711C4B229A0AF734688F0B5C","previous_id":754637,"previous_guid":"5197C665416C45A0A2969544FB3C9ED9","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BD07680377B847418137152ACA6F7418","order_id":1,"type_id":6,"title":"Section","source":{"title":"Transposase Denaturation and PCR setup"}},{"id":1054724,"guid":"F0292042355F48B6BDB08368