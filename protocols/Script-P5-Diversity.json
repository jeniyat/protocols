xt' evenness; save '.\/negative_clean_subsample_150K_phaccs_with_genome_lengths_higher_richness\/${1}_most_abund.txt' most_abund; save '.\/negative_clean_subsample_150K_phaccs_with_genome_lengths_higher_richness\/${1}_sw_index.txt' sw_index; exit\"\n}\nexport -f run.phaccs.with.avg.genome.length","description":"","os_name":"","os_version":"","can_edit":"0"},"is_project":0}]},{"id":"140499","is_changed":"0","original_id":"0","is_skipped":"0","is_checked":"0","guid":"0F402DD6C13B409BA5768CDC56316A1E","previous_guid":"46185B7034FC46C0AB510C0A21C6E491","previous_id":"140498","last_modified":"1457537698","components":[{"component_id":"146261","previous_id":0,"original_id":"0","guid":"89651DEB1C85476DA2E33AE1745C7D2D","previous_guid":null,"component_type_id":"1","data_id":"0","data":"Make directory for the output.","order_id":"0","name":"Description","data_by_id":"0","type_id":"1","source_data":{"description":"Make directory for the output."},"is_project":0},{"component_id":"146262","previous_id":"146261","original_id":"0","guid":"C59FC1922FE74C83974EF61D4AB4495B","previous_guid":"89651DEB1C85476DA2E33AE1745C7D2D","component_type_id":"6","data_id":"0","data":"Virome Alpha Diversity","order_id":"1","name":"Section","data_by_id":"0","type_id":"6","source_data":{"section":"Virome Alpha Diversity"},"is_project":0},{"component_id":"146263","previous_id":"146262","original_id":"0","guid":"F4AEF7B437E94DFF98A9B296B2B09376","previous_guid":"C59FC1922FE74C83974EF61D4AB4495B","component_type_id":"15","data_id":"432","data":null,"order_id":"2","name":"Command","data_by_id":"1","type_id":"15","source_data":{"name":"mkdir .\/negative_clean_subsample_150K_phaccs_with_genome_lengths_higher_richness\nls .\/negative_clean_subsample_150K_cironspect\/*.csp | sed 's\/.*\\\/\/\/g' | xargs -I {} --max-procs=16 sh -c \"run.phaccs.with.avg.genome.length {}\"","description":"","os_name":"","os_version":"","can_edit":"0"},"is_project":0}]},{"id":"140500","is_changed":"0","original_id":"0","is_skip