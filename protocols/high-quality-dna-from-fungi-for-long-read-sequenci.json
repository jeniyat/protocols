er_id":1,"type_id":6,"title":"Section","source":{"title":"Extraction I"}},{"id":1054724,"guid":"DC29FD6530A947108423907F19367914","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Flick tube slightly for mixing and add 200uL of TE buffer. DO NOT! vortex as it shears DNA.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":742869,"guid":"8CB84116F5A54363A67CD94F5AAEEBDE","previous_id":742868,"previous_guid":"E38D38AE055840DEAA3AE934435658E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1402E012CF514BFF8AB78F954703D2F2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Extraction I"}},{"id":1054724,"guid":"2674B4C4FF2349FF84AFD47C2FF7E92E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">leave at RT over night <\/div><\/div>"}},{"id":1054725,"guid":"6E39382D2CEB4D05908E14E89B6C68E5","order_id":2,"type_id":4,"title":"duration","source":{"duration":57600,"title":""}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":742870,"guid":"86F98814E7CD42D7A9F27DEB9888E198","previous_id":742869,"previous_guid":"8CB84116F5A54363A67CD94F5AAEEBDE","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"192FA8ABCB454FE8841C9B87B7F7CB32","order_id":1,"type_id":6,"title":"Section","source":{"title":"Extraction I"}},{"id":1054724,"guid":"C2B593667DEC4A9BA3B414A5E3056EE3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Next day add another 100uL TE buffer and incubate for 1h at 28 \u00b0C with 1400rpm shaking <\/div><\/div>"}},{"id":1054725,"guid":"E8682114440F4CDD9566AC27D7083551","order_id":2,"type_id":4,"title":"duration","source":{"duration":3600,"title":""}}],"cases":null,"data":null,"section":null,"section_color":null,"dur