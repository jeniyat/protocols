_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"094C9CCD1CA34AA9B3D8E59B47328386","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pour oesophagus onto 100mm glass petri dish and add another 10ml fresh cold PBS.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754897,"guid":"8C279A7BB7594DE5A20CD746F008E10D","previous_id":754896,"previous_guid":"B35C70ECF75C41C8AD169B4E2667882E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0AC2A5B321924463B4074DAE8203E844","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EB32997DA55B46D580441E0F2AC1949C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open the samples longitudinally.<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><\/ul><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754898,"guid":"C9CD22A09EA44648ACD21F6E3B0FFE20","previous_id":754897,"previous_guid":"8C279A7BB7594DE5A20CD746F008E10D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F8066409AEC24288A1370423CF1353A0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"225B6E9AE6D741D189C194E6D7743231","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using two forceps (scissors if necessary) separate the mucosa\/epithelium layer from the stroma. Place them onto separate 100mm petri dishes each with 10ml PBS (this is a relatively crude separation protocol and is not 'pure' but avoids long overnight incubations in dispase).<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><\/ul><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754899,"guid":"C703DBC2488944BFB8D359EE9A7A1241","previous_id":754898,"previous_guid":"C9CD22A09EA44648ACD21F6E3B0FFE20","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"13B6E8A128534E7AB9B5BCFB7F3B71EB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"80D99C3C8FCE4086979448CF7BD41EB3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate PBS.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754900,"guid":"455FD32E0BBA402FB77E04532768C449","previous_id":754899,"previous_guid":"C703DBC2488944BFB8D359EE9A7A1241","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"638EEE04542E4C16BCC402DD1EA9FB49","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0BE96D847F0440FCBDE8B4F4EAE057C5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In a few drops of PBS (~200\u00b5l), finely mince the epithelium using two scalpels simultaneously.<\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><\/ul><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754901,"guid":"11909C4AFECA447096526FB9EDBC8356","previous_id":754900,"previous_guid":"455FD32E0BBA402FB77E04532768C449","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"212980E57DF843ADA2867BA3B6B3C6A3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D0FCD2AC739C40A8A87C229788D47359","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 19ml Trypsin-EDTA 0.25% containing 100\u00b5g\/ml DNase I to the dish and transfer the tissue to a 50ml falcon tube.  (DNase is critical for this protocol).  Note later tests indicated that this volume can be reduced to 5ml total.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754902,"guid":"A422EFA03D744089BAAD68EE16DBAE0C","previous_id":754901,"previous_guid":"11909C4AFECA447096526FB9EDBC8356","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"973D2AFA6EB74789A7358E10267D0BBE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"3AB2B55E199A41BB88170F24B05919C9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash the scalpel and the dish with a further 1ml of Trypsin-EDTA 0.25% and transfer it into the Falcon with the tissue.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754903,"guid":"A8789D632D754D0DBBA42BF8F45B96F2","previous_id":754902,"previous_guid":"A422EFA03D744089BAAD68EE16DBAE0C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B11FDDB3830F4A649E400D6583CB865D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"93C712C326324EEDAA9C8F0EC7F43696","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate the biopsies for 30 min at 37\u00b0C on a rocker.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":754904,"guid":"3F64382DF94F45679C8932C3FA20156E","previous_id":754903,"previous_guid":"A8789D632D754D0DBBA42BF8F45B96F2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5A97966381B94AE49BD0D400E4C4C3A1","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"56E00F6551324A54BE84FF24A8A6804B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge sample 500g, 5 m