d_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6F978557A55D4C6885FBEAFF2804391B","order_id":1,"type_id":6,"title":"Section","source":{"title":"1st layer"}},{"id":1054724,"guid":"CE9B8FEFAD264AF981AE5A729F8631AE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After 5 min pipet tissue + enzyme mix into new sterile petri dish on ice. Mince 2 min using razor blade to further break up residual chunks of tissue.<\/div><\/div>"}},{"id":1054725,"guid":"F5CA77E9A8F74C0797A448BC2548E849","order_id":2,"type_id":4,"title":"duration","source":{"duration":300,"title":"After 5 min. digesting pipet digest mix into petri dish"}},{"id":1054726,"guid":"0294DB4B40DB4DB5875F095EE4FE8C2B","order_id":3,"type_id":4,"title":"duration","source":{"duration":120,"title":"mince in petri dish"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713733,"guid":"4AFD9A1F4F5447E786313028472F825B","previous_id":713732,"previous_guid":"7D8C369023664FD197E8294A0212192F","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31E4C94DA69C458F94F7C8C3CBD742E8","order_id":1,"type_id":6,"title":"Section","source":{"title":"1st layer"}},{"id":1054724,"guid":"B6D6BD1247834BFFB3FD05DF160349D1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipet\u00a0digest mix back into 1.5 mL tube. Rinse petri dish with 0.5 mL coll. A\/elastase\/dispase enzyme mix and pipet into same tube.<\/div><\/div>"}},{"id":1054725,"guid":"5CFA4E1B2CD84433BAD73EB04ECC882A","order_id":2,"type_id":3,"title":"amount","source":{"amount":0,"unit":"ml","title":"rinse petri dish"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713734,"guid":"7B921D6AA11247359E211E18C72DC484","previous_id":713733,"previous_guid":"4AFD9A1F4F5447E786313028472F825B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"700508538F0D4EF1ADA2D8891D127A95","order_id":1,"type_id":6,"title":"Section","source":{"title":"1st layer"}},{"id":1054724,"guid":"6D583176E5D94A8AA3C2A7DCD7137401","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Continue triturating and shaking on ice for 2 additional minutes until you reach 9 minutes total digestion time<\/div><\/div>"}},{"id":1054725,"guid":"1B2AEE4735CD48DD85B33F7CDAF2F192","order_id":2,"type_id":4,"title":"duration","source":{"duration":120,"title":"triturate on ice"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713735,"guid":"33DB8541480A483B9E7FEE7413EDCD31","previous_id":713734,"previous_guid":"7B921D6AA11247359E211E18C72DC484","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"273A9F4534CD4DEF9C43A8B0828F8052","order_id":1,"type_id":6,"title":"Section","source":{"title":"1st layer"}},{"id":1054724,"guid":"DF0811A8706D483AA79F0F3F7ED32C0D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">At 9 min total digest time let tube settle for one min on ice. The chunks of tissue will settle to the bottom of the tube, leaving released cells in the supernatant. Pipet 80% of supernatant onto 30 \u00b5M filter on sterile 15 mL conical.<\/div><\/div>"}},{"id":1054725,"guid":"F9D33B16FFA64E9EB11671A62C4CE31A","order_id":2,"type_id":4,"title":"duration","source":{"duration":60,"title":"settle on ice"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713736,"guid":"3BBBB5B41A894C45813E166AE56B152D","previous_id":713735,"previous_guid":"33DB8541480A483B9E7FEE7413EDCD31","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8EFB3459A9494A298B2B4CD9EC36134B","order_id":1,"type_id":6,"title":"Section","source":{"title":"1st layer"}},{"id":1054724,"guid":"387DB00413F3428F9F9FBB604E41D287","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rinse filter w\/6 mL ice-cold PBS\/BSA 0.04%. Leave filter on 15 mL conical for next steps. <\/div><\/div>"}},{"id":1054725,"guid":"54752C8BEB0B44D59148090B349F0964","order_id":2,"type_id":3,"title":"amount","source":{"amount":6,"unit":"ml","title":"ice-cold PBS\/BSA 0.04%"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713737,"guid":"23A335D4B65B4EB790A000EC2F3F3B41","previous_id":713736,"previous_guid":"3BBBB5B41A894C45813E166AE56B152D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E71B74E60FCB49199341A9FD55867EFD","order_id":1,"type_id":6,"title":"Section","source":{"title":"2nd layer"}},{"id":1054724,"guid":"9EEB3D544CAA4CD89C9C0441A58A6CEF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add additional 1 mL of 10 mg\/mL b. lich enzyme mix to residual clumps of tissue in enzyme in the 1.5 mL tube. <\/div><\/div>"}},{"id":1054725,"guid":"9B2E28FDA1844C2AA14AA9B4D96FC7BF","order_id":2,"type_id":3,"title":"amount","source":{"amount":1,"unit":"ml","title":"B. lich enzyme mix"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713738,"guid":"F84D72E684F34FE4A9BF2E514C3CB4C1","previous_id":713737,"previous_guid":"23A335D4B65B4EB790A000EC2F3F3B41","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC684B752E4A493BAFAD97E0D109CF81","order_id":1,"type_id":6,"title":"Section","source":{"title":"2nd layer"}},{"id":1054724,"guid":"C5E063422B9947E58296D3CEDC300A95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Continue triturating on ice\u00a010x every 1.5 minute for 10 additional minutes (20 min total time). Shake every 30 sec.\u00a0<\/div><\/div>"}},{"id":1054725,"guid":"5E96CC8FA6944C68AFD8EC791CA75372","order_id":2,"type_id":4,"title":"duration","source":{"duration":600,"title":"digest on ice"}},{"id":1054726,"guid":"F07A9FC9A84041A6BFB6AD0021E0B29E","order_id":3,"type_id":4,"title":"duration","source":{"duration":90,"title":"triturate 10x"}},{"id":1054727,"guid":"3FC7170E41C24BE6BE031FCE2D8C3AE8","order_id":4,"type_id":4,"title":"duration","source":{"duration":30,"title":"shake"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713739,"guid":"5234E8125AC648BCBD9FE22D73AE95D1","previous_id":713744,"previous_guid":"5D457F56CAC74A808B180F2310BA9AD1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"773C2E51C6CF453E9638FAF6A88C1AA1","order_id":1,"type_id":6,"title":"Section","source":{"title":"2nd layer"}},{"id":1054724,"guid":"B6CC2EAD4A9F45CAA1F0493A716BDBAC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin 300 g for five minutes at 4 \u00baC. Remove all but 100 \u00b5L of supernatant - being careful not to disturb pellet.<\/div><\/div>"}},{"id":1054725,"guid":"A324FE0A8637460F95F5422E526D8C13","order_id":2,"type_id":24,"title":"temperature","source":{"concentration":4,"unit":"\u00b0C","title":"spin at 300 g"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713740,"guid":"3DC611C8E4DA4717B083BCED3F72CDCD","previous_id":713739,"previous_guid":"5234E8125AC648BCBD9FE22D73AE95D1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F428580C80AE4D38803E3FD854913964","order_id":1,"type_id":6,"title":"Section","source":{"title":"RBC Lysis"}},{"id":1054724,"guid":"BD42FB7CB7E94965A4DB850DFBD0A289","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 900 \u00b5L RBC lysis buffer to pellet. Triturate 20X using 1 mL pipet set to 700 \u00b5L and incubate for two min on ice.<\/div><\/div>"}},{"id":1054725,"guid":"10ABE24008764C5EBE2F2002F6C25F45","order_id":2,"type_id":3,"title":"amount","source":{"amount":900,"unit":"\u00b5l","title":"RBC lysis buffer"}},{"id":1054726,"guid":"3B15D16241C64A9F96F26A53359A7B48","order_id":3,"type_id":4,"title":"duration","source":{"duration":120,"title":"incubate on ice"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713741,"guid":"28F16885B29C432BBFD3EE76626E6CF1","previous_id":713740,"previous_guid":"3DC611C8E4DA4717B083BCED3F72CDCD","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"810109B11CC945DF9D4939B514AD0415","order_id":1,"type_id":6,"title":"Section","source":{"title":"RBC Lysis"}},{"id":1054724,"guid":"3E5DACC7797A409A946A08DB41096061","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 12 mL ice-cold PBS\/BSA 0.04% to 15 mL conical to dilute RBC lysis buffer.<\/div><\/div>"}},{"id":1054725,"guid":"F6D58A2BBDC3486086D442C07249784A","order_id":2,"type_id":3,"title":"amount","source":{"amount":12,"unit":"ml","title":"ice-cold PBS\/BSA 0.04%"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713742,"guid":"DEB1A7B73C614B76922760CF79DADEA7","previous_id":713741,"previous_guid":"28F16885B29C432BBFD3EE76626E6CF1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B6AA37D9321F4C62B6ABBCD8C3168466","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparing for Single-Cell Sequencing"}},{"id":1054724,"guid":"A4597F2C130147718BC5A49C440447CF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin 15 mL conical 200 g (low-g spin) for 5 min at 4 \u00baC to pellet cells and leave small debris and platelets in supernatant.<\/div><\/div>"}},{"id":1054725,"guid":"E31EBF860DE341738FE073E92CEEC2A9","order_id":2,"type_id":4,"title":"duration","source":{"duration":300,"title":"spin at 200 g"}},{"id":1054726,"guid":"292FD4AE1DF54796AA16D5DB6C868FC6","order_id":3,"type_id":24,"title":"temperature","source":{"concentration":4,"unit":"\u00b0C","title":""}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713743,"guid":"89E7100EC733486F8424F5676EA3CE25","previous_id":713742,"previous_guid":"DEB1A7B73C614B76922760CF79DADEA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"659859889C194651A12E4C45B67B03E5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Preparing for Single-Cell Sequencing"}},{"id":1054724,"guid":"E94FFAC22B724AFB860046729027ED10","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove supernatant and re-suspend in 200 \u00b5L ice-cold PBS\/BSA 0.04%.\u00a0<\/div><\/div>"}},{"id":1054725,"guid":"9237F097C6594142AED0FB96FCBB6252","order_id":2,"type_id":3,"title":"amount","source":{"amount":200,"unit":"\u00b5l","title":"ice-cold PBS-BSA 0.04%"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713744,"guid":"5D457F56CAC74A808B180F2310BA9AD1","previous_id":713738,"previous_guid":"F84D72E684F34FE4A9BF2E514C3CB4C1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C7EF384172A84CD09D356F730DE83C4E","order_id":1,"type_id":6,"title":"Section","source":{"title":"2nd layer"}},{"id":1054724,"guid":"9783DC3C7A9C4C22BC6C3C3B073E37D7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Pipet entire volume onto same 30 \u00b5M filter on 15 mL conical - rinse w\/6 mL ice-cold PBS\/BSA 0.04%. <\/div><\/div>"}},{"id":1054725,"guid":"723DD89F00C44F84A964EB675FD2110D","order_id":2,"type_id":3,"title":"amount","source":{"amount":6,"unit":"ml","title":"ice-cold PBS\/BSA 0.04%"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":713745,"guid":"3C14FE89EA944C86BB3EE5A0B039BF05","previous_id":713743,"previous_guid":"89E7100EC733486F8424F5676EA3CE25","modified_on":0,"protocol_id":0,"com