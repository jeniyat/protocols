orks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":747411,"guid":"02B96280816311E9A47F91A02F4A4174","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"300AD0434BE4443ABBB32A53E0A30140","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"99777B20C142428281F6E2DFE8368EF9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Use scissors to cut the sealed tube located between the animal\u2019s ears. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747469,"guid":"E1E2F480816811E9B23ABFA6AA401E75","previous_id":747411,"previous_guid":"02B96280816311E9A47F91A02F4A4174","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"290467C176E94C0EBF26383708F8DA7B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DD5D8EEAED0241D6AFFB9B1D45346CDB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Attach the end of the tube not connected to the metabolic cage swivel device to the tube between the animal\u2019s ears. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747470,"guid":"E8873DF0816811E9B23ABFA6AA401E75","previous_id":747469,"previous_guid":"E1E2F480816811E9B23ABFA6AA401E75","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"95A6F431C0E740C1ADB0A1EF0610D65A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B551B49B776C412C93F10CA25A2063C2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Start the saline syringed filled pump (Kent Scientific Corporation, Torrington, CT, USA) to flush debris within the animal\u2019s tube for about 2 minutes. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747471,"guid":"EF9AC3F0816811E9B23ABFA6AA401E75","previous_id":747470,"previous_guid":"E8873DF0816811E9B23ABFA6AA401E75","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B74F2CB1576844F38BB91CCCD68D2252","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A29CD83242594F36B4B76E33FA029BF7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using the preset pump configurations (3 ml\/hour) fill the bladder via a stopcock with room temperature saline and record bladder pressure by using a pressure transducer connected to an amplifier (BP-100, CB Sciences, USA) for a period of 2-5 hours. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747472,"guid":"F7803FF0816811E9B23ABFA6AA401E75","previous_id":747471,"previous_guid":"EF9AC3F0816811E9B23ABFA6AA401E75","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A1F0676517B8415491414A1A0ED58EBF","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AB0FBDECAB4549CBBA5EA2262F897E5B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Collect urine in a beaker mounted on a force transducer (FT-03 D, Grass Instrument Co, Quincy Massachusetts) placed beneath each cage. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747473,"guid":"FF7461F0816811E9B23ABFA6AA401E75","previous_id":747472,"previous_guid":"F7803FF0816811E9B23ABFA6AA401E75","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31106933502F44C9A71281E283F0F716","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DBF1524392FE4B95AB6C7F2B61A4468F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The pressure and force transducers should be connected to a multi-port controller with software (Polyview, Grass Instrument Co, Quincy Massachusetts) to record data through the computer. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Summary: <\/span><\/div><div class = \"text-block\">Cystometry is a test of bladder function in which pressure and volume of fluid in the bladder is measured during filling, storage, and voiding. A cystometry study is performed to diagnose problems with urination, including incontinence and urinary retention.<\/div><div class = \"text-block\"><span style = \"font-weight:bold;font-style:italic;\">This test should be done on the mice two days after SPT Implantation.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Diabetic Complication:<\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/bib64kcn.jpg\" \/><\/div><div class = \"text-block\"><span style = \"font-weight:bold;\">Reference <\/span><\/div><div class = \"text-block\">1. Cannon TW, Damaser MS: Effects of anesthesia on cystometry and leak point pressure of the female rat. Life Sci 69:1193-1202, 2001<\/div><\/div>","changed_on":1565706006}