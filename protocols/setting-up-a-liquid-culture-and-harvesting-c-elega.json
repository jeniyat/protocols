cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700527,"guid":"968B66A01E3C11E99743FF2E4B48DB8D","previous_id":700526,"previous_guid":"968B18801E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"749DE48A8C8B4CE3967EA31C7B4770C9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FA23355127C5444CACED38A60BC0A594","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the flask on a shaker at 200rpm<\/div><\/div>"}},{"id":1054725,"guid":"64FDBE201E3D11E99743FF2E4B48DB8D","order_id":2,"type_id":4,"title":"duration","source":{"duration":302400,"title":""}},{"id":1054726,"guid":"6334BDF01E3D11E99743FF2E4B48DB8D","order_id":3,"type_id":24,"title":"temperature","source":{"temperature":20,"unit":"\u00b0C","title":""}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700529,"guid":"968C29F01E3C11E99743FF2E4B48DB8D","previous_id":700527,"previous_guid":"968B66A01E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"395499C47CE745679EF0C44C3C1994AF","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"14F39004FDDA4524BF22B70CC4D47B6F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Put the flask on ice to allow the worms to settle.<\/div><\/div>"}},{"id":1054725,"guid":"8A341C201E3D11E99743FF2E4B48DB8D","order_id":2,"type_id":4,"title":"duration","source":{"duration":900,"title":""}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700530,"guid":"968C78101E3C11E99743FF2E4B48DB8D","previous_id":700529,"previous_guid":"968C29F01E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1E5CA5F3BBA44DB98386C168A4891F4F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AA196F3180B6414886ACDEA6C26C0A66","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate most of the liquid from the flask.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700531,"guid":"968C9F201E3C11E99743FF2E4B48DB8D","previous_id":700530,"previous_guid":"968C78101E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6B798F67D6F4412BA72503EFC3527868","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E881B55D95A24C309D6B6711E02FF9EF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the remaining liquid to a 15 ml borosil conical tube using a glass pasteur pipette and spin for at least 2 min at 500 \u00d7 g to pellet the worms. Young larvae may take longer than 2 min to pellet. <\/div><\/div>"}},{"id":1054725,"guid":"C94918C01E3D11E99743FF2E4B48DB8D","order_id":2,"type_id":4,"title":"duration","source":{"duration":120,"title":""}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700532,"guid":"968D14501E3C11E99743FF2E4B48DB8D","previous_id":700531,"previous_guid":"968C9F201E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"50F3A315618B4B1EA931776C1E1ECA67","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D8E7ABAC862F4617A4342E37AE881B9A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aspirate the remaining liquid and wash with 10x volume of M9 twice. Spin for at least 2 min at 500 \u00d7 g to pellet the worms<\/div><\/div>"}}],"cases":[{"guid":"926755BCB9DA11E99CA40242AC110005","step_guid":"968D14501E3C11E99743FF2E4B48DB8D","title":"No Contamination found","short_title":null,"description":"{\"blocks\":[{\"key\":\"ctnkq\",\"text\":\"In case of no contamination the worms will be seperated from OP50 since OP 50 does not settle at 500xg\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","first_step_id":null,"first_step_guid":"FAFA8B001E3E11E99743FF2E4B48DB8D"},{"guid":"9268EA93B9DA11E99CA40242AC110005","step_guid":"968D14501E3C11E99743FF2E4B48DB8D","title":"Contamination found","short_title":null,"description":"{\"blocks\":[{\"key\":\"cqgrf\",\"text\":\"In this case, clumps of bacteria or fungal balls of hyphae are seen in the culture. Depending on the application, the bacteria can be removed and C.elegans used for experiments or the culture has to be discarded in case of sensitive experiments\",\"type\":\"unstyled\",\"depth\":0,\"inlineStyleRanges\":[],\"entityRanges\":[],\"data\":{}}],\"entityMap\":{}}","first_step_id":null,"first_step_guid":"406E2AA01E4111E99743FF2E4B48DB8D"}],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700547,"guid":"FAFA8B001E3E11E99743FF2E4B48DB8D","previous_id":700532,"previous_guid":"968D14501E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E7AFF418A4834394A7D278725D707D7D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F8BE251B947847C99DABD9548B5B13FD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the worms to a 1.5ml eppendorf, incubate on ice for 10 mins<\/div><div class = \"text-block\"> and remove as much supernatant as possible.<\/div><\/div>"}},{"id":1054725,"guid":"2C2388D01E3F11E99743FF2E4B48DB8D","order_id":2,"type_id":4,"title":"duration","source":{"duration":600,"title":""}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":700548,"guid":"3041EAB01E3F11E99743FF2E4B48DB8D","previous_id":700547,"previous_guid":"FAFA8B001E3E11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4C9B643807204562B15672D290ED731B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9C8BC20D38244AA6BED6133A9AA4C1B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add IP Buffer to the Pellet with 2 parts homogenization buffer : 1 part worm pellet and store at -80C for upto 3 months<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":700550,"guid":"A84D6C501E3F11E99743FF2E4B48DB8D","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D627174C6A0F4585BED4566F25DC49AA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"345B84EDFAF943CCA042E3FA4EC33B97","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer 20 L4 hermaphrodites each to 10 90cm plates. They will starve out in approximately 6 days. Start the following steps on the day the plates are almost starved.<\/div><\/div>"}},{"id":1054725,"guid":"2CDAFC801E4011E99743FF2E4B48DB8D","order_id":2,"type_id":24,"title":"temperature","source":{"temperature":20,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"3E548EE01E4011E99743FF2E4B48DB8D","order_id":3,"type_id":4,"title":"duration","source":{"duration":518400,"title":""}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700551,"guid":"406E2AA01E4111E99743FF2E4B48DB8D","previous_id":700532,"previous_guid":"968D14501E3C11E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"52B37608A4B541F3B022917D626A8478","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C06EDB6634C644229F664D19BD6F54E3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After the second wash step, animals were resuspended in 20 ml of ice-cold M9 lacking MgSO4, followed by the addition of 20 ml of ice-cold 60 % sucrose in H2O.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700552,"guid":"409F73D01E4111E99743FF2E4B48DB8D","previous_id":700551,"previous_guid":"406E2AA01E4111E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"940AC923BD8B411D9AE2D8775905026F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"294F94D02CF94891B3B93F07C5136E14","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After vigorous mixing of the sucrose\/worm mixture, 4 ml of ice-cold M9 lacking MgSO4 was gently layered on top, and the worms were centrifuged at 400 g for 2 min at 4 \u00b0C. <\/div><\/div>"}},{"id":1054725,"guid":"D63BC2A4DA5447FB947A1A276919DD4A","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">The sucrose float steps were performed as quickly as possible, as otherwise, the layer of animals failed to form properly.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"comments":[]}},{"id":1054726,"guid":"73F36420B7BC41E29259B6877997B181","order_id":3,"type_id":17,"title":"result","source":{"body":"<div class = \"text-blocks\"><div class = \"text-block\">A layer of animals was now visible on top of the sucrose, while contaminants sedimented at the bottom. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700553,"guid":"409FE9001E4111E99743FF2E4B48DB8D","previous_id":700552,"previous_guid":"409F73D01E4111E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39399B47C775437DAFD52B48FF310F6A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"85FF7701B77A4D6ABBFFBC1BB6C9FF76","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">To maximize recovery, 30 ml of supernatant was aspirated from the sucrose float, and distributed into four 50 ml tubes that were subsequently filled by addition of room-temperature M9 lacking MgSO4. The room temperature M9 allowed the animals to digest OP50 bacteria in their intestine.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":700554,"guid":"40A010101E4111E99743FF2E4B48DB8D","previous_id":700553,"previous_guid":"409FE9001E4111E99743FF2E4B48DB8D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F1E0226845D542178CC6D7035D178C17","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E968B5B3D051468AA12453D18ED253BD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The four tubes were placed on ice to cool down for 30 min, after which the animals were washed twice in M9 buffer. Add IP Buffer to the Pellet with 2 parts homogenization buffer : 1 part worm pellet and store at -80C for upto 3 months<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><\/div>","changed_on":1571220817}