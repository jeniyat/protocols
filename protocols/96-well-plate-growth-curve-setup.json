iations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},{"name":"Magnus Rattray","affiliation":"University of Manchester","affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},{"name":"Elaine Bignell","affiliation":"University of Manchester","affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},{"name":"Mike Bromley","affiliation":"University of Manchester","affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false}],"versions":[],"groups":[],"has_subprotocols":0,"is_subprotocol":1,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":1,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":760714,"guid":"E93DDF80934A11E99C188F6CD56748A2","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E56FC6D06D0642EA961C89208975B331","order_id":1,"type_id":6,"title":"Section","source":{"title":"Plate preparation"}},{"id":1054724,"guid":"70C97445DA5F47168C86E171A64EFAE0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Harvest and dilute spores in PBS+0.01% Tween to 4*10<\/span><span style = \"vertical-align:super;\">5 <\/span><span>spores\/mL.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":1800},{"id":760717,"guid":"6265A690934B11E99C188F6CD56748A2","previous_id":760714,"previous_guid":"E93DDF80934A11E99C188F6CD56748A2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FEF1A9927C96490C85BE9A2A954F4EB6","order_id":1,"type_id":6,"title":"Section","source":{"title":"Plate preparation"}},{"id":1054724,"guid":"66D7EA19B13B4509A7FE82F83E73ED58","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Aliquot 5 uL of this spore stock per well of a CytoOne 96-well plate (Starlab) non-coated, this will be overlayed with 195 uL of liquid media to a total volume of 200 uL.<\/div><\/div>"}},{"id":1054725,"guid":"0B667F86032248CFA878C764F927786C","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">This equals to 2000 spores (absolute) per well. 1000 spores or 500 spores are possible to use, but result in more variance within replicates.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":1200},{"id":760758,"guid":"C3E16E00936D11E9B347F76D97504271","previous_id":760999,"previous_guid":"6B19B2D0936F11E987116590BFB9C228","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BCA2E02AE3F64936B2FACDDB6DB1A9B1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Machine setup"}},{"id":1054724,"guid":"AB06EFC670BA47D2A62DB3FD6A25D097","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In the Manchester Fungal Infection Group we either use a Powerwave X-2 with KC software or a BioTek platereader. Set this up to run for <\/div><div class = \"text-block\">  at <\/div><div class = \"text-block\"><span>  to measure OD<\/span><span style = \"vertical-align:sub;\">600 <\/span><span>every 10 minutes. Take a blank reading before running the plate and preheat the machine. Run your experiment.<\/span><\/div><div class = \"text-block\">This will result in a \"growth curve\".<\/div><div class = \"text-block\">Extract or manipulate the data to look like attached (txt file format).<\/div><\/div>"}},{"id":1054725,"guid":"2F119D80936E11E9B347F76D97504271","order_id":2,"type_id":4,"title":"duration","source":{"duration":172800,"title":""}},{"id":1054726,"guid":"349EF8B0936E11E9B347F76D97504271","order_id":3,"type_id":24,"title":"temperature","source":{"temperature":37,"unit":"\u00b0C","title":""}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":300},{"id":760997,"guid":"2F7D1410936F11E987116590BFB9C228","previous_id":760717,"previous_guid":"6265A690934B11E99C188F6CD56748A2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"973A1741FCDA419E81CC6B73DA725D27","order_id":1,"type_id":6,"title":"Section","source":{"title":"Plate preparation"}},{"id":1054724,"guid":"58E5E7E2237E4BDF92CD30C7916EB500","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Fill the space between the wells with either media or H<\/span><span style = \"vertical-align:sub;\">2<\/span><span>O. This will avoid an \"edge effect\", where the wells around the edge of the plate show increased growth compared to other wells.<\/span><\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":300},{"id":760999,"guid":"6B19B2D0936F11E987116590BFB9C228","previous_id":760997,"previous_guid":"2F7D1410936F11E987116590BFB9C228","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A9A2CA830F28403AB3C5ADD90FE72563","order_id":1,"type_id":6,"title":"Section","source":{"title":"Plate preparation"}},{"id":1054724,"guid":"7D632BFD4EE54BD091A0B111DFFA51B3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cover the 96 well plate with a breathable cover to allow gas exchange.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":120},{"id":761020,"guid":"BCD59E40936F11E987116590BFB9C228","previous_id":760758,"previous_guid":"C3E16E00936D11E9B347F76D97504271","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FA052AE28A144625A3A0AC724A1828D3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Analysis"}},{"id":1054724,"guid":"A135567BC89F4016BF45EE8C517F46C1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">See our next protocol for analysis. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Generally, growth assays for filamentous fungi have been performed on solid media, either as dilution series or spot tests. However, the solid media environment does not accurately mimic the environment encountered during infection (ie the mammalian lung). Previously, we have developed a methodology to perform liquid growth assays in time for <\/span><span style = \"font-style:italic;\">A. fumigatus <\/span><span>and other filamentous fungi including analysis via mathematical modelling.<\/span><\/div><div class = \"text-block\"><span>This protocol is designed for <\/span><span style = \"font-style:italic;\">Aspergillus <\/span><span>species (and other filamentous fungi) to generate growth curves in liquid media in a 96-well plate. This can be done in high-throughput to generate 96 growth curves per run.<\/span><\/div><\/div>","changed_on":1566313358}