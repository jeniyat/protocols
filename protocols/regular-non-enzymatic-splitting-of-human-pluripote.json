liations":[],"username":null,"link":"https:\/\/www.thermofisher.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"id":95},"can_edit":0,"stats":{"total_protocols":0}},{"id":204075,"mol_weight":0,"name":"DMEM\/F12 w\/o L-Glutamine HEPES 500 ml","linfor":null,"url":"https:\/\/www.sigmaaldrich.com\/catalog\/product\/sigma\/d6421?lang=en&region=GB&gclid=EAIaIQobChMIu5f7lvmC4wIVSfhRCh0fdgTtEAAYASAAEgLN_vD_BwE","sku":"D6421-500ML","cas_number":"","rrid":"","public":0,"vendor":{"name":"Sigma Aldrich","affiliation":null,"affiliations":[],"username":null,"link":"http:\/\/sigmaaldrich.com","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"id":17},"can_edit":0,"stats":{"total_protocols":0}},{"id":204076,"mol_weight":0,"name":"RevitaCell Suppplement 100X","linfor":null,"url":"https:\/\/www.thermofisher.com\/order\/catalog\/product\/A2644501?SID=srch-srp-A2644501","sku":"A2644501","cas_number":"","rrid":"","public":0,"vendor":{"name":"Thermo Fisher Scientific","affiliation":null,"affiliations":[],"username":null,"link":"https:\/\/www.thermofisher.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"id":95},"can_edit":0,"stats":{"total_protocols":0}},{"id":204077,"mol_weight":0,"name":"Nunc\u2122 Delta Cell-Culture Treated Multidishes; 6-well","linfor":null,"url":"https:\/\/www.thermofisher.com\/order\/catalog\/product\/140685?SID=srch-hj-140685","sku":"140685","cas_number":"","rrid":"","public":0,"vendor":{"name":"Thermo Fisher Scientific","affiliation":null,"affiliations":[],"username":null,"link":"https:\/\/www.thermofisher.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"id":95},"can_edit":0,"stats":{"total_protocols":0}}],"description":"<div class = \"text-blocks\"><div class = \"text-block\">This protocol describes the non-enzymatic splitting of human pluripotent stem cells with ReLeSR (Stem Cell Technologies) and RevitaCell (Thermo Fisher Scientific). This method allows pluripotent stem cells to preserve their undifferentiated state and enhances their survival. Success will hinge upon: <\/div><div class = \"text-block\"><ul style = \"list-style-type:disc;\"><li style = \"counter-reset:ol0;list-style-type:disc;\">Ensuring the right size of colony clumps following ReLeSR treatment, which requires optimal pipetting (speed and number of mixing steps)<\/li><li style = \"counter-reset:ol0;list-style-type:disc;\">Sticking to a consistent maintenance schedule (splitting the cells at regular intervals, preferrably at roughly the same time of day)<\/li><\/ul><\/div><\/div>","changed_on":1561552916}