bprotocol":0,"is_bookmarked":0,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":733841,"guid":"63EF4A0EACBC44EF81B4904CA5F20155","previous_id":733853,"previous_guid":"FBA1A6F7A886404F98AAED20B34BA0A0","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CEC04761F72641328BF263CED5E9C831","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"097B0A90E79B40A39976E2E498645ED5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Collect and concentrate cells down to ~40 ml by pouring the culture through a 20 um Nitex membrane.<\/div><div class = \"text-block\">Photo of 20um Nitex membrane and a frame constructed by cutting out the top and bottom of a Tupperware container.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/hmfdny6.jpg\" \/><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733842,"guid":"78973F80C49F40B88F613718D1BBD398","previous_id":733841,"previous_guid":"63EF4A0EACBC44EF81B4904CA5F20155","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7DDD678C83404E7E8D3C6515FB8DDC73","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8034F562AEA34E75957CDD9CAB1615F3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Transfer the concentrated <\/span><span style = \"font-style:italic;\">Euplotes<\/span><span> cells to a 50 ml screw cap centrifuge tube and pellet cells by centrifugation at 1,000 rpm<\/span><\/div><div class = \"text-block\">(~200 x g) for 1.5 min in a clinical centrifuge.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733843,"guid":"4572E2924C61407F999405D28200AADB","previous_id":733842,"previous_guid":"78973F80C49F40B88F613718D1BBD398","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"92576F888D27424080254917E14AFDE5","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C4730AA2B55C471B80C8448199E4FA4C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove all but the last 10 ml of the supernatant, resuspend cell pellet by gently flicking tube, and transfer resuspended cells to a 15 ml centrifuge tube.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733844,"guid":"753A392080864322B8DF5E0EF7B053B1","previous_id":733843,"previous_guid":"4572E2924C61407F999405D28200AADB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D0FBA88926E440A29B09D04470DD6778","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"50CF364279854340A43DE35C752C1DED","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge cells at 200 x g for 1.5 min, remove all but the last ~100 ul of supernatant, and resuspend pelleted cells by gently agitating tube.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733845,"guid":"58DE87C53FD14F1290F380ED5781F8C5","previous_id":733844,"previous_guid":"753A392080864322B8DF5E0EF7B053B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D193C95869534759A2587DC5DD021EE9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D15B384DE1B54B93BD31FB892595D5BB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1 ml of freshly prepared 0.3 M glucose to the tube of cells and gently mix.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733846,"guid":"32D034D7E7B0406E949CA9734CD99A03","previous_id":733845,"previous_guid":"58DE87C53FD14F1290F380ED5781F8C5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"39BDFAB432214D07BE8C2CF657B504B2","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AEDCB23407254314932363D902A9A169","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Allow cells to equilibrate for 15 min at room temperature.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733847,"guid":"FFBF3DF2BBC6464AB489C5211C236B14","previous_id":733846,"previous_guid":"32D034D7E7B0406E949CA9734CD99A03","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E03EAA750FFF428FA3175C829EF513D7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AD6197C9E9F94AD099A3A937BFAC69CB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For electroporation, transfer 300 ul of cells to a 0.2 cm electroporation cuvette.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733848,"guid":"28CDE905697340DAB6FACD7CBC7036E1","previous_id":733847,"previous_guid":"FFBF3DF2BBC6464AB489C5211C236B14","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FB85A657AFED4C88BCB688E031307A28","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BABEAC69085A446890D392AE9A3B0664","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 3 ul of a 10 mg\/ml solution of DAPI (or DNA) to the cells and gently agitate to mix.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733849,"guid":"EEF87A53DAE1435BAAEAF5AC55FEBA1A","previous_id":733848,"previous_guid":"28CDE905697340DAB6FACD7CBC7036E1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E13CA311219D4009924E2EE5682F3337","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7E444F3833984381BCBD1C15E0442376","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place cuvette in BTX ECM 630 electroporation device and electroporate using either 150 volts, 25 ohms, and 50 uF or 250 volts, 25 ohms, and 50 uF. Allow cells to recover for at least one min.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733850,"guid":"4248144C3502419EBD643477A1B950F1","previous_id":733849,"previous_guid":"EEF87A53DAE1435BAAEAF5AC55FEBA1A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AB99B0EB3BAE4A019358C523AF029D43","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"ECF09A86D7FD44ED9F19FEC639F0BDBA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Transfer electroporated cells to a 15 ml screw-cap tube containing either 10 ml of a <\/span><span style = \"font-style:italic;\">Dunaliella<\/span><span> algae culture grown in artificial seawater to a low-moderate density or 9 ml of 0.3 M glucose + 1 ml of a moderate-high density algae culture.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733851,"guid":"078B86C505154DC99E296EDE5274BB53","previous_id":733850,"previous_guid":"4248144C3502419EBD643477A1B950F1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"782BE43FDEC347E59D01EAD22BCE4574","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"91BFE8D13AE2472E8081D6E742C82DDD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Examine a sample of cells using a fluorescent microscope equipped with a DAPI filter. About 50% of cells are killed using these electroporation conditions, but a high percentage of the remaining cells should be motile and have brightly staining C-shaped macronuclei. Control, non-electroporated cells will have very dim macronculear staining, with the exception of any dead cells present in the culture.<\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/pr-journal\/hm5dny6.jpg\" \/><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733852,"guid":"7DD5C77CC2D44C099F1636D3F3F2FBBC","previous_id":733851,"previous_guid":"078B86C505154DC99E296EDE5274BB53","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"40ECA20418D1456F913087FBC5413F32","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CE9A8EC159284336AC054BEE12DE8EB5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>NOTES: The electroporation procedure and conditions described allow for the entry of DAPI into the cell, resulting in nuclear staining. We have not yet been successful in using these conditions for transformation of <\/span><span style = \"font-style:italic;\">Euplotes crassus<\/span><span>, but only a couple of constructs have been tried to date.<\/span><\/div><div class = \"text-block\">\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0\u00a0 In step 11, the 0.3 M glucose + algae solution should be used if subsequent selection of transformants with a drug will be attempted, as we have found that this increases the sensitivity of E. crassus to at least some selective agents (e.g., G418 or paromomycin).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":733853,"guid":"FBA1A6F7A886404F98AAED20B34BA0A0","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FBB8919C1DC74EE39388D0C720E5CAA8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F6E51272B2E6472697321D30D2733740","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>Grow a 600 ml culture of <\/span><span style = \"font-style:italic;\">Euplotes crassus<\/span><span> using <\/span><\/div><div class = \"text-block\"><a style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">Dunaliella salina grown in artificial seawater<\/span><\/a><\/div><div class = \"text-block\"><span> as the food source until all the algae is eaten, and then allow the <\/span><span style = \"font-style:italic;\">Euplotes<\/span><span> culture to starve an additional 24 hours.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>The electroporation procedure and conditions described allow for the entry of DAPI into the cell, resulting in nuclear staining. We have not yet been successful in using these conditions for transformation of <\/span><span style = \"font-style:italic;\">Euplotes crassus<\/span><span>, but only a couple of constructs have been tried to date.<\/span><\/div><\/div>","changed_on":1556119628}