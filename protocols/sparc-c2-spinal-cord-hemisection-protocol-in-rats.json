y of Florida","affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false}],"versions":[],"groups":[],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":738245,"guid":"127E9AB06DC611E9A001C5BCCF119A1E","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AD6C5E457F0B479E82045AF09FB5F0BE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1DCF3538D87D4356827E72C14D3B6CAD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Rats are anesthetized in a closed chamber (3.5% isoflurane in 100% O2) and transferred to a heated surgical station where anesthesia is maintained via nose cone at 2-2.5% in 100% O2 for the duration of the surgical procedure. Prep injection site for injection using aseptic methods approved by Institutional approval body (IACUC)<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738246,"guid":"EE6ACE406DC611E9AF1C836F04E759F5","previous_id":738245,"previous_guid":"127E9AB06DC611E9A001C5BCCF119A1E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A3A1FF76527E4954A7E139F79E16F526","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D5E2F365AEED477CB71A96E0D80E4D4D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A 1 inch dorsal incision is made from the base of the skull to ~ the 6th cervical segment and the dorsal cervical muscles are dissected in layers to expose the C2 lamina.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738247,"guid":"025DEC206DC711E9AF1C836F04E759F5","previous_id":738246,"previous_guid":"EE6ACE406DC611E9AF1C836F04E759F5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F35E29C44C46487A8C8DA8D43D14624D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"52481CBF6B9E4975A3C367C7A91CFF76","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A C2 laminectomy is performed to expose the dorsal C2 spinal cord.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738248,"guid":"0BC3EDA06DC711E9AF1C836F04E759F5","previous_id":738247,"previous_guid":"025DEC206DC711E9AF1C836F04E759F5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"34C977FFB57E4701A5A89A3DCCED2E8B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5A2B8756B63141CE908E7A985799D210","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The C2 and C3 dorsal roots are visualized and a durotomy performed just caudal to the C2 nerve roots.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738249,"guid":"18355A606DC711E9AF1C836F04E759F5","previous_id":738248,"previous_guid":"0BC3EDA06DC711E9AF1C836F04E759F5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E237C4AA77334E1D8449981757B57EE1","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"988CFB2F75CF4C9CBA0E6E8A53EB0428","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A complete lateral hemisection is performed on the left side of the spinal cord, extending from the spinal midline to the lateral edge of the spinal cord using a microscalpel.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738250,"guid":"1EFBD1D06DC711E9AF1C836F04E759F5","previous_id":738249,"previous_guid":"18355A606DC711E9AF1C836F04E759F5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CA8A0E5CF88F47FC8B3763CF0B184589","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B83F7845D7064E57B478BAE9E5809839","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using gentle aspiration, the left C2 hemisegment is removed (~1mm) between the C2 and C3 nerve roots, enabling visualization and consistency of lesion completeness.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738251,"guid":"3A48CD806DC711E9AF1C836F04E759F5","previous_id":738250,"previous_guid":"1EFBD1D06DC711E9AF1C836F04E759F5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DFE00F0C0DFB4A9E9C3905C43FF0C11D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"191A0F18E4A64D918FA92A6519D8D28F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Closure of Surgical Site:<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":738252,"guid":"3DFB9C506DC711E9AF1C836F04E759F5","previous_id":738251,"previous_guid":"3A48CD806DC711E9AF1C836F04E759F5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"gui