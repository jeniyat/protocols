,"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747715,"guid":"3CE9643081AB11E98E1717E6256AE635","previous_id":747714,"previous_guid":"3CDDCB7081AB11E98E1717E6256AE635","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C0EF3661825842468C87C7286502B7E7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"59D162A96B784C5C9D62BFD0057419C0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1 mL of prewarmed (65\u00b0C) extraction buffer to each tube, mix well and incubate for 5 mins at 65\u00b0C (can be longer if needed e.g. 10-15 mins)<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747717,"guid":"3CE9643281AB11E98E1717E6256AE635","previous_id":747715,"previous_guid":"3CE9643081AB11E98E1717E6256AE635","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8BBF1ABA83354D1580FDE299A9A48F5F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D54054C894054FB385C8D833D21CC890","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 200 ul of chloroform:IAA (24:1), mix well and spin @ 14,000 rcf for 10 mins. Remove upper aqueous phase to a new tube. Make sure not to disturb or pipette any material from the interface. Repeat chloroform:IAA step twice, being increasingly conservative when recovering the aqueous phase.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747719,"guid":"3CE98B4181AB11E98E1717E6256AE635","previous_id":747717,"previous_guid":"3CE9643281AB11E98E1717E6256AE635","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"27774A60AEAA4E2EAA5A9699E768E283","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"33B30B2130A54DF5B2A285208B127BB5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add equal volume of 5 M LiCl to aqueous layer, mix well and incubate overnight @ -20\u00b0C.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747720,"guid":"3CE98B4281AB11E98E1717E6256AE635","previous_id":747719,"previous_guid":"3CE98B4181AB11E98E1717E6256AE635","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5010B738903F439F921D74D83D3C88D4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AF79F436B56E459DA324518256C24B2E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Spin tubes @ 14,000 rcf for 20 mins @ 4\u00b0C.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747721,"guid":"3CE9B25081AB11E98E1717E6256AE635","previous_id":747720,"previous_guid":"3CE98B4281AB11E98E1717E6256AE635","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F803A8C1CFC346A1AE246C22327384C0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CBAE4F35AB8E464FBCF4BEFDBEE6CB77","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove supernatant with pipette, add 1 mL 80% ethanol and invert tube ~10X. Centrifuge @ 7,500 rcf for 5 minutes @ RT. Remove ethanol and repeat.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":747722,"guid":"3CE9B25181AB11E98E1717E6256AE635","previous_id":747721,"previous_guid":"3CE9B25081AB11E98E1717