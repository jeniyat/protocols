_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858718,"guid":"5D4A28001BAB11EAA2E1270471DE7C5B","previous_id":858717,"previous_guid":"5D48A1601BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F2428594B8A41FFAFF7D41AF32FF4C9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Fixation"}},{"id":1054724,"guid":"7ED6C59481284C7DB618B5A349F5A835","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge at 350xg for 5 minutes, discard supernatant.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858719,"guid":"5D4C23D01BAB11EAA2E1270471DE7C5B","previous_id":858718,"previous_guid":"5D4A28001BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7B1DD8D297A34F6AA98347E0C4B9EC98","order_id":1,"type_id":6,"title":"Section","source":{"title":"Fixation"}},{"id":1054724,"guid":"1AD52213D7E44EF9BF2095B60F83D551","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">To put the experiment \u201con hold\u201d at this point for future staining and analysis, wash cells 1x with Cell Staining Buffer. Resuspend cells in Cell Staining Buffer and store cells at 4\u00b0C (short term) or in 90% FCS\/10% DMSO forstorage at -80\u00b0C (long term, for fixed cells without surface antigen staining). Alternatively, cells can be kept in Cyto-Last\u2122 Buffer for the storage of cytokine-producing cells for up to two weeks. The frequencies of cytokine-producing cells present in activated human PBMC cultures can vary widely due to donor variability. Therefore, cryopreserved cells from a single donor are useful for longitudinal studies.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858721,"guid":"7820E2401BAB11EAA2E1270471DE7C5B","previous_id":858719,"previous_guid":"5D4C23D01BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C2480D5777C94046AAAF688CE8B50F99","order_id":1,"type_id":6,"title":"Section","source":{"title":"Permeabilization"}},{"id":1054724,"guid":"B6AF7E0A64FC432AA91C363689274741","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dilute 10X Intracellular Staining Perm Wash Buffer to 1X in DI water.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858722,"guid":"7822DE101BAB11EAA2E1270471DE7C5B","previous_id":858721,"previous_guid":"7820E2401BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8FFEE4D3B3EF47119A602D58AA7AEEBE","order_id":1,"type_id":6,"title":"Section","source":{"title":"Permeabilization"}},{"id":1054724,"guid":"E16C0FE4330249A2977611B508FAA32B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend fixed cells in Intracellular Staining Perm Wash Buffer and centrifuge at 350xg for 5-10 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858723,"guid":"782528001BAB11EAA2E1270471DE7C5B","previous_id":858722,"previous_guid":"7822DE101BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"387CB0853F9C49E48EE8A8CE00AD682F","order_id":1,"type_id":6,"title":"Section","source":{"title":"Permeabilization"}},{"id":1054724,"guid":"739B9CDBB1224FADA0FAFC22DD5B72CA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Repeat step 5 twice.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#94EBFF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858725,"guid":"854C02101BAB11EAA2E1270471DE7C5B","previous_id":858723,"previous_guid":"782528001BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F6DF9D65B4224474842DEB8A44777A7A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Intracellular Staining"}},{"id":1054724,"guid":"53164A9677494602895E2C8065D30526","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend fixed\/permeabilized cells in residual Intracellular Staining Perm Wash Buffer and add a predetermined optimum concentration of fluorophore-conjugated antibody of interest (e.g. PE anti-IFN-\u03b3) or an appropriate negative control for 20 minutes in the dark at room temperature.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858726,"guid":"854F0F501BAB11EAA2E1270471DE7C5B","previous_id":858725,"previous_guid":"854C02101BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DF02D23E2BCC4BB687B8B0144569E804","order_id":1,"type_id":6,"title":"Section","source":{"title":"Intracellular Staining"}},{"id":1054724,"guid":"C6BD9AD2BA274FBC913FFB807AF9CEE9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Wash 2x with 2 ml of Intracellular Staining Perm Wash Buffer and centrifuge at 350xg for 5 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858727,"guid":"855291C01BAB11EAA2E1270471DE7C5B","previous_id":858726,"previous_guid":"854F0F501BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2FC59C1B6B474B3DA2059E739A5A68F3","order_id":1,"type_id":6,"title":"Section","source":{"title":"Intracellular Staining"}},{"id":1054724,"guid":"DCA0B2E940DD4FD1877906F22316900B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If primary intracellular antibody is biotinylated, it will be necessary to perform fluorophore conjugated Streptavidin incubations and subsequent washes in Intracellular Staining Perm Wash Buffer.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858728,"guid":"855614301BAB11EAA2E1270471DE7C5B","previous_id":858727,"previous_guid":"855291C01BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"44F4C8B408364D5F8FBACAE82D88F8EF","order_id":1,"type_id":6,"title":"Section","source":{"title":"Intracellular Staining"}},{"id":1054724,"guid":"E7A1BA9D0CD746559896B0CC9FBFCA71","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend fixed and intracellularly labeled cells in 0.5 ml Cell Staining Buffer and analyze with appropriate controls.<\/div><div class = \"text-block\">Tip:\u00a0To confirm specific anti-cytokine staining, a blocking experiment is recommended in which cells are fixed\/permeabilized then preincubated with an excess amount of unlabeled anti-cytokine antibody and\/or the recombinant cytokine of interest is preincubated with fluorophore-conjugated anti-cytokine antibody before its addition to the cells.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858730,"guid":"AAE197101BAB11EAA2E1270471DE7C5B","previous_id":858728,"previous_guid":"855614301BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C54D03F8232A4CD0977CBAE128274CFC","order_id":1,"type_id":6,"title":"Section","source":{"title":"Activation and Intracellular Staining of Whole Blood"}},{"id":1054724,"guid":"DA9427F21EFC4A7B8505F499F25C8935","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dilute heparinized whole blood 1:1 with sterile appropriate tissue culture medium.<\/div><div class = \"text-block\">Tip:\u00a0For details on stimulation methods, please see our\u00a0<\/div><div class = \"text-block\"><a href=\"https:\/\/www.biolegend.com\/en-us\/intracellular-staining-stimulation-guide\" style = \"text-decoration:underline;color:blue;cursor:pointer;\"><span style = \":;\">stimulation guide<\/span><\/a><\/div><div class = \"text-block\">\u00a0for cytokines\/chemokines.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858731,"guid":"AAE652001BAB11EAA2E1270471DE7C5B","previous_id":858730,"previous_guid":"AAE197101BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"03AE4E430C904C148689E50B7BD43D4C","order_id":1,"type_id":6,"title":"Section","source":{"title":"Activation and Intracellular Staining of Whole Blood"}},{"id":1054724,"guid":"B87DABF0B9F64620B21DDB4B55BAC070","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">At this stage, in vitro cellular stimulation by either antigen or mitogen can be performed. If intending to stain intracellular cytokines or chemokines (e.g. IFN-\u03b3 or IL-4), addition of an efficient protein transport inhibitor such as brefeldin A or monensin is critical. After addition of a suitable cellular activator, aliquot 200 \u00b5l of the whole blood cell suspension into 12 x 75mm plastic tubes and incubate for 4-6 hours in 5% CO2\u00a0at 37\u00b0C.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858732,"guid":"AAEA97C01BAB11EAA2E1270471DE7C5B","previous_id":858731,"previous_guid":"AAE652001BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8A0F1AD907464F31B14CD417D96E3FC5","order_id":1,"type_id":6,"title":"Section","source":{"title":"Activation and Intracellular Staining of Whole Blood"}},{"id":1054724,"guid":"AA8898F5D799463FB0661C3594F38EFE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 2ml of 1X Red Blood Cell Lysis Buffer and incubate for 5-10 minutes at room temperature.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858733,"guid":"AAEF52B01BAB11EAA2E1270471DE7C5B","previous_id":858732,"previous_guid":"AAEA97C01BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"09F79B52FE404BFE9786EF85A43FB5E2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Activation and Intracellular Staining of Whole Blood"}},{"id":1054724,"guid":"DF508700A44C4DA48F14D01B0EEB63C9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge at 350xg for 5 minutes and discard the supernatant.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#FFED92","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":858734,"guid":"AAF5BB501BAB11EAA2E1270471DE7C5B","previous_id":858733,"previous_guid":"AAEF52B01BAB11EAA2E1270471DE7C5B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D8CD1D6D66A04E3584AFB0771EF8239A","order_i