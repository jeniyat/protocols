rotocol_id":0,"components":[{"id":1054723,"guid":"ABA883E453034947BF9DFA280C4ADEBA","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"F95A5D7C6F4140ECA0960597D7A4C013","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer 3 \u2013 4 ml of broth from tube 21 (TSB with no E. coli) to the spectrophotometer cuvette, carefully wipe the outside of the tube with a kimwipe, place the tube in the machine, and set the 100%T setting (blank the spectrophotometer). Switch the spectrophotometer setting to \u201cAbsorbance.\u201d<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826530,"guid":"6B7E4B30EFEC11E9909ED5935C86AC1B","previous_id":826529,"previous_guid":"651C3AE0EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"24A176343381416EBD078BF2CE365832","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"F9F329D4F59D48D7AF936FF34C015FE8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the cuvette and pour the broth back into tube 21. Carefully drain the last few drops from the lip of the cuvette into a kimwipe. Put the contaminated kimwipe in the biohazard bag.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826531,"guid":"76469BD0EFEC11E9909ED5935C86AC1B","previous_id":826530,"previous_guid":"6B7E4B30EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6E9B4F34836A40C99063FB7AD6A69433","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"86D409745D0E417B998D617DA09CA77C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Starting with tube #20 and going backwards to tube #1, repeat steps 5-6. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826533,"guid":"7D5B3340EFEC11E9909ED5935C86AC1B","previous_id":826531,"previous_guid":"76469BD0EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"775C753537E14E1EA19D505697206585","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"0FBA8F5E3B514EC0A2A1B7CA3709C414","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the data to a Microsoft Excel spreadsheet. The spreadsheet should contain the following columns: volume of E. coli culture, volume of broth, dilution factor, and measured absorbance.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826534,"guid":"854235E0EFEC11E9909ED5935C86AC1B","previous_id":826533,"previous_guid":"7D5B3340EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"15445256D6C14E86BF66D39A39050C26","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"F352FA3B8C374CA78CAEEF4DD079E944","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">In the spreadsheet, create an X-Y scatter plot of dilution factor vs absorbance. <\/div><div class = \"text-block\">* Please note: All charts must be added as separate sheets in the workbook, not pasted into the worksheet with the data columns. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826536,"guid":"8DDBBFA0EFEC11E9909ED5935C86AC1B","previous_id":826534,"previous_guid":"854235E0EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0AE80CE7C86347599AC467B15BD45F6E","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"70A82ECAF02A4DFCA9DA1D41921C1CDA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add a second order polynomial trendline to the plot, and set the options on the trendline to show the equation and the correlation coefficient.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826540,"guid":"BBF3ACE0EFEC11E9909ED5935C86AC1B","previous_id":826536,"previous_guid":"8DDBBFA0EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A82AF25C76FD4C6DB06583D3CFA02911","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"44938F9B52E74C538AADEFA100EBC19F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">From the plot, identify the range in which absorbance is proportional to bacterial concentration. Create a second plot, plotting only the linear range of dilution factor vs absorbance.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0},{"id":826542,"guid":"C23515D0EFEC11E9909ED5935C86AC1B","previous_id":826540,"previous_guid":"BBF3ACE0EFEC11E9909ED5935C86AC1B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DA85A5A7F96A4D5D98DD8DF7C0EDACE1","order_id":1,"type_id":6,"title":"Section","source":{"title":"Making an absorbance curve for Escherichia coli"}},{"id":1054724,"guid":"64E57DFF5B4747B2917FAAB9585D1B8E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add a linear trendline to the second plot, and set the options for the trendline to show the equation and the correlation coefficient. <\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">Bacterial concentration can be measured by several methods. One of them is turbidity. Turbidity, or light scattering, is measured in a spectrophotometer such as a Spectronic 20. This method has the advantage of being the quickest method at the lab bench.<\/div><\/div>","changed_on":1571235564}