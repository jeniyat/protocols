onents":[{"id":1054723,"guid":"04C0B3FC1F90423BB928AD14F8709451","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E2EAF86D9CD84FAA8E6325F362936DF6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Expected procedure duration:<\/span><\/div><div class = \"text-block\">30 minutes<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721777,"guid":"01FB0730475D11E9A86B75575F311564","previous_id":721776,"previous_guid":"F68622E0475C11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E18EBEE300E74404B0B14F203210741B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"82F99520A2CB47F5A7E7D519B4D5F16A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Adequacy or depth of anesthesia is monitored by:<\/span><\/div><div class = \"text-block\">Respiratory Rate and Toe Pinch<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721778,"guid":"1287EFA0475D11E9A86B75575F311564","previous_id":721777,"previous_guid":"01FB0730475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2C87D99E85704C0CB8755A7A7CA1422B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"10C97626509B4E90A67EC5C5723D653D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Frequency of anesthesia depth assessment:<\/span><\/div><div class = \"text-block\">At the start of surgical procedure, a toe or ear pinch can be used to assess the depth of anesthesia. Visual monitoring should be performed thought-out the procedures, as well as toe\/ear pinches.<\/div><div class = \"text-block\">Deviations from expected behavior Should be noted.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721779,"guid":"232B1F30475D11E9A86B75575F311564","previous_id":721778,"previous_guid":"1287EFA0475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"2125FC727D7147A38F17BC9DC01907C1","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D5564656023E4483B7B9D78CE9BFCB42","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Anesthesia Regimen:<\/span><\/div><div class = \"text-block\">Ketamine (80-100mg\/kg), Xylazine (5-20mg\/kg) or Isoflurane 1-3%\n<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721780,"guid":"3A465810475D11E9A86B75575F311564","previous_id":721779,"previous_guid":"232B1F30475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6BBF4ECF2D624BEAA28277A936CF8AAB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0F88207C4A3F49CE86ADB11867EC1CA3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Pre-surgical Analgesics:<\/span><\/div><div class = \"text-block\">Approximately 30 minutes prior to undergoing the surgical procedure, mice receive an S.C. injection of Buprenorphine (0.05mg\/kg) and Meloxicam (5mg\/kg).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721781,"guid":"4AAB4620475D11E9A86B75575F311564","previous_id":721780,"previous_guid":"3A465810475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BC6B96773A954492B18CAAFD011650C8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"82FB2A63535643C1BC9D5231EB9B3E72","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Surgical prep:<\/span><\/div><div class = \"text-block\">Aseptic technique will be maintained by:\nClipping\/shaving fur around incision site, Sterile Instruments.<\/div><div class = \"text-block\">\nIsoflurane, heating pad, forceps, scissors, needle driver, eye ointment, 6-0 Prolene, 7-0 silk suture, 1 ml-syringes, 0.9% NaCl, Sterile gloves, Povidone-idoine, 70% ETOH<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721782,"guid":"5FCE7F90475D11E9A86B75575F311564","previous_id":721781,"previous_guid":"4AAB4620475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BA1EA455E3EE4704B5BEDFF97133987C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"96A9AB27228C47E283870D1941DE0B8A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Surgical Procedure:<\/span><\/div><div class = \"text-block\">(1). Anesthetize the mice and ensure depth of anesthesia with a toe pinch.<\/div><div class = \"text-block\">(2). Place the mice supine on the heating pad.<\/div><div class = \"text-block\">(3). Remove the hair from 0.5 cm above the elbow to 0.5 cm below the knee joint of surfaces to be\njoined.<\/div><div class = \"text-block\">(4). Prep the surgical field with 70% isopropanol as well as Betadine solution and drape the mice.<\/div><div class = \"text-block\">(5). Apply ointment to animal\u2019s eyes.<\/div><div class = \"text-block\">(6). Confirm depth of anesthesia with a toe pinch.<\/div><div class = \"text-block\">(7). Make an incision in the skin from the medial thigh towards the knee.<\/div><div class = \"text-block\">(8). Blunt dissect away subcutaneous fat tissue to reveal the underlying femoral artery.<\/div><div class = \"text-block\">(9). Pierce the membranous femoral sheath to expose the neurovascular bundle. Dissect and separate the femoral artery from the femoral vein and nerve from the inguinal ligament to the epigastrica.<\/div><div class = \"text-block\">(10). Place 2 7-0 silk sutures around femoral artery 2mm apart and ligate. Divide the femoral artery between the 2 ligating sutures.<\/div><div class = \"text-block\">(11). Close the skin incision using 6-0 Prolene suture.<\/div><div class = \"text-block\">(11). Injection 0.5 ml of 0.9% NaCl subcutaneously to each mouse to prevent dehydration.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721783,"guid":"F8DA2F90475D11E9A86B75575F311564","previous_id":721782,"previous_guid":"5FCE7F90475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6557CEAEEB2745CB902571EFA728F94E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"54638D2222644C9484DB5C1A4241641C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Surgical Procedure (alternate): <\/span><span>Severe ischemic model<\/span><\/div><div class = \"text-block\">(1). Anesthetize the mice and ensure depth of anesthesia with a toe pinch.<\/div><div class = \"text-block\">(2). Place the mice supine on the heating pad.<\/div><div class = \"text-block\">(3). Remove the hair from 0.5 cm above the elbow to 0.5 cm below the knee joint of surfaces to be joined.<\/div><div class = \"text-block\">(4). Prep the surgical field with 70% isopropanol as well as Betadine solution and drape the mice.<\/div><div class = \"text-block\">(5). Apply ointment to animal\u2019s eyes.<\/div><div class = \"text-block\">(6). Confirm depth of anesthesia with a toe pinch.<\/div><div class = \"text-block\">(7). Make an incision in the skin from the medial thigh towards the knee.<\/div><div class = \"text-block\">(8). Blunt dissect away subcutaneous fat tissue to reveal the underlying femoral artery.<\/div><div class = \"text-block\">(9). Pierce the membranous femoral sheath to expose the neurovascular bundle. Dissect and separate the femoral artery, femoral vein and nerve from the inguinal ligament to the sapheno-popliteal bifurcation.<\/div><div class = \"text-block\">(10). Ligate the femoral artery using 7-0 silk sutures between the superficial epigastric artery and the sapheno-popliteal bifurcation.<\/div><div class = \"text-block\">(11). Close the skin incision using 6-0 Prolene suture.<\/div><div class = \"text-block\">(11). Injection 0.5 ml of 0.9% NaCl subcutaneously to each mouse to prevent dehydration.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721784,"guid":"8D1544B0475E11E9A86B75575F311564","previous_id":721783,"previous_guid":"F8DA2F90475D11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"96F1A1AF2F904CF6B486968E15E511A4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9701FE1834A84E9EB64087A0D96336A1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Post-procedure Analgesics:<\/span><\/div><div class = \"text-block\">Buprenorphine (0.05mg\/kg) every 12 hours, for 72 hours post-op. <\/div><div class = \"text-block\">Meloxicam (5mg\/kg) every 24 hours, for 72 hours post-op<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":721785,"guid":"9D87A040475E11E9A86B75575F311564","previous_id":721784,"previous_guid":"8D1544B0475E11E9A86B75575F311564","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BDB94CE13B31441A8CC2B826A5A360FB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C52EEE09C53D40ACA29FF6C881BB3EB6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Post-procedure Monitoring:<\/span><\/div><div class = \"text-block\">Mice are monitored 2x daily for the first 5 days post the surgery. Thereafter, mice are monitored at least 3x per week.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[{"id":182051,"mol_weight":0,"name":"Ketamine","linfor":null,"url":"https:\/\/www.pattersonvet.com\/ProductItem\/078036637","sku":"07-803-6637","cas_number":"","rrid":"","public":0,"vendor":{"name":"Patterson Veterinary","affiliation":null,"username":null,"link":"https:\/\/www.pattersonvet.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":470},"can_edit":0,"stats":{"total_protocols":0}},{"id":182052,"mol_weight":0,"name":"Xylazine","linfor":null,"url":"https:\/\/nationalstocknumber.info\/part-number\/07-869-6707_011854192","sku":"07-869-6707","cas_number":"","rrid":"","public":0,"vendor":{"name":"Patterson Veterinary","affiliation":null,"username":null,"link":"https:\/\/www.pattersonvet.com\/","image":{"source":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"id":470},"can_edit":0,"stats":{"total_protocols":0}},{"id":182053,"mol_weight":0,"name":"Isoflurane 1-3%","linfor":null,"url":"https:\/\/www.pattersonvet.com\/ProductItem\/078931389","sku":"07-893-1389","cas_number":"","rrid":"","public":0,"vendor":{"na