ass = \"text-block\">was added to the dextran tracer (0.01 mg per 100 \u03bcl solution).<\/div><\/div>"}},{"id":1054725,"guid":"E394D5206DDE11E9921149D9B4314996","order_id":2,"type_id":20,"title":"reagents","source":{"id":193065,"mol_weight":0,"name":"Dextran-Biotin 3k, Lysine fixable","linfor":"","url":"https:\/\/www.thermofisher.com\/order\/catalog\/product\/D7135","sku":"D7135","cas_number":"","rrid":"","public":0,"vendor":{"name":"Thermo Fisher Scientific","affiliation":null,"affiliations":[],"username":null,"link":"https:\/\/www.thermofisher.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":95},"can_edit":0,"stats":{"total_protocols":0}}},{"id":1054726,"guid":"302D4AC06DDF11E9921149D9B4314996","order_id":3,"type_id":20,"title":"reagents","source":{"id":193066,"mol_weight":0,"name":"Dextran-Biotin 10k, Lysine fixable","linfor":"","url":"https:\/\/www.thermofisher.com\/order\/catalog\/product\/D1956","sku":"D1956","cas_number":"","rrid":"RRID:AB_2307337","public":0,"vendor":{"name":"Thermo Fisher Scientific","affiliation":null,"affiliations":[],"username":null,"link":"https:\/\/www.thermofisher.com\/us\/en\/home.html","image":{"source":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png","placeholder":"https:\/\/www.protocols.iohttps:\/\/www.protocols.io\/img\/vendors\/placeholder.png"},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false,"id":95},"can_edit":0,"stats":{"total_protocols":0}}},{"id":1054727,"guid":"CD7BFBA06DDF11E9921149D9B4314996","order_id":4,"type_id":20,"title":"reagents","source":{"id":193067,"mol_weight":0,"name":"35-gauge NANOFIL needle","linfor":"","url":"https:\/\/www.wpiinc