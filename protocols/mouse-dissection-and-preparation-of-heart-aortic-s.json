_of_bookmarks":0,"number_of_comments":0,"number_of_exports":0,"number_of_runs":0,"number_of_votes":0,"is_voted":0},"parent_protocols":[],"parent_collections":[]},"created_on":1564596932,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":745817,"guid":"FAB8FF107D7E11E988414D8242BB5B6E","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F64BDFBA4BA541DF99CD335E24392A6E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"33B756C01A564CC388EAF5D88A22A629","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare a 1 ml syringe by filling with 15\u00b5l 0.5 M EDTA ph 8.0 and then capping with a 23g needle. The syringe will be used for drawing blood from the heart.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745818,"guid":"05EDA6107D7F11E988414D8242BB5B6E","previous_id":745817,"previous_guid":"FAB8FF107D7E11E988414D8242BB5B6E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"722046F7A0894B0C9BC5DAC50CCE3C71","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1E11C3FCDDA945C6896DA7472E09D449","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare a 10 ml syringe with 50-100 \u00b5l of heparin (1000 units per ml), then fill the syringe to 10 ml with PBS. Remove all the air from the syringe. Cap with a 30g needle. The syringe will be used to flush the heart.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745819,"guid":"0FB06D907D7F11E988414D8242BB5B6E","previous_id":745818,"previous_guid":"05EDA6107D7F11E988414D8242BB5B6E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F7FF631A32B240A9ABFDA0F046D92126","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"91D80361741745268C71047625DA889F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Anesthetize the mouse.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745821,"guid":"158902907D7F11E988414D8242BB5B6E","previous_id":745819,"previous_guid":"0FB06D907D7F11E988414D8242BB5B6E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"52029F4FAFD34913BD706BA68B9F5C7C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C6775DD4F393440797CF0F0619EA72DA","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Determine weight and length of the mouse.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745823,"guid":"209DC5807D7F11E988414D8242BB5B6E","previous_id":745821,"previous_guid":"158902907D7F11E988414D8242BB5B6E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F69F76173AE64927B6A51EF9B095CD19","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FBA3AA13FCB743BE98396889319EE9CC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Tape the mouse\u2019s arms to 3-4 layers of paper towels and place under a light source.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745833,"guid":"5A67A0107D7F11E981A25B9CEBFE22B1","previous_id":745823,"previous_guid":"209DC5807D7F11E988414D8242BB5B6E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F53B7E601DC740E3A66E6B13F9969B51","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BA881A04034148688DAF029E06B2FA65","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cut the skin of the mouse from the abdomen to the top of the thorax.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745834,"guid":"6458CA907D7F11E981A25B9CEBFE22B1","previous_id":745833,"previous_guid":"5A67A0107D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3567522AF4104E1FBD6F0CBDCDA2CDA8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"38669DF6A83E4C87A0710D8E81CB92D3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Open the abdominal wall below the ribcage.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745835,"guid":"6AE4D1107D7F11E981A25B9CEBFE22B1","previous_id":745834,"previous_guid":"6458CA907D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A780339F8C5046C6A18C11C559F8398D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E8B3A9F1FC8E424F9C9294DF4469EE3A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Lift the sternum with tweezers and cut the diaphragm. Then cut away the lower part of the ribcage to partially expose the heart.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745836,"guid":"76B961907D7F11E981A25B9CEBFE22B1","previous_id":745835,"previous_guid":"6AE4D1107D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"50A398B29E6F44978BC8E770197FF329","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"68610CDB50E741F69C24ADD8D956FC9F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Draw blood from the heart by sticking the needle of the 1 ml syringe (see 1) into the apex of the left ventricle. Draw blood by slowly pulling the plunger, twisting the needle. Do not draw for over a minute to avoid blood clots. Transfer the blood into an Eppendorf tube. Mix the tube by inverting it a few times. Centrifuge the tube of blood and transfer the plasma into a fresh tube.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745838,"guid":"7D9361007D7F11E981A25B9CEBFE22B1","previous_id":745836,"previous_guid":"76B961907D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4C28A0FB5CF34A01A09A1939C8174734","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"30BD87A05ACC4373ACA270A67B1B701E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Make a small incision in the right atrium for drainage.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745839,"guid":"890EFC107D7F11E981A25B9CEBFE22B1","previous_id":745838,"previous_guid":"7D9361007D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"05B757679AEB4073BB38ADEDC953CB68","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BE8482AC3F274783985CC5344DB3C166","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Stick the 10 ml syringe into the apex of the left ventricle and flush the blood from the mouse with 10 ml of PBS.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745840,"guid":"93161F907D7F11E981A25B9CEBFE22B1","previous_id":745839,"previous_guid":"890EFC107D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EDCF2A617C2343DA99EEDE8DB1B5BD13","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5B72E4A82E1744E089966F536D31F9E7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">If the liver is to be saved take several sections and immediately freeze at \u201380\u00baC.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745841,"guid":"A1C5BC807D7F11E981A25B9CEBFE22B1","previous_id":745840,"previous_guid":"93161F907D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9576CAA7B0184C9786C50D5DBA7CB567","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"55061E1F6F014CF480812D4E8AFDE8FD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dry the mouse by turning it over and pressing it against paper towels. Transfer to clean, dry paper towels and re-tape the arms.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745842,"guid":"AC5BD9907D7F11E981A25B9CEBFE22B1","previous_id":745841,"previous_guid":"A1C5BC807D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"EB9BEDFFD99A44719CBD732B951D5F01","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8C79C40704834F7EAD4FD2C7D341F3C6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Place the mouse under a dissecting microscope.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745843,"guid":"B28267807D7F11E981A25B9CEBFE22B1","previous_id":745842,"previous_guid":"AC5BD9907D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F391E84560A54F7C8B48FD7A99ED1EA3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BBE824CC64B04E6FAACF9B5E311F7676","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Remove the remaining ventral part of the ribcage, carefully cut the right clavicle leaving the brachiocephalc artery and its branches intact. Using microdissection scissors, cut out all of the fat around the ascending aorta and brachiocephalic artery.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745844,"guid":"BDF1A6807D7F11E981A25B9CEBFE22B1","previous_id":745843,"previous_guid":"B28267807D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C1B7CD1B3AD54A7B952CC86FBA898CF2","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7BDEF368212F4E41943FF2A609CB67D8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cut all the fat and tissue surrounding the heart, including the pulmonary artery, and veins.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745845,"guid":"C8F2E1707D7F11E981A25B9CEBFE22B1","previous_id":745844,"previous_guid":"BDF1A6807D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5F4E4E5762144301923853532BC4F212","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"0D38DA3D47424E6486DBCDDF1BCC2B23","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Flush the heart again through the right ventricle, left atrium, and left ventricle using a 10 ml syringe containing a total of 3 ml of PBS to clean out residual blood.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745846,"guid":"DF25DD307D7F11E981A25B9CEBFE22B1","previous_id":745845,"previous_guid":"C8F2E1707D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F1B93ABE84624487917515528DBB6993","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E70D4D3DB405437EAE2639A9CF57C12B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cut the brachiocephalic artery distal to where it branches into the right subclavian and right carotid and then cut it at its branching site from the aorta so you end up with a Yshaped piece. Put it in a base mold (Fisher cat no 22-038217) and using a 10 ml syringe perfuse it with 1 ml of PBS to get it clean. It is important while handling the brachiocephalic artery to touch it only at its ends and not in the middle.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745847,"guid":"E65883F07D7F11E981A25B9CEBFE22B1","previous_id":745846,"previous_guid":"DF25DD307D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"801D2D34D1A64A5A9DB56513D881C8A7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6A19EB209F1F406490C6A7F5944720C9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Cut the aorta proximal to the branching site of the brachiocephalic artery.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745848,"guid":"F6B13D007D7F11E981A25B9CEBFE22B1","previous_id":745847,"previous_guid":"E65883F07D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"320462C04F5B44EDB9CF621332992931","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C3DEF1254C874E3AB3D22C6FB4D83065","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take out the heart with the aorta. The atria must remain intact as they serve as landmarks when cutting the heart in the atherosclerosis assay.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745849,"guid":"FCF8E7807D7F11E981A25B9CEBFE22B1","previous_id":745848,"previous_guid":"F6B13D007D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DE54CCCA20B3428E80ED4FADE7F067C3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E27C1E2FBC094954B358065A9ABF3628","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Flush the heart-aorta preparation through the aorta with a 10 ml syringe containing 2 ml of PBS and place the heart in a 20 ml scintillation vial containing 10 ml of formaldehyde solution (10% v\/v in aqueous phosphate buffer, Mallinckrodt).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745850,"guid":"056E6E807D8011E981A25B9CEBFE22B1","previous_id":745849,"previous_guid":"FCF8E7807D7F11E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C8AF280E4BED4C3C910C91ED8771F740","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B40870F1854F4D3AAD1E1F3E591446AE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Take the cleaned piece of the brachiocephalic artery and place it in a new base mold, fill it up to the first lip of the base mold with Tissue Tek O.C.T. compound. Move the brachiocephalic artery around carefully to make sure it fills with OCT.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":745851,"guid":"0DE2BD007D8011E981A25B9CEBFE22B1","previous_id":745850,"previous_guid":"056E6E807D8011E981A25B9CEBFE22B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BB11ED07F5EE464F90A1B6EA2EE9BFA7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"211E616EA9F8465597F21A3694EB724D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Turn the Y-shaped piece of brachiocephaic artery piece so the prongs of the fork face the bottom of the base mold and freeze it in that position on a block of