2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">Sample volume should be a maximum 600 \u03bcL after cleaning with chloroform: isoamyl alcohol. <\/span><span style = \"font-style:italic;\"><\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","critical":null,"critical_id":null,"duration":0},{"id":844727,"guid":"413413D2680A4DCE84FB0ACCECDA97F4","previous_id":844726,"previous_guid":"411EA565C46F4AECB48D19BDEB98A187","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3CE5A9AD82FD4048A593BD5C7D018919","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA PRECIPITATION"}},{"id":1054724,"guid":"027E1D447BEC45528408C81322B3126F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge at 16,000 rcf for 1 min. Carefully decant the supernatant as soon as possible, without disturbing the pellet.<\/div><\/div>"}},{"id":1054725,"guid":"50BDFB4B66D440C09BFFC2FAF296C321","order_id":2,"type_id":26,"title":"notes","source":{"id":0,"parent_id":0,"uri":"","title":"","body":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-style:italic;\">High-molecular weight DNA should pellet easily, thus could pulse spin for 10 s at maximum speed. If no pellet can be seen, centrifuge for longer. Bringing ethanol concentration to 2x volume is also an option. <\/span><\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","affiliations":[],"username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false,"hide_following":false},"comments":[]}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","critical":null,"critical_id":null,"duration":0},{"id":844728,"guid":"0E9F9CD208D547E198B5A574657A5A27","previous_id":844727,"previous_guid":"413413D2680A4DCE84FB0ACCECDA97F4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"892399FCD7374EABB93CEC01CDEDFDB3","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA PRECIPITATION"}},{"id":1054724,"guid":"EA3E5271B13740E8A6AA3180A9631A46","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add approximately 700 \u00b5L of freshly prepared 70% ethanol, enough to cover the pellet. Let the pellet soak for 1 min at room temperature to dissolve excess salts.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","critical":null,"critical_id":null,"duration":0},{"id":844729,"guid":"E4A65AC91B724A0A89740EA520DEB859","previous_id":844728,"previous_guid":"0E9F9CD208D547E198B5A574657A5A27","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"30559F2816DB495385BEDAC125EE0654","order_id":1,"type_id":6,"title":"Section","source":{"title":"DNA PRECIPITATION"}},{"id":1054724,"guid":"A8D0BC48EBDE4014ACA23881F77D4A65","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge at 16,000 rcf for 1 min at 4\u00b0C. Carefully decant the supernatant as soon as possible, without disturbing the pellet.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#84CE84","critical":null,"critical_id":null,"duration":0},{"id":844730,"guid":"B0F73DF9800847889C6A4457E05CBAB8","previous_id":844729,"previous_guid":"E4A65AC91B724A0A89740EA520DEB859","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"26B8BE3A