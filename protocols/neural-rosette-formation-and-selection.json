itle":"","body":"<div class = \"text-blocks\"><div class = \"text-block\">To maintain a pure culture, it is best to leave some rosettes behind rather than collect all of the rosettes and additional cells.<\/div><\/div>","created_on":0,"changed_on":0,"creator":{"name":" ","affiliation":"","username":"","link":null,"image":{"source":"","placeholder":""},"badges":[],"research_interests":null},"comments":[]}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":709585,"guid":"5CEE639032E411E9AC957DCD0629DAD2","previous_id":709584,"previous_guid":"5663959032E411E9AC957DCD0629DAD2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"518A5DC5EE89477DB1FE1334A99430DA","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BC54F1D964CE4200923E2E76809C0C49","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Centrifuge rosette clusters at 750 rpm for<\/div><div class = \"text-block\">.\n<\/div><\/div>"}},{"id":1054725,"guid":"721853C032E411E9AC957DCD0629DAD2","order_id":2,"type_id":4,"title":"duration","source":{"duration":180,"title":""}}],"cases":null,"data":null,"section":null,"section_color":null}],"materials":[],"description":null,"changed_on":1551229456}