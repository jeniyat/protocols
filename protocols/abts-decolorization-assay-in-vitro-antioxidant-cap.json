blic":0},"access":{"can_view":1,"can_remove":0,"can_add":0,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":785048,"guid":"D4B049F0ACB211E99A4FD3EF37A6B6E5","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1DB1A7B007B64EB981F26E8E74B03BE3","order_id":1,"type_id":6,"title":"Section","source":{"title":"ABTS radical preparation"}},{"id":1054724,"guid":"15CA97F90A1742EFB910C5B6FD1C3F40","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Prepare a 7 mM ABTS (e.g., A1888, Sigma-Aldrich) stock solution in ultrapure water.<\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example:<\/span><span> Dilute 3.8408 mg in a final volume of 1,000 \u00b5L<\/span><span style = \"vertical-align:super;font-weight:bold;\">1<\/span><span>.<\/span><\/div><div class = \"text-block\"><span style = \"vertical-align:super;font-weight:bold;\">1<\/span><span>Note that you will need 570 \u00b5L (190 \u00b5L for each replicate) of the final ABTS<\/span><span style = \"vertical-align:super;\">\u2022 <\/span><span>stock solution per sample; 400 \u00b5L of the ABTS solution produces approximatelly 10,000 \u00b5L of ABTS<\/span><span style = \"vertical-align:super;\">\u2022 <\/span><span>stock solution (see step 6). <\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785049,"guid":"D6AD2B60ACB211E99A4FD3EF37A6B6E5","previous_id":785048,"previous_guid":"D4B049F0ACB211E99A4FD3EF37A6B6E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D3F33E47AA39459FA9D4FF08786F4ADB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"A9B38EFFEF1D4070B2A36C79B3E870B6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Prepare a 245 mM APS (e.g., A3678, Sigma-Aldrich) solution in ultrapure water.<\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example:<\/span><span> Dilute 5.5909 mg in a final volume of 100 \u00b5L.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785079,"guid":"037FF4D0ACBB11E994BC554D7434B93A","previous_id":785091,"previous_guid":"8C371F90ACBD11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FC53D7E6BC0C48CFAE55CF19C2172D42","order_id":1,"type_id":6,"title":"Section","source":{"title":"ABTS decolorization assay"}},{"id":1054724,"guid":"4BC6DC52022E4DD98CA74DB1F5EE9504","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Pipet 10 \u00b5L of PBS (or other solvent used) plus 190 \u00b5L of ultrapure water into separate microtiter plate wells in triplicate. <\/span><span>These will be used as reference\/zero for the readings.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785081,"guid":"63CDBC00ACBB11E994BC554D7434B93A","previous_id":785090,"previous_guid":"A1ECF180ACBC11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"317DB5E8BC2349148735E6B8EF03E834","order_id":1,"type_id":6,"title":"Section","source":{"title":"Trolox standard solutions preparation"}},{"id":1054724,"guid":"F389EDFA35EF41FDAAF34336A1359DD8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Prepare a 2 mM Trolox (e.g., 238813, Sigma-Aldrich) stock solution in PBS<\/span><span style = \"font-weight:bold;vertical-align:super;\">3<\/span><span style = \"font-weight:bold;vertical-align:super;\">.<\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example:<\/span><span> Dilute 1.0012 mg in a final volume of 2,000 \u00b5L.<\/span><\/div><div class = \"text-block\"><span style = \"vertical-align:super;font-weight:bold;\">3<\/span><span>If compatible, use the same solvent\/buffer in which the sample to be analyzed is prepared to make the Trolox standard solutions.  <\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785084,"guid":"91842850ACBB11E994BC554D7434B93A","previous_id":787560,"previous_guid":"0117AD50AFAE11E9A87EA1CCB24FA6BA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9F29DF1CD7F144E0BB32405C0685A67A","order_id":1,"type_id":6,"title":"Section","source":{"title":"Calculation"}},{"id":1054724,"guid":"1F39A504A4EF4E819BCCA985FF56A77E","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Using the average values of the triplicates for each standard concentration, calculate the decolorization effect caused by each samples\/standard relative to the absorbance of the control (ABTS<\/span><span style = \"font-weight:bold;vertical-align:super;\">\u2022<\/span><span style = \"font-weight:bold;\"> + solvent) at 734 nm.<\/span><span><\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngm7qv6.png\" \/><\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:left\"><span style = \"font-style:italic;\">Example:<\/span><\/div><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngp7qv6.png\" \/><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bng37qv6.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"EEDBF97571F14FA49FB57A69E6FE5860","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngm7qv6.png\" \/><\/div>"}},{"id":1054726,"guid":"12F2784038334625A9775A65DADEF6DF","order_id":3,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngp7qv6.png\" \/><\/div>"}},{"id":1054727,"guid":"13A552E23A9245E3B4C968CA8A40126D","order_id":4,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bng37qv6.png\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785086,"guid":"C49EBCF0ACBB11E994BC554D7434B93A","previous_id":785049,"previous_guid":"D6AD2B60ACB211E99A4FD3EF37A6B6E5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F437C453ACB843AEA3DCDAE529A3700A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"E6E4E02E723D4C469629ACEA9AA3D85C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Add APS to the ABTS solution so that the final APS concentration is 2.45 mM. This step is necessary to generate the ABTS radical (ABTS<\/span><span style = \"font-weight:bold;vertical-align:super;\">\u2022<\/span><span style = \"font-weight:bold;\">).<\/span><span><\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example: <\/span><span>Add 5.05 \u00b5L of APS to 500 \u00b5L of ABTS; final volume 505.05 \u00b5L.<\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bnf47qv6.png\" \/><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bnf57qv6.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"DF272AFB46AD47AD9FC6A79EDB677191","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bnf47qv6.png\" \/><\/div>"}},{"id":1054726,"guid":"E9143E6593DD40C78ECA5FE3B0F4B9D1","order_id":3,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bnf57qv6.png\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785087,"guid":"F7DDC980ACBB11E994BC554D7434B93A","previous_id":785086,"previous_guid":"C49EBCF0ACBB11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BDB646CABF7A4C6FAA77A6CB17868424","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B66F16ED4EC5462088E8C278E08266F5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Incubate it overnight (12\u201316 h) at room temperature in the dark.<\/span><span><\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bnf77qv6.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"42F32227F3064ED4A6B152B6BDE175E6","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bnf77qv6.png\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785088,"guid":"13180080ACBC11E994BC554D7434B93A","previous_id":785087,"previous_guid":"F7DDC980ACBB11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"497092C44AA143449DD612BC87372902","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"2DA5B967E04D47629D655E682B6075E4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Check the concentration of the ABTS radical (ABTS<\/span><span style = \"font-weight:bold;vertical-align:super;\">\u2022<\/span><span style = \"font-weight:bold;\">) stock solution at 734 nm.<\/span><span><\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example: <\/span><span>Dilute 10 uL of ABTS<\/span><span style = \"vertical-align:super;\">\u2022<\/span><span> solution in a final volume of 1,000 \u00b5L using ultrapure water; read it at 734 nm.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785089,"guid":"78AC07C0ACBC11E994BC554D7434B93A","previous_id":785088,"previous_guid":"13180080ACBC11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"370AD85D1D19460D9014BAB481F725FE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8C6807C8F27C47479189D024F842BC10","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Prepare an ABTS<\/span><span style = \"font-weight:bold;vertical-align:super;\">\u2022<\/span><span style = \"font-weight:bold;\"> solution that absorbs ~0.700 at 734 nm.<\/span><span><\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example: <\/span><span>If the solution prepared in the previous step absorbed 0.689, dilute 100 \u00b5L of the ABTS<\/span><span style = \"vertical-align:super;\">\u2022 <\/span><span>stock solution in a final volume of 9,842.9 \u00b5L<\/span><span style = \"vertical-align:super;font-weight:bold;\">2<\/span><span>. <\/span><\/div><div class = \"text-block\"><span style = \"vertical-align:super;font-weight:bold;\">2<\/span><span>Remember that you will need 190 \u00b5L per well in the assay; each sample\/control\/standard is analyzed in triplicate (3 wells).<\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example:<\/span><span> If you are going to assess two samples at four concentrations each, build a standard curve with six concentrations, and read a blank reaction control (all in triplicate), you will need 8,550 \u00b5L of the ABTS<\/span><span style = \"vertical-align:super;\">\u2022 <\/span><span>stock solution (absorbing ~0.700).<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785090,"guid":"A1ECF180ACBC11E994BC554D7434B93A","previous_id":785089,"previous_guid":"78AC07C0ACBC11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"372CFF0C16064122A769E1D0E494F0DB","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DA7410FA09EE4311AE8200593B60682F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Check the absorbance at 734 nm, adjust if necessary, and store until use.<\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example:  <\/span><span>If the solution prepared in the previous step absorbed 0.805, diluted it 1.15-fold (9,000 \u00b5L in  a final volume of 10,350 \u00b5L) to achieve an absorbance of ~0.700. <\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bm7y7qv6.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"CCE4E8E200F748DE8280326822C83D74","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bm7y7qv6.png\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785091,"guid":"8C371F90ACBD11E994BC554D7434B93A","previous_id":785094,"previous_guid":"0AFF90D0ACC011E990C65BAEE7CA4D0C","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"83A1A1EB1A4642FCACDC9DDBAF0860AD","order_id":1,"type_id":6,"title":"Section","source":{"title":"Sample preparation"}},{"id":1054724,"guid":"F6D3BC80834F427E9E92E318B65768B7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Dilute<\/span><span style = \"font-weight:bold;vertical-align:super;\">5<\/span><span style = \"font-weight:bold;\"> the samples at the desired concentrations<\/span><span style = \"font-weight:bold;vertical-align:super;\">6<\/span><span style = \"font-weight:bold;vertical-align:super;\">.<\/span><\/div><div class = \"text-block\"><span style = \"vertical-align:super;font-weight:bold;\">5<\/span><span>If a solvent other than PBS is necessary to dilute the sample, prepare adequate solvent controls in the next step. <\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example<\/span><span>: If you need 50% (v\/v) DMSO to dilute your sample, also assess 10% DMSO (without sample) solution in the assay to check for any interference.<\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;vertical-align:super;\">6<\/span><span>For peptides, we usually use 2.0, 1.0, 0.5, and 0.25 mg\/mL concentrations. Note that for peptides with strong activity (i.e., 100% ABTS<\/span><span style = \"vertical-align:super;\">\u2022 <\/span><span>scavenging), we further dilute them to 0.125\u20130.031 mg\/mL.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":785094,"guid":"0AFF90D0ACC011E990C65BAEE7CA4D0C","previous_id":785081,"previous_guid":"63CDBC00ACBB11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E1527749D93C4DA891BA1A6F3C32B682","order_id":1,"type_id":6,"title":"Section","source":{"title":"Trolox standard solutions preparation"}},{"id":1054724,"guid":"3276D1DB736F44FD99E71F95BCA756DD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Prepare several Trolox solutions at concentrations from 12.5 to 400 \u00b5M<\/span><span style = \"font-weight:bold;vertical-align:super;\">4 <\/span><span style = \"font-weight:bold;\">using PBS in Eppendorf tubes.<\/span><span style = \"font-weight:bold;\"><\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example:<\/span><span> First, dilute the 4 mM stock by 5-fold to 400 \u00b5M, and dilute it serially to 200, 100, 50, 25, and 12.5 \u00b5L (e.g., 100 \u00b5L of the previous concentration + 100 \u00b5L PBS). <\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;vertical-align:super;\">4<\/span><span>Under the conditions described in this protocol, the change in absorbance at 734 nm is linear within the 12.5\u2013400 \u00b5M Trolox range (see <\/span><span style = \"font-style:italic;\">Calculation section<\/span><span>).<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":787558,"guid":"517B5130AFAD11E9A87EA1CCB24FA6BA","previous_id":785079,"previous_guid":"037FF4D0ACBB11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"6BEC2144B54949A785C795A357DBB709","order_id":1,"type_id":6,"title":"Section","source":{"title":"ABTS decolorization assay"}},{"id":1054724,"guid":"04593AEA098A477B8E8C9986AA4741BB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Pipet 10 \u03bcL of each trolox standard solution (including the control<\/span><span style = \"font-weight:bold;vertical-align:super;\">7<\/span><span style = \"font-weight:bold;\">, solvent only) and each sample dilution into separate microtiter plate wells in triplicate.<\/span><span style = \"font-weight:bold;\"><\/span><\/div><div class = \"text-block\"><span style = \"font-weight:bold;vertical-align:super;\">7<\/span><span>These are the reference wells of maximum ABTS<\/span><span style = \"vertical-align:super;\">\u2022<\/span><span> concentration, in which no sample or standard were added.<\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":787559,"guid":"D9BF59B0AFAD11E9A87EA1CCB24FA6BA","previous_id":787558,"previous_guid":"517B5130AFAD11E9A87EA1CCB24FA6BA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"31613D0ABE124377B69DECA8FD2E059F","order_id":1,"type_id":6,"title":"Section","source":{"title":"ABTS decolorization assay"}},{"id":1054724,"guid":"8421AD22CCE1464987C387CEAE859D3A","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Add 190 \u00b5L of the ABTS<\/span><span style = \"font-weight:bold;vertical-align:super;\">\u2022 <\/span><span style = \"font-weight:bold;\">solution prepared in Step 7 into each well.<\/span><span><\/span><\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":787560,"guid":"0117AD50AFAE11E9A87EA1CCB24FA6BA","previous_id":787559,"previous_guid":"D9BF59B0AFAD11E9A87EA1CCB24FA6BA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"444FBAA916C84174A1893B6B2C81CE21","order_id":1,"type_id":6,"title":"Section","source":{"title":"ABTS decolorization assay"}},{"id":1054724,"guid":"65C98153BE6B4B87A991626915B79014","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Mix the wells content using the 'shake' function of the microtiter plate reader, incubate it for 5 minutes in the dark and read it at 734 nm.<\/span><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngt7qv6.png\" \/><\/div><\/div>"}},{"id":1054725,"guid":"F120B8E205034289B67D86C14EEDA67D","order_id":2,"type_id":1,"title":"description","source":{"description":"<div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngt7qv6.png\" \/><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":787625,"guid":"59763F10AFAF11E9A87EA1CCB24FA6BA","previous_id":785084,"previous_guid":"91842850ACBB11E994BC554D7434B93A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FFC76934C3D4455FA0E13FFDB3FA5E17","order_id":1,"type_id":6,"title":"Section","source":{"title":"Calculation"}},{"id":1054724,"guid":"0C5EF16381B2401A981FFB2443B833B3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Build a standard curve: plot trolox concentrations in mg\/mL on the X-axis and decolorization (%) on the Y-axis. Calculate a linear regression (Y = a <\/span><span style = \"font-weight:bold;font-weight:bold;\">\u00d7<\/span><span style = \"font-weight:bold;\"> X + b; e.g., 'add a linear trendline' in Microsoft Excel).<\/span><span style = \"font-weight:bold;\"><\/span><\/div><div class = \"text-block\"><span style = \"font-style:italic;\">Example<\/span><span style = \"font-style:italic;\">:<\/span><\/div><div class = \"text-block\"><div class = \"justify\" style = \"text-align:center\"><span style = \"font-weight:bold;\"> <\/span><span style = \"font-weight:bold;\"> <\/span><\/div><\/div><div style = \"text-align :; float : ;\"><img style = \"\" src = \"https:\/\/s3.amazonaws.com\/protocols-files\/public\/122b747dad6924a1a9df2b640ea5639b72668682ea5df4ac14d7846ae134d204\/bngv7qv6.png\" \/><\/div><\/div>"}},{"id":1054725,"