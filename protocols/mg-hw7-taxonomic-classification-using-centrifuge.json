,"guid":"95D6759A9DED4B4F874386B924D9FDF6","previous_guid":"3483521F5C904010B8A7DEE804024EE0","component_type_id":"15","data_id":"1354","data":null,"order_id":"3","name":"Command","data_by_id":"1","type_id":"15","source_data":{"name":"#!\/bin\/bash\n\n#PBS -W group_list=bh_class\n#PBS -q windfall\n#PBS -l jobtype=cluster_only\n#PBS -l select=1:ncpus=12:mem=23gb\n#PBS -l pvmem=22gb\n#PBS -l walltime=24:00:00\n#PBS -l cput=24:00:00\n#PBS -M netid@email.arizona.edu\n#PBS -m bea\n\n#--------------EDIT THESE---------------\nFASTA_DIR=\"\/rsgrps\/bh_class\/username\/fasta\"\nOUT_DIR=\"\/rsgrps\/bh_class\/username\/taxonomy\"\nBT2_OUT_DIR=\"\/rsgrps\/bh_class\/username\/unmapped\"\n#---------------------------------------\n\nCENT_DB=\"\/rsgrps\/bh_class\/b_compressed+h+v\/b_compressed+h+v\"\nBT2_INDEX=\"\/rsgrps\/bh_class\/bowtie2_index\/human_index\"\n\ncd \"$FASTA_DIR\"\nexport FASTA_LIST=\"$FASTA_DIR\/fasta-list\"\nls *.fasta > $FASTA_LIST\necho \"FASTA files to be processed:\" $(cat $FASTA_LIST)\n\nmodule load bowtie2\/2.2.5\nwhile read FASTA; do\n  export FASTA=\"$FASTA\"\n  export FILE_NAME=`basename $FASTA | cut -d '.' -f 1`\n  bowtie2 -x $BT2_INDEX -U $FASTA -f --very-sensitive-local -p 4 --un $BT2_OUT_DIR\/$FILE_NAME.unmapped\n\ndone < $FASTA_LIST\n\ncd \"$BT2_OUT_DIR\"\nexport UNMAPPED_LIST=\"$BT2_OUT_DIR\/unmapped-list\"\nls *.unmapped > $UNMAPPED_LIST\necho \"Running Centrifuge on the following files:\" $(cat $UNMAPPED_LIST)\n\nwhile read UNMAPPED; do\n  export UNMAPPED=\"$UNMAPPED\"\n  export UNMAPPED_NAME=$(basename $UNMAPPED | cut -d '.' -f 1)\n  centrifuge -x $CENT_DB -U $UNMAPPED -S $OUT_DIR\/$UNMAPPED_NAME-classout --report-file $OUT_DIR\/$UNMAPPED_NAME-centrifuge_report.tsv -f\ndone < $UNMAPPED_LIST","description":"As indicated in the script, edit the FASTA_DIR and OUT_DIR to include the path to YOUR Fasta files and the taxonomy directory created in the previous step. \n\nRemember to replace netid with YOUR netid to receive email notifications","os_name":"","os_version":"","can_edit":"1"},"is_project":0}]},{"id":"187799","is_changed":"0","original_id":"0","is_skipped":"0","is_checked":"0","guid":"AE015BC09EEC4F9889F1F6421899E569","previous_guid":"703B0ED84D934BE8B6591C6404F7195A","previous_id":"187798","last_modified":"1502718954","components":[{"component_id":"987395","previous_id":0,"original_id":"0","guid":"7C40BEA96ECA41E28F5CA52300633695","previous_guid":null,"component_type_id":"6","data_id":null,"data":null,"order_id":"0","name":"Section","data_by_id":"0","type_id":"6","source_data":{"section":null},"is_project":0},{"component_id":"227767","previous_id":"987395","original_id":"0","guid":"A536A401801E4CC5935AF841CB1D9677","previous_guid":"7C40BEA96ECA41E28F5CA52300633695","component_type_id":"1","data_id":"0","data":"<p>Submit centrifuge_tax.sh\u00a0using qsub:<\/p>","order_id":"1","name":"Description","data_by_id":"0","type_id":"1","source_data":{"description":"<p>Submit centrifuge_tax.sh\u00a0using qsub:<\/p>"},"is_project":0},{"component_id":"227768","previous_id":"227767","original_id":"0","guid":"3F4F59410EE4489CBEBC99274DA8185D","previous_guid":"A536A401801E4CC5935AF841CB1D9677","component_type_id":"15","data_id":"1355","data":null,"order_id":"2","name":"Command","data_by_id":"1","type_id":"15","source_data":{"name":"$ qsub -e std-err\/ -o std-out\/ centrifuge_tax.sh","description":"","os_name":"","os_version":"","can_edit":"1"},"is_project":0}]},{"id":"187800","is_changed":"0","original_id":"0","is_skipped":"0","is_checked":"0","guid":"F48BA0D33AA14EEE9238755C405A96B6","previous_guid":"AE015BC09EEC4F9889F1F6421899E569","previous_id":"187799","last_modified":"1502718954","components":[{"component_id":"987396","previous_id":0,"original_id":"0","guid":"6F35D714A16B49F2B496EAAF4B5799FA","previous_guid":null,"component_type_id":"6","data_id":null,"data":null,"order_id":"0","name":"Section","data_by_id":"0","type_id":"6","source_data":{"section":null},"is_project":0},{"component_id":"227769","previous_id":"9