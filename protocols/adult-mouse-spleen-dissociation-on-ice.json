er":"https:\/\/s3.amazonaws.com\/pr-journal\/psucz36.png"},"is_member":1,"title":"Human Cell Atlas Method Development Community","requester":{"name":" ","affiliation":null,"affiliation_url":null,"username":null,"link":null},"protocol":{"id":0,"title":"Adult Mouse Spleen Dissociation (On ice)","title_html":"Adult Mouse Spleen Dissociation (On ice)","image":{"source":null,"placeholder":null},"doi":null,"doi_status":0,"uri":"adult-mouse-spleen-dissociation-on-ice-ydwfs7e","type_id":1,"published_on":null,"stats":{"number_of_views":0,"number_of_steps":0,"number_of_bookmarks":0,"number_of_comments":0}},"created_on":1550697114,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null}}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"forks_count":{"private":0,"public":0},"steps":[{"id":710909,"guid":"2A60257C24BC47008A0832536FC1A3E3","previous_id":710920,"previous_guid":"779A5E6FC7D942BB86D522F889CDD62E","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0927C8A776B4428494E24CCFB2914B38","order_id":1,"type_id":6,"title":"Section","source":{"title":"Isolate Tissue"}},{"id":1054724,"guid":"2A7912783E154D69B980AD1DBFD2D5F6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Using sterile forceps, transfer spleen to petri dish on ice. Remove excess DPBS using pipet. Chop whole spleen coarsely\u00a0for 45 sec\u00a0using razor blade on petri dish on ice until a fine paste. Manipulate tissue with forceps while mincing with razorblade to thoroughly break up.<\/div><\/div>"}},{"id":1054725,"guid":"AD3365C8746C4B01BA9F967105E41D0F","order_id":2,"type_id":4,"title":"duration","source":{"duration":45,"title":"mince on ice"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710910,"guid":"816E2ED75F894A598599969B7782F0FA","previous_id":710909,"previous_guid":"2A60257C24BC47008A0832536FC1A3E3","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5E5F6E33D9D142C5A3E1C347FFDD128B","order_id":1,"type_id":6,"title":"Section","source":{"title":"First layer of dissociation"}},{"id":1054724,"guid":"704890F72D194EDF9C9A819ABE91BAD9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Weigh out 12 mg of minced spleen on petri dish. Using razor blade, transfer to 1.5 mL tube containing 1 mL of enzyme mix on ice.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710911,"guid":"437DCE9B24FA48B3B97C6C52327C4ABB","previous_id":710910,"previous_guid":"816E2ED75F894A598599969B7782F0FA","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E73A2F226DFA404CBDD3A7C5FDACDC20","order_id":1,"type_id":6,"title":"Section","source":{"title":"First layer of dissociation"}},{"id":1054724,"guid":"65954540D2CF490B8A16014C59C5D897","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate tube on ice for 10 minutes. Triturate 10X every 2 mins and shake every min.<\/div><\/div>"}},{"id":1054725,"guid":"09B244247BC24225941C3BB24A8BC8D2","order_id":2,"type_id":4,"title":"duration","source":{"duration":600,"title":"Incubate on ice"}},{"id":1054726,"guid":"358EE0D0355111E9984567E583A10B9D","order_id":3,"type_id":4,"title":"duration","source":{"duration":120,"title":"triturate 10X every 2 min"}},{"id":1054727,"guid":"C889831DB32749B3B1D5AE19370DF78B","order_id":4,"type_id":4,"title":"duration","source":{"duration":60,"title":"Shake every min"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710912,"guid":"E79EF8B44C9A4C27AA96ADAEA6206465","previous_id":710911,"previous_guid":"437DCE9B24FA48B3B97C6C52327C4ABB","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A187A180A529449FB57C46C379B04945","order_id":1,"type_id":6,"title":"Section","source":{"title":"First layer of dissociation"}},{"id":1054724,"guid":"D1F44161865B499CB87EC0E9309FDA2D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After 10 mins of digestion, let tissue chunks settle for 1 min on ice & save 80% of supernatant with released cells & apply cells to 30 \u00b5M filter on 50 mL conical, on ice. Rinse filter with 5 mL ice-cold PBS\/0.04% BSA. Leave filter and 50 mL conical on ice, it will be used for the\u00a0steps as well.<\/div><\/div>"}},{"id":1054725,"guid":"68938DB55C264C478C0A3CA2BB4BCF9D","order_id":2,"type_id":3,"title":"amount","source":{"amount":5,"unit":"ml","title":"ice-cold PBS\/BSA 0.04%"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710913,"guid":"07E48185B0ED44AE890194AAD85A30DF","previous_id":710912,"previous_guid":"E79EF8B44C9A4C27AA96ADAEA6206465","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C9F56C7609EC43BBB09250F19C17B5E9","order_id":1,"type_id":6,"title":"Section","source":{"title":"Second layer of dissociation"}},{"id":1054724,"guid":"5D864DD39E21431B94626E36C9B3CF81","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add additional 1 mL enzyme mix to tissue chunks.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710914,"guid":"229C4D13319647DDA2A78DC575CDDF3D","previous_id":710913,"previous_guid":"07E48185B0ED44AE890194AAD85A30DF","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E610867DD75F4AA9BC0011F8432EBA82","order_id":1,"type_id":6,"title":"Section","source":{"title":"Second layer of dissociation"}},{"id":1054724,"guid":"5AEBCCB3A667494393A567F11802F0EB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Continue to triturate 10x every 2 minutes and shake every minute while incubating on ice, for 10 additional minutes (21 min total time).<\/div><\/div>"}},{"id":1054725,"guid":"37F39A7C00D24584849CCB18FC6CE90C","order_id":2,"type_id":4,"title":"duration","source":{"duration":600,"title":"Incubate on ice"}},{"id":1054726,"guid":"2A1F20E2EEEB4D9F983EF6B24CF7A4FC","order_id":3,"type_id":4,"title":"duration","source":{"duration":120,"title":"triturate 10x every 2 min"}},{"id":1054727,"guid":"693F1B57EF6F4A08A101A38F8D6154AE","order_id":4,"type_id":4,"title":"duration","source":{"duration":60,"title":"shake every min"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710915,"guid":"FBAA5CBE41B24D5E8F61CCB85DB4A1B1","previous_id":710914,"previous_guid":"229C4D13319647DDA2A78DC575CDDF3D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"857D43C3198B4964BCC4B7D8DBE2FC10","order_id":1,"type_id":6,"title":"Section","source":{"title":"Second layer of dissociation"}},{"id":1054724,"guid":"AA9FC351B4294A6AB4C0C9C8C8F5920B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After 21 min total time, triturate 10x and add entire volume of remaining digest mix\u00a0to\u00a0the same 30 \u00b5M filter on 50 mL conical as in the previous step. Rinse filter w\/5 mL ice-cold PBS\/0.04% BSA.<\/div><\/div>"}},{"id":1054725,"guid":"1931ED69026C42D1B2561901CD61E1BB","order_id":2,"type_id":3,"title":"amount","source":{"amount":5,"unit":"ml","title":"ice-cold PBS\/BSA 0.04%"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710916,"guid":"06AA9B5529284F728031907C4D537221","previous_id":710915,"previous_guid":"FBAA5CBE41B24D5E8F61CCB85DB4A1B1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8BC8C6C2DA7B4ADC805FFA078B0E1A5F","order_id":1,"type_id":6,"title":"Section","source":{"title":"RBC Lysis"}},{"id":1054724,"guid":"0062CCC846EF41D1821871641BCAB226","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer flow-through to 15 mL conical. Spin 650 g for five minutes at 4 \u00b0C. After spin, remove supernatant, leaving 100 \u00b5L volume in 15 mL conical tube.<\/div><\/div>"}},{"id":1054725,"guid":"B576CABC06064101AD3833ACF365E2BE","order_id":2,"type_id":24,"title":"temperature","source":{"concentration":4,"unit":"\u00b0C","title":""}},{"id":1054726,"guid":"2D0993D9463A46B4B638DD9A564A98E8","order_id":3,"type_id":4,"title":"duration","source":{"duration":300,"title":"spin 650 g for 5 min"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id":710917,"guid":"6D003B26E9A441B89E8AB6DF63E7C756","previous_id":710916,"previous_guid":"06AA9B5529284F728031907C4D537221","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4569A19AD37C42ABA20D440A402A6232","order_id":1,"type_id":6,"title":"Section","source":{"title":"RBC Lysis"}},{"id":1054724,"guid":"00159D168B3A4628B431A64A24CD79C6","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Perform RBC lysis: add 1 mL RBC lysis buffer to the 100 \u00b5L of cells and triturate 10X. Let sit 3 minutes on ice. After incubating 3 min in RBC lysis buffer, add 13 mL ice-cold PBS\/BSA 0.04% in the 15 mL conical to dilute the RBC lysis buffer. Pipet mix.<\/div><\/div>"}},{"id":1054725,"guid":"1DA8E8AEEE1B4DDB8ED02DC23E600FEC","order_id":2,"type_id":4,"title":"duration","source":{"duration":180,"title":"incubate on ice"}},{"id":1054726,"guid":"FD062CF91D7049BB8A0489A2D02C03EA","order_id":3,"type_id":3,"title":"amount","source":{"amount":13,"unit":"ml","title":"ice-cold PBS\/BSA 0.04%"}},{"id":1054727,"guid":"6069B6E701EB44A58679DF00A9472644","order_id":4,"type_id":3,"title":"amount","source":{"amount":1,"unit":"ml","title":"RBC lysis buffer"}}],"cases":null,"data":null,"section":null,"section_color":null},{"id