emove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"guid":"B05B66E6C5224F71A976A411D2787D6B","steps":[{"id":828541,"guid":"88B9D56779824150B17D2A1A8D66A896","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"77913B48D20541B6B4BB483B1C79E443","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"396D12FAB06C4F6595863FF6AF0DF0C7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare the RPA pellet rehydration solution in a 1.5 ml tube following the table in the Materials section.<\/div><div class = \"text-block\">If possible do a Master Mix that will then be divided.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":828556,"guid":"D91FEFF0F19911E9AC9BA545919536A1","previous_id":830268,"previous_guid":"A7108700F38C11E9ACAB5775FCEEC6DC","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C7D54719871A4CE5B78F88298311305F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"950836161DDD4D92AA5973DF0B41DF89","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">For each sample, transfer the entire volume (47.5 \u00b5l) to a reaction pellet. Pipette up and down until the pellet is fully resuspended.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":828557,"guid":"D925BC50F19911E9AC9BA545919536A1","previous_id":828556,"previous_guid":"D91FEFF0F19911E9AC9BA545919536A1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F676A1E638E3453B9A864AC1CCFEA803","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"17E12C0432D444E19BCFAD4DFCF21DA4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 2.5 \u00b5l of 280mM MgOAc to the cap of each reaction tube. MgOAc will make the reaction start, it is added to the cap so that all reactions start simultaneously when spun down.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":828558,"guid":"D92AEC70F19911E9AC9BA545919536A1","previous_id":830242,"previous_guid":"712A21D0F38411E9AF70272813C3924D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"E89B5419D18C4ED1BBD1D88BF7587E62","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"FA93EB87EC2C4D92B24629605E5302E2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate at 39\u00b0C for 20 min<\/div><div class = \"text-block\">For low copy numbers, incubate 4 minutes, vortex and spin briefly and incubate again for 16 minutes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":828559,"guid":"D92FF580F19911E9AC9BA545919536A1","previous_id":828558,"previous_guid":"D92AEC70F19911E9AC9BA545919536A1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"34883D1F7E2445368B16E9529B539D8D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B0B567B9B28A4DB79E3CF25B8E7330B5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After the incubation, purify the sample if you want to run the RPA product on an agarose gel.<\/div><div class = \"text-block\">PCR purification kits work well, but since RPA primers can be very long (60+ bp), a high cutoff is recommended.<\/div><div class = \"text-block\">Do not open post-amplification RPA reactions in the same workspace as you set up pre -amplification RPA, or take appropriate measures to limit DNA contamination.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":830242,"guid":"712A21D0F38411E9AF70272813C3924D","previous_id":828557,"previous_guid":"D925BC50F19911E9AC9BA545919536A1","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7D5355C37BAE4E4181388349E29D4250","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"CDEA2E9671744FA99C77F6604128E9DE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Do a short spin to make the MgOAc drop fall down into the tube, vortex briefly and do another quick spin.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":830268,"guid":"A7108700F38C11E9ACAB5775FCEEC6DC","previous_id":828541,"previous_guid":"88B9D56779824150B17D2A1A8D66A896","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4CC3950C1C494EA7BA4BE690B949EFF7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5919927DC0E94B9C96344D531D5225FC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Vortex and spin briefly.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span>This protocol describes the procedure to perform an RPA DNA Amplification using the TwistAmp<\/span><span style = \"vertical-align:super;\">TM<\/span><span> Basic<\/span><\/div><\/div>","changed_on":1571651762}