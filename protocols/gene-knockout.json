col_id":0,"components":[{"id":1054723,"guid":"0E2E0C56DBF14E9883D12A5DF2BECDDF","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"76AE98586D34431E9541037E3AF7BFCC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer the pCas9 plasmid into the recipient E. coli \u0394pfkA (Kana resistance, temperature sensitive).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812883,"guid":"2FEB3CE0D6D311E989463DC965F14512","previous_id":812882,"previous_guid":"24F46230D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5D0DC7740CFC416AB2825117A2AB8A3F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BD0BEB63CCA64319A61C63CE8C8B1C59","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Preparation of pCas9 \/\u0394pfkA competent cells:<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812884,"guid":"3BAB0CE0D6D311E989463DC965F14512","previous_id":812883,"previous_guid":"2FEB3CE0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1D9415D8731F44AFB5AF2B9A8610BA4F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AD09B3D3875D4335B0BDD169DAFD81D2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 200 \u03bcL of bacterial solution to 100 mL of LB medium, incubate at 30\u2103, 200 rpm for about 12 h, Add 2% to LB medium, add 0.5% of arabinose at a final concentration after 0.5 hours of culture;<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812885,"guid":"47D81DA0D6D311E989463DC965F14512","previous_id":812884,"previous_guid":"3BAB0CE0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7A4294E150C24D9ABDC2154BE890BB37","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"968C7CEB16C34C4BA35301102B32FAE3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Incubate at 30\u2103 for about 2~2.5 h, until the OD600 is about 0.55-0.6, then take it out and let it stand on ice for 30 min;<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812886,"guid":"4D053060D6D311E989463DC965F14512","previous_id":812885,"previous_guid":"47D81DA0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"68EA63C45A7D4AFABBD00FEDE9125BD3","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1843D2C07A6B42AEBFB20EE0CB585B04","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Collect cells at 4200 rpm for 10 min in a 4\u2103 refrigerated centrifuge;<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812887,"guid":"522A05C0D6D311E989463DC965F14512","previous_id":812886,"previous_guid":"4D053060D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D7D0563546C948FEAADD3670A52762FC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"B5FAE27CF91846FEB6E27ECFC14D441B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Resuspend the cells gentlely with 10 ml of pre-cooled and sterilized 10% glycerol on ice.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812888,"guid":"5B44A660D6D311E989463DC965F14512","previous_id":812887,"previous_guid":"522A05C0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"890DDDD3035246F0BDB8B4323BC1AFE0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"24D92D5D07544CBC9654CF504E67E06D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Repeat steps 2.3 and 2.4 three times, using 10 mL of pre-cooled and sterilized 10% glycerol for the first two washes. The last time it was resuspended with 0.5 mL of glycerol.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812889,"guid":"612DDD30D6D311E989463DC965F14512","previous_id":812888,"previous_guid":"5B44A660D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C11238D2D23146CCAAB93491775EA71F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5581295D475348F2AA050097FC92C712","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dispense in 100 \u03bcL\/tube into a pre-cooled centrifuge tube, It can be used for electroporation or immediately placed in a -80\u2103 refrigerator. . The bacteria can be stored at -80\u2103 for half a year.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812890,"guid":"67889A80D6D311E989463DC965F14512","previous_id":812889,"previous_guid":"612DDD30D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4CD32774516343759F13928D8E66D9A9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6C6339509DC940C4BEC5311E7BAD76D2","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Prepare target segment: <\/div><div class = \"text-block\">Clone upstream and downstream sequences of pykF by PCR to about 500 bp each, Then connect upstream and downstream homology arms together by overlap.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812891,"guid":"732A3420D6D311E989463DC965F14512","previous_id":812890,"previous_guid":"67889A80D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3082E21697304F1EBBD5C5EC51102D57","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1D60A32EF123404C9F900DB53C3DC53C","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">pTarget-pykF vector construction(Str resistance): <\/div><div class = \"text-block\">Download the pykF gene sequence, Find the appropriate gRNA affinity site and replace 1961 to 1980 bases of the pTarget plasmid by digestion and ligation or overlap.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812892,"guid":"82B4C7C0D6D311E989463DC965F14512","previous_id":812891,"previous_guid":"732A3420D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C542E0E3BB82454283AA57DDCB1DC5DD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"DCA7F3EC1A8E472F98AA0365E3CE73F1","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Electroporation:<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812893,"guid":"851E4DB0D6D311E989463DC965F14512","previous_id":812892,"previous_guid":"82B4C7C0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"AF471A1E3F5040C0837B5D2B827F78CD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"2AB0E44891694361A78AC1E7764C9BB3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add >200ng target fragment and >40ng pTarget-pykF plasmid to 100\u03bcL competent cells on ice for 10~30 min, The plasmid and the ligation product should not exceed 5\u03bcL;<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812894,"guid":"8AD3CA50D6D311E989463DC965F14512","previous_id":812893,"previous_guid":"851E4DB0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"9EA71E67302D40BE94CF8AA456749F55","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"79E603F320B04A63AEF99BCB2C1AABD4","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">UV-sterilize the washed and dried 2 mm electroporation cuvette for 20 min and pre-cooled on ice, then quickly transfer the competent cells to the bottom of the electroporation cuvette;<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812895,"guid":"9060D760D6D311E989463DC965F14512","previous_id":812894,"previous_guid":"8AD3CA50D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FD56FABC26204ECA9705E2C51C35A1AE","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"23FB5C5DD8864F60AA7F9B626DE82F25","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dry the outer wall of the cup. Immediately after the electric shock, add 900~1000\u03bcL of 37 \u00b0C preheated LB medium, mix gently, transfer to a 1.5 mL centrifuge tube, shake at 30 \u00b0 C, 150 rpm, 45 ~ 60 min, then apply an appropriate amount of bacterial solution to the plate containing Kan and Str.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812897,"guid":"9A4F69D0D6D311E989463DC965F14512","previous_id":812895,"previous_guid":"9060D760D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B58DD767C4244A92A021204FADCFE931","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"102D793B0AF342B19630447974D815D8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Knockout verification: <\/div><div class = \"text-block\">Primers were designed at 700-1000 bp upstream and downstream of the pykF in the genome to perform colony PCR validation on clones on the plates.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812898,"guid":"9F4C92F0D6D311E989463DC965F14512","previous_id":812897,"previous_guid":"9A4F69D0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A692335DA7A6468698B9ED90CB51827C","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D14CA68F1329424095E29C52E2D54777","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">pTarget-pykF elimination: <\/div><div class = \"text-block\">Take the correct clone, add 1mM IPTG at 30\u2103for 12 h , streak to isolate the monoclone. The clones were tested for the presence of Str resistance, Clones with pTarget eliminated is not resistant to str. The pCas9 plasmid can be eliminated by incubation at 37\u2103.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0},{"id":812899,"guid":"A439B680D6D311E989463DC965F14512","previous_id":812898,"previous_guid":"9F4C92F0D6D311E989463DC965F14512","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D8AE86872B194A2099AA613A9A1FE6FC","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"6A30F85E3A6349AA8AF41A086FBFE8FD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Strains that retain the pCas9 plasmid can be used to knock out other genes.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"","critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\">We use this protocol to knock out pykF in E.coli \u0394pfkA<\/div><\/div>","changed_on":1568455503}