:null,"critical_id":null,"duration":0},{"id":841448,"guid":"CD49D070004D11EA9ABE03CB5CADC0E4","previous_id":841447,"previous_guid":"CC17E020004D11EA9ABE03CB5CADC0E4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"ACB6C3286C114D12BEB7A41FD7083AA2","order_id":1,"type_id":6,"title":"Section","source":{"title":"Reagent setup"}},{"id":1054724,"guid":"1B0CE100F3B44D66BE84F072C2B04A95","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Add 1g of Agarose (I\/Ultrapure) to the 100 ml 0.5x TBE, to make a 1% agarose gel. Heat and mix until homogenous. Add 10 \u00b5l SYBR safe dye and mix again.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":841449,"guid":"37E22CC0004E11EA9ABE03CB5CADC0E4","previous_id":841448,"previous_guid":"CD49D070004D11EA9ABE03CB5CADC0E4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5D58EFFD56164D34B508CCB791296402","order_id":1,"type_id":6,"title":"Section","source":{"title":"Reagent setup"}},{"id":1054724,"guid":"713051EC0333434B9776DEDF42AC80F5","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Transfer 200 ul of the 1% gel solution to a separate Eppendorf tube. Keep this tube heated at 60\u00baC on a heat block.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#A492FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":841450,"guid":"80D95FC0004E11EA9ABE03CB5CADC0E4","previous_id":845134,"previous_guid":"91C98670043611EA9DA98DC2BFC64DF6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"A1AEA4FC85174982BFA365F8300F5454","order_id":1,"type_id":6,"title":"Section","source":{"title":"Equipment setup"}},{"id":1054724,"guid":"0A35D2A771F645B98D0B257F7BF62188","order_id":1,"type_id":1,"