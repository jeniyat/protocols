al":null,"critical_id":null,"duration":0},{"id":845091,"guid":"489ABF20042A11EA9DA98DC2BFC64DF6","previous_id":845087,"previous_guid":"16B40180042811EA9DA98DC2BFC64DF6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"FEA074F403A3486DAC58193504BCB412","order_id":1,"type_id":6,"title":"Section","source":{"title":"After run completion"}},{"id":1054724,"guid":"0978FC50B8C243BB8079BB62A44C4AAE","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">After the program has finished, remove the gel and place it in a suitable tray. (Find the smallest container which fits the gel but is large enough to comfortably slip the gel with gel backplate in and out.)<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#E57785","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":845094,"guid":"AAA13D60042B11EA9DA98DC2BFC64DF6","previous_id":845091,"previous_guid":"489ABF20042A11EA9DA98DC2BFC64DF6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"7DD53FB930AB41BC85CABF2B57A99EF2","order_id":1,"type_id":6,"title":"Section","source":{"title":"After run completion"}},{"id":1054724,"guid":"8AE663F9F40E4E7CA9640D1263C469EF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Image the gel as usual.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#E57785","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":845095,"guid":"B091C730042B11EA9DA98DC2BFC64DF6","previous_id":845094,"previous_guid":"AAA13D60042B11EA9DA98DC2BFC64DF6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0E45DA1936F448FFA2BD26F4ABFD4747","order_id":1,"type_id":6,"title":"Section","source":{"title":"After run completion"}},{"id":1054724,"guid":"C651364B031C4D5ABE98E4EF09C015EC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Dispose of the gel and rinse and dry the gel backplate.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#E57785","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":845096,"guid":"C68A03E0042B11EA9DA98DC2BFC64DF6","previous_id":845095,"previous_guid":"B091C730042B11EA9DA98DC2BFC64DF6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"813BF4482DA04028871A404EE2C1FAC5","order_id":1,"type_id":6,"title":"Section","source":{"title":"System cleanup"}},{"id":1054724,"guid":"C6980A9DC670484F900F27C10B9BA55F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Acquire a large bucket or other liquid container (2L or more recommended). Place this below the output port of the electrophoresis cell. (marked \"TUBE TO CHILLER\")<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#EA94FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":845097,"guid":"16BD7A90042C11EA9DA98DC2BFC64DF6","previous_id":845096,"previous_guid":"C68A03E0042B11EA9DA98DC2BFC64DF6","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C5464B967F604DF7A938D241FFB4ECF5","order_id":1,"type_id":6,"title":"Section","source":{"title":"System cleanup"}},{"id":1054724,"guid":"0AC58C6490BD45F8BAE101A8CDA077AD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Acquire a second large bucket (2-5L), and fill it with RO\/DI water. Place this in a stable location within reach of the input tube to the cooling module.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":"#EA94FF","section_duration":0,"critical":null,"critical_id":null,"duration":0},{"id":845098,"guid":"488E5120042C11EA9DA98DC2BFC64DF6","previous_id":845101,"previous_guid":"658E7B10