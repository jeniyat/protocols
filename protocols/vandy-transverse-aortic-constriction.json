er":1,"can_download":1,"is_locked":0},"steps":[{"id":718852,"guid":"2FA2AE1041D911E9885415A86AB4DCA7","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BEFCD5591019406586C4B786858E959B","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F30D0489CC3B46B7BFDA06DE90F387D0","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mice are anesthetized with pentobarbital (50 mg.kg, IP).<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718853,"guid":"2FB23E7041D911E9885415A86AB4DCA7","previous_id":718852,"previous_guid":"2FA2AE1041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"09952818FAB14E88918314AC861556A4","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EC4261FA1AB64D6F870F182BF1786183","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The ventral neck and left parasternal region isshaved and disinfected with Betadine followed by 70% alcohol.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718854,"guid":"2FB23E7141D911E9885415A86AB4DCA7","previous_id":718853,"previous_guid":"2FB23E7041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"46580B6AC7D14D47A63CB167EC686432","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"7CAC6903E40F4129A75D56854FBDE5AF","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is positioned supineon a heating padand asmallincision is made through the skin underlying the trachea.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718855,"guid":"2FB2658041D911E9885415A86AB4DCA7","previous_id":718854,"previous_guid":"2FB23E7141D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C2031585C5D84B40BBE26864737DBA6E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"94FFED05E06A4282B6A7E71EF8A54B7B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The trachea is exposed, a small puncture is made in the trachea, and endotracheal intubation is performed using a PE-50 tube.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718856,"guid":"2FB2658141D911E9885415A86AB4DCA7","previous_id":718855,"previous_guid":"2FB2658041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"412246B128E74E828E31BF66002D2455","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"5BB24B0E465C49A9A8DDD2E992B27185","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The endotrachealtube is connected to a small rodent ventilator (Harvard Apparatus) for mechanical ventilation of the mouse.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718857,"guid":"2FB28C9041D911E9885415A86AB4DCA7","previous_id":718856,"previous_guid":"2FB2658141D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"3FF154963FA74055A34617B624A2AF18","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AC5FCD2A07C64C21B6C686E5DD105923","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">With the use of a surgical microscope, a left thoracotomy is performed and the secondintercostal space is entered using scissors and blunt dissectionto expose the aortic arch.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718858,"guid":"2FB28C9141D911E9885415A86AB4DCA7","previous_id":718857,"previous_guid":"2FB28C9041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BF63CAFE19EB4071A985FE78B621C410","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"8D086FADEB064A508E6C9F8B0998F276","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A 27G blunt needle is placed parallel to the transverse aorta between the innominate and left common carotid arteries. A 7-0 ligature is placed around the transverse aorta and needle and tied.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718859,"guid":"4DCD755041D911E9885415A86AB4DCA7","previous_id":718858,"previous_guid":"2FB28C9141D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D0F210B9D22046D8A79D9EBC54293BE7","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"EA07BAF941974617B91BDEA4ED15C1CC","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The needle is removed to yield a transverse aortic constriction (TAC) of ~0.4 mm in diameter. In sham mice, the entire procedure is identical except for the ligation of the aorta.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718860,"guid":"4DCDC37041D911E9885415A86AB4DCA7","previous_id":718859,"previous_guid":"4DCD755041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"C2E00D50285E4FB781CC89740470FA9A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"71BAB050634A4179842706AF931E27DD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The chest is closed in layers with 7-0 sutures.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718861,"guid":"4E299F1041D911E9885415A86AB4DCA7","previous_id":718860,"previous_guid":"4DCDC37041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"588BA1499525496ABA7BD27E3241B18A","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"AF5DAB5713294AE2A121AC26C1426775","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is gradually weaned from the ventilator to resolve any possible pneumothorax.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718862,"guid":"4E3A8F0041D911E9885415A86AB4DCA7","previous_id":718861,"previous_guid":"4E299F1041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"1B2D48B494664741A825E9040C9E7099","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"1979E04005504F2590AB010DFF635236","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once spontaneous respiration resumes, the endothracheal tube is removed and the trachea is closed with 8-0 suture. The skin is then closedwith 6-0 suture.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718863,"guid":"4E3AB61041D911E9885415A86AB4DCA7","previous_id":718862,"previous_guid":"4E3A8F0041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"DA05D488364E407BAD27D087A9CE5F8F","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"77DEDF8A9CD14F5DBA451CC20A345624","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouseis maintained on the heating pad until fully recovered from anesthesia.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718864,"guid":"4E3AB61141D911E9885415A86AB4DCA7","previous_id":718863,"previous_guid":"4E3AB61041D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"BB36FD7D31604D7D8653E9E2258B1951","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C43D142C2B7F49D4B88AB44AA82A42F7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Buprenorphine is administered SC immediately following surgery and every 8-12 hr for 72 hr.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0},{"id":718865,"guid":"4E3ADD2041D911E9885415A86AB4DCA7","previous_id":718864,"previous_guid":"4E3AB61141D911E9885415A86AB4DCA7","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"63388EEC089945098EB92D150E0AB977","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"535A1DC2E12C47C1AA0445712F411885","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">One day following the TAC procedure, the mouse is subjected to Doppler echocardiography (Vevo2100) to determine the degree of stenosis induced by the ligation.<\/div><\/div>"}}],"cases":null,"data":null,"section":null,"section_color":null,"duration":0}],"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Summary:<\/span><\/div><div class = \"text-block\">Hypertension in man can be modeled in the mouse by introducing a constriction in the major artery (aorta) thereby obstructing outflow and increasing the afterload to the heart. This protocol describes the procedure for aortic banding aka transverse aortic constriction (TAC).<\/div><\/div>","changed_on":1558014592}