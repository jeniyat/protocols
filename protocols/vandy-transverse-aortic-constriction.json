eated_on":1567088349,"resolve_on":0,"resolved_user":{"name":" ","affiliation":null,"affiliations":[],"username":null,"link":null,"image":{"source":null,"placeholder":null},"badges":[],"research_interests":null,"blocked_by_you":false,"blocked_you":false},"shared":false}}],"has_subprotocols":0,"is_subprotocol":0,"is_bookmarked":0,"can_be_copied":1,"can_remove_fork":1,"forks_count":{"private":0,"public":0},"access":{"can_view":1,"can_remove":0,"can_add":1,"can_edit":0,"can_publish":0,"can_get_doi":0,"can_share":1,"can_move":1,"can_move_outside":1,"can_transfer":1,"can_download":1,"is_locked":0},"steps":[{"id":806373,"guid":"D3D525CBB1324F7B8169F622C705511B","previous_id":null,"previous_guid":null,"modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CA18B295C1CC48CF89D774A2CECF1640","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F5DEBAB6F89D4893AF98F9C370D8844F","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Mice are anesthetized with pentobarbital (50 mg.kg, IP).<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806374,"guid":"DB295FBC2EA14796A1459EEC9B8096C2","previous_id":806373,"previous_guid":"D3D525CBB1324F7B8169F622C705511B","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"8746508D1AAB48D3B06EB92623D81B1D","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"D05C062BAD2B405F8E74ED40B95A7D7D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The ventral neck and left parasternal region is shaved and disinfected with Betadine followed by 70% alcohol.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806375,"guid":"675C9F8F84094D7EB88FA7693B1DBD1D","previous_id":806374,"previous_guid":"DB295FBC2EA14796A1459EEC9B8096C2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"CC4461D39F1A4736842E8ABCC3E2AC47","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"9E653DD692C54945AB3020DC219D84A3","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is positioned supine on a heating pad and a small incision is made through the skin underlying the trachea.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806376,"guid":"070D8EE174084D1DAB9504792ABCA6D4","previous_id":806375,"previous_guid":"675C9F8F84094D7EB88FA7693B1DBD1D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"D3C06609F94A4660A0F24BF586DBB7D8","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"F9FD890947D14BACA0192504892EFB6B","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The trachea is exposed, a small puncture is made in the trachea, and endotracheal intubation is performed using a PE-50 tube.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806377,"guid":"DAA2350668E742D3BD384AA6B94C269A","previous_id":806376,"previous_guid":"070D8EE174084D1DAB9504792ABCA6D4","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0FE5DDC62F934BB5AEBA2779832845BD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"102B658345D341FAB7DA8F229DE49440","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The endotracheal tube is connected to a small rodent ventilator (Harvard Apparatus) for mechanical ventilation of the mouse.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806378,"guid":"405F1DC209D541FA80A5FDE5B5FCC0A5","previous_id":806377,"previous_guid":"DAA2350668E742D3BD384AA6B94C269A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"72FB5E74BE584E39A6D82681F7DAF8E1","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"C12B1165A81F4FEA84AB0F25F728FDA7","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">With the use of a surgical microscope, a left thoracotomy is performed and the second intercostal space is entered using scissors and blunt dissection to expose the aortic arch.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806379,"guid":"D3ABFF3662ED4FF798005A215A9EEFC2","previous_id":806378,"previous_guid":"405F1DC209D541FA80A5FDE5B5FCC0A5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"B0F1472D6B0A4C86973FFE2BF42863D9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"147C20C253AA43E098ED2272262C1CB8","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">A 27G blunt needle is placed parallel to the transverse aorta between the innominate and left common carotid arteries. A 7-0 ligature is placed around the transverse aorta and needle and tied.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806380,"guid":"FCE3924B5EF74DB79A117B4E98D69464","previous_id":806379,"previous_guid":"D3ABFF3662ED4FF798005A215A9EEFC2","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"72E3A7F039A244FFBF5F4461D764A603","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"151015258448466F9A0584A7B953E1EB","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The needle is removed to yield a transverse aortic constriction (TAC) of ~0.4 mm in diameter. In sham mice, the entire procedure is identical except for the ligation of the aorta.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806381,"guid":"8158AD8A5F8540BEB44A183786375931","previous_id":806380,"previous_guid":"FCE3924B5EF74DB79A117B4E98D69464","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"F6F41FE3A4404A7B84AD03D53DD60ADD","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"633502013E4C44BFB22BF3E3491D6348","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The chest is closed in layers with 7-0 sutures.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806382,"guid":"97E61820CBA84425BFAD5EDA73AC617A","previous_id":806381,"previous_guid":"8158AD8A5F8540BEB44A183786375931","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"4EBE9561AD6C41018279231D0662E6C0","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"44561B1C7D104587962D238F468B37B9","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is gradually weaned from the ventilator to resolve any possible pneumothorax.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806383,"guid":"5BFCC608E06E4369BD403ADFAEB4D821","previous_id":806382,"previous_guid":"97E61820CBA84425BFAD5EDA73AC617A","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5F4111EEE52D4064A45878388FF08D96","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"817EEB3947334335AA889F2E7546A768","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Once spontaneous respiration resumes, the endothracheal tube is removed and the trachea is closed with 8-0 suture. The skin is then closed with 6-0 suture.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806384,"guid":"8BC2671CF3584B318B61D20917A5318D","previous_id":806383,"previous_guid":"5BFCC608E06E4369BD403ADFAEB4D821","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5709D5DBB19442DF8C5D16B689EC95C9","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"85BB69FC013148D29CFE8E513B0165AD","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">The mouse is maintained on the heating pad until fully recovered from anesthesia.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806385,"guid":"EE91D58BBAFA43ACAC4D04C99A0E9DC5","previous_id":806384,"previous_guid":"8BC2671CF3584B318B61D20917A5318D","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"5D8DBA7A8CA041DD9A62F49FBE85EE3E","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"BAF9F13B5B124E66A39C08CA4385A962","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">Buprenorphine is administered SC immediately following surgery and every 8-12 hr for 72 hr.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0},{"id":806386,"guid":"D16DAE3E099B41E09C86727619FFD97D","previous_id":806385,"previous_guid":"EE91D58BBAFA43ACAC4D04C99A0E9DC5","modified_on":0,"protocol_id":0,"components":[{"id":1054723,"guid":"0996F0DB053641D895619CFA749B3369","order_id":1,"type_id":6,"title":"Section","source":{"title":""}},{"id":1054724,"guid":"07F1010A441D482AB18C06721D4CCF9D","order_id":1,"type_id":1,"title":"description","source":{"description":"<div class = \"text-blocks\"><div class = \"text-block\">One day following the TAC procedure, the mouse is subjected to Doppler echocardiography (Vevo2100) to determine the degree of stenosis induced by the ligation.<\/div><\/div>"}}],"cases":[],"data":null,"section":null,"section_color":null,"critical":null,"critical_id":null,"duration":0}],"document":null,"materials":[],"description":"<div class = \"text-blocks\"><div class = \"text-block\"><span style = \"font-weight:bold;\">Summary:<\/span><\/div><div class = \"text-block\">Hypertension in man can be modeled in the mouse by introducing a constriction in the major artery (aorta) thereby obstructing outflow and increasing the afterload to the heart. This protocol describes the procedure for aortic banding aka transverse aortic constriction (TAC).<\/div><\/div>","changed_on":1567088349}